call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,25.,("◈ "+"|CFFFFB0CF\"로또\"|R|CFFCE4AFF를 입력하면 자세한 도움말을 보실수 있습니다."))
set bj_forLoopAIndex=(1+GetPlayerId(Player(0)))
set bj_forLoopAIndexEnd=(1+GetPlayerId(Player(11)))
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set integers001[bj_forLoopAIndex]=GetRandomInt(0,9)
set integers004[bj_forLoopAIndex]=1
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endfunction
