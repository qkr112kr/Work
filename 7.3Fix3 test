function hVv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function G6v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function G7v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function G8v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function G9v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hvv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hev)))>=1)and(GetPlayerName(GetTriggerPlayer())=="렘")
endfunction
function hEv takes nothing returns nothing
if(hVv())then
set re=CreateSound("Sound\\Kizaru.mp3",false,false,false,$A,$A,"")
call StartSound(re)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(G5v())then
call CreateNUnitsAtLoA(1,'h093',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
if(G3v())then
call CreateNUnitsAtLoA(1,'h093',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"fairywing.MDX")
else
call CreateNUnitsAtLoA(1,'h093',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000키자루 : |r|cffFA8072피해를 적게 끝내려면.. 역시 대장을 먼저 처리할 수 밖에 없지.!|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hxv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hov))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hrv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hiv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hav))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hnv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
