endif
if(Vz())then
if(nz())then
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.25*GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
else
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.01*GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0BQ',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueTargetOrderById(bj_lastCreatedUnit,$D00DD,GetTriggerUnit())
call RemoveLocation(ov)
endif
if(Oz())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0EJ',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0EI',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitAnimation(bj_lastCreatedUnit,"Birth Large")
call SetUnitTimeScale(bj_lastCreatedUnit,1.2)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(500.,ov,Condition(function Ez)),function Xz)
call RemoveLocation(ov)
endif
