return(GetSpellAbilityId()=='A0IU')
endfunction
function op takes nothing returns boolean
return(GetOwningPlayer(GetSpellTargetUnit())!=Player(7))and(ve==0)
endfunction
function rp takes nothing returns nothing
if(op())then
set ve=(ve+1)
set Zv[1]=GetOwningPlayer(GetSpellTargetUnit())
set Zv[2]=GetOwningPlayer(GetTriggerUnit())
set yv[1]=GetSpellTargetUnit()
set yv[2]=GetTriggerUnit()
set zv[2]=bj_lastCreatedButton
call DialogSetMessage(Yv,("요정을 소유한 플레이어가 "+(GetUnitName(GetSpellTargetUnit())+"유닛을 훔치려 합니다 동의 하십니까?")))
call DialogAddButtonBJ(Yv,"예")
set zv[1]=bj_lastCreatedButton
call DialogAddButtonBJ(Yv,"아니오")
set zv[2]=bj_lastCreatedButton
call DialogDisplayBJ(true,Yv,GetOwningPlayer(GetSpellTargetUnit()))
else
call UnitResetCooldown(GetTriggerUnit())
call DisplayTimedTextToForce(pd(Zv[2]),5.,("|cffff00ff누가 훔치는 중이거나 조합식유닛은 훔칠 수 없습니다.!|r"))
endif
endfunction
function ap takes nothing returns boolean
return(GetClickedButton()==zv[2])
endfunction
function np takes nothing returns boolean
return(GetClickedButton()==zv[1])
endfunction
function Vp takes nothing returns nothing
if(np())then
call AdjustPlayerStateBJ($C,Zv[1],PLAYER_STATE_RESOURCE_LUMBER)
call AdjustPlayerStateBJ(6,Zv[2],PLAYER_STATE_RESOURCE_LUMBER)
set ve=0
call SetUnitOwner(yv[1],Zv[2],true)
call RemoveUnit(yv[2])
call DisplayTimedTextToForce(pd(Zv[2]),5.,(GetUnitName(yv[1])+"|cffff00ff 와 목재6을(를) 획득 하였습니다!|r"))
call DisplayTimedTextToForce(pd(Zv[1]),5.,(GetUnitName(yv[1])+"|cffff00ff유닛을 주고 목재12(를) 획득 하였습니다!|r"))
call DialogClear(Yv)
else
if(ap())then
set ve=0
call DisplayTimedTextToForce(pd(Zv[2]),5.,"|cffff00ff유닛 소유권 변경을 거부당했습니다!|r")
call DialogClear(Yv)
endif
endif
endfunction
