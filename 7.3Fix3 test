function v2v takes nothing returns nothing
if(v1v())then
if(v0v())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0GZ',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D0103,GetAttacker())
call CreateNUnitsAtLoA(1,'h0I0',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call RemoveLocation(ov)
else
call lf(GetAttacker(),GetTriggerUnit(),1000000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call CameraSetEQNoiseForPlayer(GetOwningPlayer(GetAttacker()),2.)
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0IM',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call CreateNUnitsAtLoA(1,'h0DI',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitScalePercent(bj_lastCreatedUnit,140.,140.,140.)
call SetUnitTimeScale(bj_lastCreatedUnit,1.2)
call CreateNUnitsAtLoA(1,'h0IN',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+35.))
call CreateNUnitsAtLoA(1,'h0IN',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())-35.))
call CreateNUnitsAtLoA(1,'h0IO',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())-45.))
call CreateNUnitsAtLoA(1,'h0IO',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())-45.))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(900.,ov,Condition(function vZv)),function v_v)
call RemoveLocation(ov)
endif
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
