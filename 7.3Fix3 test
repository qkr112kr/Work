),false)=="kimeve")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="free.ic")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="huralral")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="alrin")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="theriver")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="leal")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="bj.despair")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="no30")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="moria")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="7hgo7l")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="bj.mild")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="no25")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="ryunian")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="iloveluna")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="hisins")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="oftenprized")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="bed")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="no31")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="hucuro.ac")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="coramdeo")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="linen")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="w_amdor")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="galmegi")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="shirai")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="w_orosung")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="not_byeon")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="minijung")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="j.savior")
endfunction
