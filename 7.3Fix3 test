function qg takes nothing returns boolean
return(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)>=$A)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Pg)))>=1)
endfunction
function Qg takes nothing returns nothing
if(qg())then
if(gg())then
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fg))))
set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000해군루키-대령 코비 : |r|cffFA8072전 언젠가 꼭 대장의 자리에 오를 겁니다!|r")
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-$A))
call CreateNUnitsAtLoA(1,'h0C7',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
endif
if(hg())then
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000비비 : |r|cffFA8072다시 만나게 되면 나를 동료라고 불러주지 않을래!?|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Gg))))
set ov=GetUnitLoc(GetTriggerUnit())
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-$A))
call CreateNUnitsAtLoA(1,'h0IQ',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
endif
if(jg())then
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Hg))))
set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000돈키호테 도플라밍고 : |r|cffFA8072세계의 창조주로서 여길 떠나 성지 마리조아로 간 일족이 돈키호테 일족이다!!|r")
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-$A))
call CreateNUnitsAtLoA(1,'h096',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
endif
if(kg())then
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Jg))))
set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000겟코 모리아 : |r|cffFA8072네놈같은 어린녀석이  이 바다에서 꿈을꾸면 어떻게 되는가! 똑똑히 보여주마!|r")
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-$A))
call CreateNUnitsAtLoA(1,'h088',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
endif
if(lg())then
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Kg))))
set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000포트거스.D.에이스 : |r|cffFA8072동료들에겐 손대지마라 ! 나혼자 싸우겠다|r")
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-$A))
call CreateNUnitsAtLoA(1,'h08B',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
endif
if(mg())then
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Lg))))
set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000제트 : |r|cffFA8072난 제파가 아니다! 내이름은 제트다!!!|r")
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-$A))
call CreateNUnitsAtLoA(1,'h098',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
endif
if(pg())then
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Mg))))
set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000벤 베크만 : |r|cffFA8072살고싶다면 가만히있어라 키자루.|r")
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-$A))
call CreateNUnitsAtLoA(1,'h08O',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
endif
else
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF8200"+("목재나 변화가능 횟수가 부족합니다!|r")))
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
endif
endfunction
