function F1v takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fuv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function FUv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fwv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function FWv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fyv)))>=1)
endfunction
function F2v takes nothing returns nothing
if(F1v())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(FTv())then
call CreateNUnitsAtLoA(1,'h08Q',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
call CreateNUnitsAtLoA(1,'h08Q',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
set re=CreateSound("Sound\\Robin.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000니코 로빈 : |r|cffFA8072너희들의 지적레벨은 어느수준일걸까?|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function FYv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fzv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function FZv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function F_v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function F0v))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
