function By takes nothing returns nothing
if(Iy())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0LD',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h0LF',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(600.,ov,Condition(function Oy)),function Ry)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
if(by())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0LE',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call SetUnitTimeScale(bj_lastCreatedUnit,1.5)
call CreateNUnitsAtLoA(1,'h0L2',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(500.,ov,Condition(function Ay)),function Ny)
call RemoveLocation(ov)
endif
endfunction
