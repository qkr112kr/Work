function Jiv takes nothing returns nothing
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000'하안사냥꾼' 스모커 : |r|cffFA8072정부 상층부영감들에게 전해주겠나? 엿이나 처먹으라고!|r")
call CreateNUnitsAtLoA(1,'h0HJ',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function j9v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Jvv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Jev))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Jxv))))
call RemoveUnit(GroupPickRandomUnit(hd(GetWorldBounds(),Condition(function Jov))))
call RemoveUnit(GroupPickRandomUnit(hd(GetWorldBounds(),Condition(function Jrv))))
call SetUnitAbilityLevelSwapped('A0S4',bj_lastCreatedUnit,GetPlayerTechCountSimple('R01K',GetTriggerPlayer()))
call AdjustPlayerStateBJ(-5,GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)
set re=CreateSound("Sound\\smoker.mp3",false,false,false,$A,$A,"")
call StartSound(re)
endfunction
