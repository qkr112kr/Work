function zW takes nothing returns nothing
if(wW())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0JK',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+180.))
call CreateNUnitsAtLoA(1,'h0JH',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitAnimation(bj_lastCreatedUnit,"birth")
call CreateNUnitsAtLoA(1,'h0JH',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitAnimation(bj_lastCreatedUnit,"birth")
call CreateNUnitsAtLoA(1,'h0JH',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call RemoveLocation(ov)
call IssueTargetOrderById(bj_lastCreatedUnit,$D00FA,GetTriggerUnit())
call SetUnitAnimation(bj_lastCreatedUnit,"birth")
endif
if(YW())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0JO',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0JP',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(350.,ov,Condition(function WW)),function yW)
call RemoveLocation(ov)
call IssueTargetOrderById(bj_lastCreatedUnit,$D00B5,GetTriggerUnit())
call UnitAddAbility(GetTriggerUnit(),'A0AQ')
endif
endfunction
