return(GetRandomInt(1,30)==8)
endfunction
function GY takes nothing returns nothing
if(BY())then
call lf(GetAttacker(),GetTriggerUnit(),(.01*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())),2.,2.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
else
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),100000.,ATTACK_TYPE_HERO,DAMAGE_TYPE_UNIVERSAL)
endif
if(dY())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0JL',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0JM',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitTimeScale(bj_lastCreatedUnit,2.)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(800.,ov,Condition(function cY)),function CY)
call RemoveLocation(ov)
endif
if(FY())then
set Xe=GetUnitLoc(GetAttacker())
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0ID',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0JF',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D0080)
call SetUnitTimeScale(bj_lastCreatedUnit,1.8)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(450.,ov,Condition(function DY)),function fY)
call RemoveLocation(ov)
call RemoveLocation(Xe)
endif
if(gY())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0IE',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D0085,GetAttacker())
call RemoveLocation(ov)
endif
endfunction
