endfunction
function Uf takes nothing returns nothing
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,8.,(GetPlayerName(Player(0))+"|cffffd700플레이어가|r |cffff00ff원랜디 확장팩|r |cffffd700인증을 완료하였습니다!|r"))
set Re=GetRectCenter(Ai)
call CreateNUnitsAtLoA(1,'h0AP',Player(0),Re,bj_UNIT_FACING)
call RemoveLocation(Re)
call DisableTrigger(GetTriggeringTrigger())
endfunction
