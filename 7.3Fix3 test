call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@귀속ON|r |cff646eff(숫)|r - |cffffA500선택|r |cff646464 선택 유닛 귀속|r |cffff0000(지속)|r  |cff288C28@귀속OFF|r |cff646464 귀속 모드 해제|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@흡혈|r |cff646464 현재 흡혈량 확인|r  |cff288C28@흡혈|r |cff646eff(숫)|r |cff646464 공격시 흡혈 설정|r"+" "))
elseif fi2i==3 then
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@방폭|r |cff646464 방을 폭파|r |cffff0000(취소 불가능)|r  |cff288C28@건설완료|r - |cffffA500선택|r |cff646464 건설중인 건물 완성|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@업완료|r |cff646464 ON/OFF|r |cffff0000업그레이드 시간없앰|r  |cff288C28@유닛완료|r - |cffffA500ON/OFF|r |cff646464 유닛생성 시간없앰|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@정지|r - |cffffA500선택|r |cff646464 선택 유닛 일시정지|r  |cff288C28@정지해제|r - |cffffA500선택|r |cff646464 선택 유닛 정지해제|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@이름|r |cff646eff(문자열)|r |cff646464 이름을 (문자열)로 변경|r  |cff288C28@청소|r |cff646464 화면상 텍스트 모두 제거|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@시야|r |cff646eff(숫)|r |cff646464 숫자만큼 카메라 줌 인/아웃|r |cffff0000(50~150)|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@시야고정|r |cff646464 카메라 시야 고정|r  |cff288C28@시야고정OFF|r |cff646464 카메라 시야 고정 해제|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@치트주기|r |cff646eff(숫)|r |cff646464 치트 권한 부여|r  |cff288C28@치트빼앗기|r |cff646eff(숫)|r |cff646464 치트 권한 빼앗기|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@치트금지|r |cff646eff(숫)|r |cff646464 치트 사용 금지|r  |cff288C28@치트금지|r |cff646effALL|r |cff646464 모두 치트 사용금지|r"+" "))
