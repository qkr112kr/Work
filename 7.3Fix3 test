function Hhv takes nothing returns nothing
if(HGv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(Hcv())then
call CreateNUnitsAtLoA(1,'h094',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("origin",bj_lastCreatedUnit,"t_judgement9.mdx")
else
call CreateNUnitsAtLoA(1,'h094',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
set re=CreateSound("Sound\\Garp.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000몽키.D.거프 : |r|cffFA8072로져는 반드시 내가 체포할 것이다.|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Hfv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function HFv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Hgv))))
call AdjustPlayerStateBJ(-5,GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)
call cd(.12)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
