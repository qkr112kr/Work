function N_v takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function NYv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Nzv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function NZv)))>=1)
endfunction
function N0v takes nothing returns nothing
if(N_v())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(NUv())then
call CreateNUnitsAtLoA(1,'h092',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
call CreateNUnitsAtLoA(1,'h092',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000파이러츠 도킹6 : |r|cffFA8072로빈! 니가와서 완성이야!|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Nwv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function NWv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Nyv))))
call SetUnitAbilityLevelSwapped('A0AV',bj_lastCreatedUnit,2)
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
