set bj_cineModePriorMaskSetting=IsFogMaskEnabled()
set yd=0
loop
exitwhen yd>=bj_MAX_QUEUED_TRIGGERS
set bj_queuedExecTriggers[yd]=null
set bj_queuedExecUseConds[yd]=false
set yd=yd+1
endloop
set bj_isSinglePlayer=false
set Yd=0
set yd=0
loop
exitwhen yd>=$C
if(GetPlayerController(Player(yd))==MAP_CONTROL_USER and GetPlayerSlotState(Player(yd))==PLAYER_SLOT_STATE_PLAYING)then
set Yd=Yd+1
endif
set yd=yd+1
endloop
set bj_isSinglePlayer=(Yd==1)
set bj_rescueSound=CreateSoundFromLabel("Rescue",false,false,false,$2710,$2710)
