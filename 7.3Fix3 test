else
if(GL())then
call DisplayTimedTextToForce(bj_FORCE_PLAYER[3],3.,"|cffffd700(!)|r |cffff0000라인의 유닛이 70마리 이상입니다 !|r")
if(gL())then
call PlaySoundBJ(oa)
endif
endif
endif
endif
call GroupClear(xv)
call DestroyGroup(xv)
set xv=sf(po,Player(6))
set q[4]=CountUnitsInGroup(xv)
call MultiboardSetItemValue(wC[4],I2S(q[4]))
if(PL())then
set wv[5]=(wv[5]+1)
call SetPlayerStateBJ(Player(4),PLAYER_STATE_RESOURCE_GOLD,0)
call SetPlayerStateBJ(Player(4),PLAYER_STATE_RESOURCE_LUMBER,0)
set bj_wantDestroyGroup=true
call ForGroupBJ(sf(GetWorldBounds(),Player(4)),function kL)
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function KL)
call DisplayTimedTextToForce(bj_FORCE_PLAYER[4],20.,"TRIGSTR_11587")
else
if(pL())then
set wv[5]=(wv[5]+1)
call SetPlayerStateBJ(Player(4),PLAYER_STATE_RESOURCE_GOLD,0)
call SetPlayerStateBJ(Player(4),PLAYER_STATE_RESOURCE_LUMBER,0)
set bj_wantDestroyGroup=true
call ForGroupBJ(sf(GetWorldBounds(),Player(4)),function mL)
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function ML)
call DisplayTimedTextToForce(bj_FORCE_PLAYER[4],20.,"TRIGSTR_11589")
else
if(LL())then
call DisplayTimedTextToForce(bj_FORCE_PLAYER[4],3.,"|cffffd700(!)|r |cffff0000라인의 유닛이 70마리 이상입니다 !|r")
if(lL())then
call PlaySoundBJ(oa)
endif
endif
endif
endif
call GroupClear(xv)
call DestroyGroup(xv)
endfunction
function QL takes nothing returns nothing
local timer sL=CreateTimer()
call TimerStart(sL,.7,true,function qL)
set sL=null
endfunction
