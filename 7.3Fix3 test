function g7 takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),500000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endfunction
function G7 takes nothing returns boolean
return(Nx[bx]==5.)
endfunction
function h7 takes nothing returns nothing
set bx=1
loop
exitwhen bx>5
exitwhen GetExpiredTimer()==Ox[bx]
set bx=bx+1
endloop
call SetUnitAnimation(Rx[bx],"spell slam")
set Bx[bx]=Dd(Ix[bx],GetRandomReal(100.,175.),GetRandomReal(0,360))
set Nx[bx]=(Nx[bx]+1)
if(G7())then
call CameraClearNoiseForPlayer(GetOwningPlayer(Rx[bx]))
call CreateNUnitsAtLoA(1,'h0KE',GetOwningPlayer(Rx[bx]),Bx[bx],GetUnitFacing(Rx[bx]))
call CreateNUnitsAtLoA(1,'h0K9',GetOwningPlayer(Rx[bx]),Ax[bx],GetUnitFacing(Rx[bx]))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(600.,Bx[bx],Condition(function F7)),function g7)
call RemoveLocation(Ix[bx])
call RemoveLocation(Ax[bx])
call RemoveLocation(Bx[bx])
call UnitRemoveAbility(Rx[bx],'BNsa')
call PauseTimerBJ(true,Ox[bx])
call IssueImmediateOrderById(Rx[bx],$D0019)
else
call CreateNUnitsAtLoA(1,'h0KE',GetOwningPlayer(Rx[bx]),Bx[bx],GetUnitFacing(Rx[bx]))
call CreateNUnitsAtLoA(1,'h0K9',GetOwningPlayer(Rx[bx]),Ax[bx],GetUnitFacing(Rx[bx]))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(600.,Bx[bx],Condition(function D7)),function f7)
call RemoveLocation(Bx[bx])
endif
endfunction
