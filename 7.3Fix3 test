function Getting_StopCheat2 takes nothing returns nothing
local integer i2i=0
if Selected_Check[(20*2)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
loop
exitwhen(StopCheatUnit[i2i]==null)
set i2i=i2i+1
endloop
if(IsUnitPausedBJ(GetTriggerUnit())==true)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 정지한 유닛입니다. 정지하지 않은 유닛을 선택해 주세요.|r"+" "))
else
if(GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다! 정지할 수 없습니다!|r")))
else
set StopCheatUnit[i2i]=GetTriggerUnit()
call PauseUnitBJ(true,GetTriggerUnit())
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+("|r |cff2c952c을(를) |cffffbe0a정지|r|cff2c952c시킵니다.|r"))))
endif
endif
if GetTriggerPlayer()==Selected_Check[(20*12)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*2)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*12)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_StopCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*2)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
elseif fi2i==63 then
set Selected_Check[(20*2)+GetPlayerId(GetTriggerPlayer())]=null
else
set Selected_Check[(20*2)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set Selected_Check[(20*12)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endif
endfunction
function Getting_Stopoff takes nothing returns nothing
local integer i2i=0
if(StopCheatUnit[i2i]!=null)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000"+("정지시킨 모든 유닛"+"|r |cff2c952c을 다시|r |cffffbe0a움직이게|r |cff2c952c합니다.|r")))
loop
exitwhen StopCheatUnit[i2i]==null
call PauseUnitBJ(false,StopCheatUnit[i2i])
set StopCheatUnit[i2i]=null
set i2i=i2i+1
endloop
else
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff0000"+"주의! 정지된 유닛이 없습니다.")+"|r"))
endif
endfunction
function Getting_DeleteCheat2 takes nothing returns nothing
if Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(GetTriggerUnit()==udg_rcUnit[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 치트 선택 유닛입니다! 삭제할 수 없습니다!|r")))
else
if(GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다! 정지할 수 없습니다!|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+"|r |cff2c952c을(를) 게임 상에서|r |cffffbe0a삭제|r|cff2c952c합니다.|r")))
call RemoveUnit(GetTriggerUnit())
endif
endif
if GetTriggerPlayer()==Selected_Check[(20*13)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*13)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_DeleteCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
elseif fi2i==63 then
set Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]=null
else
set Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set Selected_Check[(20*13)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endif
endfunction
function Getting_KillCheat2 takes nothing returns nothing
if Selected_Check[(20*4)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(GetTriggerUnit()==udg_rcUnit[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 치트 선택 유닛입니다! 살해할 수 없습니다. 자살을 이용하세요!|r")))
else
if GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())]then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다! 살해할 수 없습니다!|r")))
else
if(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]==null)then
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+"|r |cff2c952c을(를) 즉시|r |cffffbe0a살해|r|cff2c952c합니다.|r")))
call KillUnit(GetTriggerUnit())
else
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+"|r |cff2c952c을(를) 즉시|r |cffffbe0a살해|r|cff2c952c합니다.|r |cffff0000( 선택 유닛이 있기에 돈과 경험치를 획득했습니다.)|r")))
call UnitDamageTargetBJ(udg_rcUnit[GetPlayerId(GetTriggerPlayer())],GetTriggerUnit(),1000000000.00,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call KillUnit(GetTriggerUnit())
endif
endif
endif
if GetTriggerPlayer()==Selected_Check[(20*14)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*4)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*14)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_KillCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*4)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
elseif fi2i==63 then
set Selected_Check[(20*4)+GetPlayerId(GetTriggerPlayer())]=null
else
set Selected_Check[(20*4)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set Selected_Check[(20*14)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endif
endfunction
function Getting_MapCheat takes integer fi2i returns nothing
if fi2i==31 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffffbe0a맵핵|r|cff2c952c을 |cffff0000가동|r |cff2c952c했습니다.|r |cffff0000(동맹은 켜지지 않습니다.)|r"+" "))
call CreateFogModifierRectBJ(true,Player(PLAYER_NEUTRAL_AGGRESSIVE),FOG_OF_WAR_VISIBLE,GetEntireMapRect())
call SetPlayerAllianceStateBJ(Player(PLAYER_NEUTRAL_AGGRESSIVE),GetTriggerPlayer(),bj_ALLIANCE_UNALLIED_VISION)
elseif fi2i==63 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffffbe0a맵핵|r|cff2c952c을|r |cffff0000중단|r |cff2c952c했습니다.|r"+" "))
call SetPlayerAllianceStateBJ(Player(PLAYER_NEUTRAL_AGGRESSIVE),GetTriggerPlayer(),bj_ALLIANCE_UNALLIED)
endif
endfunction
function Getting_AddAttack takes nothing returns nothing
local integer i2i=GetPlayerId(GetOwningPlayer(GetEventDamageSource()))
if GetEventDamageSource()==udg_rcUnit[i2i]and IsUnitIllusionBJ(GetEventDamageSource())!=true then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTarget(udg_rcUnit[i2i],GetTriggerUnit(),EndlessR[180+i2i],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_ENHANCED,WEAPON_TYPE_WHOKNOWS)
call EnableTrigger(GetTriggeringTrigger())
elseif udg_rcPlayer[i2i]==GetOwningPlayer(GetEventDamageSource())and DIALOG3[20+i2i]==true then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTarget(GetEventDamageSource(),GetTriggerUnit(),EndlessR[140+i2i],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_ENHANCED,WEAPON_TYPE_WHOKNOWS)
call EnableTrigger(GetTriggeringTrigger())
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function Getting_BasicAttack takes nothing returns nothing
local trigger t2t=CreateTrigger()
call TriggerRegisterUnitEvent(t2t,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
call TriggerAddAction(t2t,function Getting_AddAttack)
set t2t=null
endfunction
function Getting_AddDamageCheat takes unit fu2u,integer fi2i returns nothing
set EndlessR[180+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(ADDDAMAGE)
endfunction
function Getting_AddDamageGroupCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==0 then
set DIALOG3[40+GetPlayerId(GetTriggerPlayer())]=false
else
set DIALOG3[20+GetPlayerId(GetTriggerPlayer())]=true
endif
set EndlessR[140+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(ADDDAMAGE)
endfunction
function Getting_ReduceDamageCheat2 takes nothing returns nothing
local integer i2i=0
loop
exitwhen i2i>11
if(GetTriggerUnit()==udg_rcUnit[i2i])then
call SetUnitLifeBJ(GetTriggerUnit(),(GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())+(GetEventDamage()*(EndlessR[120+i2i]*0.01))))
endif
set i2i=i2i+1
endloop
endfunction
function Getting_ReduceDamageCheat takes unit fu2u,integer fi2i returns nothing
set EndlessR[120+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(REDUCE)
endfunction
function Getting_BloodCheat3 takes nothing returns nothing
local integer i2i=0
loop
exitwhen i2i>11
if(GetEventDamageSource()==udg_rcUnit[i2i]and GetEventDamage()>0.00)then
call SetUnitLifeBJ(GetEventDamageSource(),(GetUnitStateSwap(UNIT_STATE_LIFE,GetEventDamageSource())+(EndlessR[100+i2i]*GetEventDamage())))
endif
set i2i=i2i+1
endloop
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function Getting_BloodCheat2 takes nothing returns nothing
local integer i2i=0
local trigger t=CreateTrigger()
call TriggerRegisterUnitEvent(t,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
loop
exitwhen i2i>11
if(GetAttacker()==udg_rcUnit[i2i])then
call TriggerAddAction(t,function Getting_BloodCheat3)
endif
set i2i=i2i+1
endloop
endfunction
function Getting_BloodCheat takes unit fu2u,integer fi2i returns nothing
set EndlessR[100+GetPlayerId(GetTriggerPlayer())]=I2R(fi2i)*0.01
call EnableTrigger(BLOOD)
endfunction
function Skilldam_Check takes nothing returns nothing
if GetAttacker()==udg_rcUnit[GetPlayerId(GetOwningPlayer(GetAttacker()))]then
set DIALOG3[60+GetPlayerId(GetOwningPlayer(GetAttacker()))]=true
endif
endfunction
function Getting_Skilldamage2 takes nothing returns nothing
local integer i2i=GetPlayerId(GetOwningPlayer(GetEventDamageSource()))
if DIALOG3[60+i2i]==true then
set DIALOG3[60+i2i]=false
elseif GetEventDamage()>0.00 and GetEventDamageSource()==udg_rcUnit[i2i]then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTarget(udg_rcUnit[i2i],GetTriggerUnit(),EndlessR[160+i2i],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_ENHANCED,WEAPON_TYPE_WHOKNOWS)
call EnableTrigger(GetTriggeringTrigger())
elseif GetOwningPlayer(GetEventDamageSource())==udg_rcPlayer[i2i]and DIALOG3[40+i2i]==true then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTarget(udg_rcUnit[i2i],GetTriggerUnit(),EndlessR[140+i2i],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_ENHANCED,WEAPON_TYPE_WHOKNOWS)
call EnableTrigger(GetTriggeringTrigger())
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function Skilldamage_Actions takes nothing returns nothing
local trigger t2t=CreateTrigger()
call TriggerRegisterUnitEvent(t2t,GetEnumUnit(),EVENT_UNIT_DAMAGED)
call TriggerRegisterAnyUnitEventBJ(t2t,EVENT_PLAYER_UNIT_SPELL_CHANNEL)
call TriggerAddAction(t2t,function Getting_Skilldamage2)
set t2t=null
endfunction
function Skilldamage_Condition takes nothing returns boolean
return(GetWidgetLife(GetFilterUnit())>0.405 and IsUnitType(GetFilterUnit(),UNIT_TYPE_MAGIC_IMMUNE)==false and GetUnitAbilityLevel(GetFilterUnit(),'Aloc')==0 and IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))==true)
endfunction
function Getting_Skilldamage takes nothing returns nothing
set bj_wantDestroyGroup=true
if GetTriggerUnit()==udg_rcUnit[GetPlayerId(GetTriggerPlayer())]then
call ForGroupBJ(GetUnitsInRangeOfLocMatching(2500.,GetUnitLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]),Condition(function Skilldamage_Condition)),function Skilldamage_Actions)
endif
endfunction
function Getting_SkillAddDamageCheat takes unit fu2u,integer fi2i returns nothing
set EndlessR[160+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(SKILLDAM)
call EnableTrigger(SKILLDAM2)
endfunction
function Getting_SkillAddDamageGroupCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==0 then
set DIALOG3[40+GetPlayerId(GetTriggerPlayer())]=false
else
set DIALOG3[40+GetPlayerId(GetTriggerPlayer())]=true
endif
set EndlessR[140+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(SKILLDAM)
call EnableTrigger(SKILLDAM2)
endfunction
function Getting_CopyCheat2 takes nothing returns nothing
local integer i2i=1
local integer i3i=1
local unit u2u=GetTriggerUnit()
if Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
loop
exitwhen i2i>CopyI[GetPlayerId(GetTriggerPlayer())]
call CreateNUnitsAtLoA(1,GetUnitTypeId(u2u),udg_rcPlayer[GetPlayerId(GetTriggerPlayer())],GetUnitLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]),bj_UNIT_FACING)
call SetHeroLevelBJ(GetLastCreatedUnit(),GetHeroLevel(u2u),false)
call SetHeroStr(GetLastCreatedUnit(),GetHeroStatBJ(bj_HEROSTAT_STR,u2u,false),true)
call SetHeroAgi(GetLastCreatedUnit(),GetHeroStatBJ(bj_HEROSTAT_AGI,u2u,false),true)
call SetHeroInt(GetLastCreatedUnit(),GetHeroStatBJ(bj_HEROSTAT_INT,u2u,false),true)
loop
exitwhen i3i>6
call UnitAddItemByIdSwapped(GetItemTypeId(UnitItemInSlotBJ(u2u,i3i)),GetLastCreatedUnit())
call SetItemCharges(GetLastCreatedItem(),GetItemCharges(UnitItemInSlotBJ(u2u,i3i)))
call UnitDropItemSlotBJ(GetLastCreatedUnit(),GetLastCreatedItem(),i3i)
set i3i=i3i+1
endloop
set i2i=i2i+1
endloop
call ND2P(GetTriggerPlayer(),0,0,5.00,((("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c의|r |cffff0000"))+GetUnitName(GetTriggerUnit()))+(("|r |cff2c952c을(를)|r |cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+("|r |cff2c952c의 옆에|r |cffff0000"+(I2S(CopyI[GetPlayerId(GetTriggerPlayer())])+"기|r|cff2c952c만큼|r |cffffbe0a복제|r|cff2c952c했습니다.|r"))))))
if GetTriggerPlayer()==Selected_Check[(20*17)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*17)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_CopyCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set CopyI[GetPlayerId(GetTriggerPlayer())]=1
elseif fi2i==63 then
set Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]=null
else
set Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set Selected_Check[(20*17)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
if fi2i==0 then
set CopyI[GetPlayerId(GetTriggerPlayer())]=1
else
set CopyI[GetPlayerId(GetTriggerPlayer())]=fi2i
endif
endif
endfunction
function Getting_Copy2Cheat2 takes nothing returns nothing
local integer i2i=1
local integer i3i=1
local unit u2u=GetTriggerUnit()
if Selected_Check[(20*8)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
loop
exitwhen i2i>CopyI[20+GetPlayerId(GetTriggerPlayer())]
call CreateNUnitsAtLoA(1,GetUnitTypeId(u2u),GetOwningPlayer(u2u),GetUnitLoc(u2u),bj_UNIT_FACING)
call SetHeroLevelBJ(u2u,GetHeroLevel(u2u),false)
call SetHeroStr(u2u,GetHeroStatBJ(bj_HEROSTAT_STR,u2u,false),true)
call SetHeroAgi(u2u,GetHeroStatBJ(bj_HEROSTAT_AGI,u2u,false),true)
call SetHeroInt(u2u,GetHeroStatBJ(bj_HEROSTAT_INT,u2u,false),true)
loop
exitwhen i3i>6
call UnitAddItemByIdSwapped(GetItemTypeId(UnitItemInSlotBJ(u2u,i3i)),u2u)
call SetItemCharges(GetLastCreatedItem(),GetItemCharges(UnitItemInSlotBJ(u2u,i3i)))
call UnitDropItemSlotBJ(u2u,GetLastCreatedItem(),i3i)
set i3i=i3i+1
endloop
set i2i=i2i+1
endloop
call ND2P(GetTriggerPlayer(),0,0,5.00,((("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c의|r |cffff0000"))+GetUnitName(GetTriggerUnit()))+(("|r |cff2c952c을(를)|r |cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+("|r |cff2c952c의 옆에|r |cffff0000"+(I2S(CopyI[20+GetPlayerId(GetTriggerPlayer())])+"기|r|cff2c952c만큼|r |cffffbe0a복제|r|cff2c952c했습니다.|r"))))))
if GetTriggerPlayer()==Selected_Check[(20*18)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*8)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*18)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_Copy2Cheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*8)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set CopyI[20+GetPlayerId(GetTriggerPlayer())]=1
elseif fi2i==63 then
set Selected_Check[(20*8)+GetPlayerId(GetTriggerPlayer())]=null
else
set Selected_Check[(20*8)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set Selected_Check[(20*18)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
if fi2i==0 then
set CopyI[20+GetPlayerId(GetTriggerPlayer())]=1
else
set CopyI[20+GetPlayerId(GetTriggerPlayer())]=fi2i
endif
endif
endfunction
function Getting_CopyItemCheat2 takes nothing returns nothing
local item array CopyItem
if Selected_Check[(20*9)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[GetPlayerId(GetTriggerPlayer())])==null)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 선택한 슬롯은 비어있습니다.|r"+""))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,((("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+GetUnitName(GetTriggerUnit()))+("|r |cff2c952c의|r |cffff0000"+(I2S(ItemCopyI[GetPlayerId(GetTriggerPlayer())])+"|r|cff2c952c번째 인벤토리의 아이템을|r |cffffbe0a복제|r|cff2c952c했습니다.|r"))))
set CopyItem[GetPlayerId(GetTriggerPlayer())]=UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[GetPlayerId(GetTriggerPlayer())])
call UnitAddItemByIdSwapped(GetItemTypeId(CopyItem[GetPlayerId(GetTriggerPlayer())]),udg_rcUnit[GetPlayerId(GetTriggerPlayer())])
call SetItemCharges(GetLastCreatedItem(),GetItemCharges(UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[GetPlayerId(GetTriggerPlayer())])))
call UnitDropItemSlotBJ(udg_rcUnit[GetPlayerId(GetTriggerPlayer())],GetLastCreatedItem(),ItemCopyI[GetPlayerId(GetTriggerPlayer())])
endif
set Selected_Check[(20*9)+GetPlayerId(GetTriggerPlayer())]=null
endif
endfunction
function Getting_CopyItemCheat takes unit fu2u,integer fi2i returns nothing
set ItemCopyI[GetPlayerId(GetTriggerPlayer())]=fi2i
set Selected_Check[(20*9)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endfunction
function Getting_DeleteItemCheat2 takes nothing returns nothing
if Selected_Check[(20*10)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[20+GetPlayerId(GetTriggerPlayer())])==null)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 선택한 슬롯은 비어있습니다.|r"+""))
else
if GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())]then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다! 아이템을 삭제 할 수 없습니다!|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,((("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+GetUnitName(GetTriggerUnit()))+("|r |cff2c952c의|r |cffff0000"+(I2S(ItemCopyI[20+GetPlayerId(GetTriggerPlayer())])+"|r|cff2c952c번째 인벤토리의 아이템을|r |cffffbe0a삭제|r|cff2c952c했습니다.|r"))))
call RemoveItem(UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[20+GetPlayerId(GetTriggerPlayer())]))
endif
endif
endif
set Selected_Check[(20*10)+GetPlayerId(GetTriggerPlayer())]=null
endfunction
function Getting_DeleteItemCheat takes unit fu2u,integer fi2i returns nothing
set ItemCopyI[20+GetPlayerId(GetTriggerPlayer())]=fi2i
set Selected_Check[(20*10)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endfunction
function Getting_ReiterationItemCheat takes unit fu2u,integer fi2i,integer fi3i returns nothing
if UnitItemInSlotBJ(fu2u,fi2i)==null then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 선택한 슬롯은 비어있습니다.|r"+""))
else
if(GetItemCharges(UnitItemInSlotBJ(fu2u,fi2i))==0)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 선택한 슬롯의 아이템은 중첩할 수 없는 종류의 아이템입니다.|r"+""))
else
if(fi3i<=0)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 아이템의 중첩 수를 0이나 음수로 할 수 없습니다.|r"+""))
else
call SetItemCharges(UnitItemInSlotBJ(fu2u,fi2i),fi3i)
endif
endif
endif
endfunction
function Getting_KickCheat takes integer fi2i returns nothing
set fi2i=fi2i-1
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
if GT_Privilege[fi2i]>=GT_Privilege[GetPlayerId(GetTriggerPlayer())]then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(Player(fi2i))+"는 이미 치트를 사용하고 있습니다. 당신과 동등한 권한을 가지고 있기 때문에 빼앗지 못합니다.|r")))
call ND2P(Player(fi2i),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+" 이(가) 당신을 강퇴하려고 시도했습니다.|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cff"+Player_Color[fi2i]+(GetPlayerName(Player(fi2i))+"|r |cffB4C3FF를|r |cffffbe0a강퇴|r|cffB4C3FF시켰습니다.|r")))
call KickcheatP(Player(fi2i))
endif
endif
endfunction
function CheckI takes integer ITEM returns integer
if ModuloInteger(ITEM,128)==57 then
set ITEM=ITEM+8
elseif ModuloInteger(ITEM,128)==90 then
set ITEM=ITEM+214
else
set ITEM=ITEM+1
endif
return ITEM
endfunction
function Getting_AllItemCheat takes unit fu2u returns nothing
local integer ITEM='I000'
loop
exitwhen ITEM>'I1ZZ'
call CreateItemLoc(ITEM,GetUnitLoc(fu2u))
set ITEM=CheckI(ITEM)
endloop
endfunction
function Getting_ChatCheat2 takes nothing returns nothing
local integer i2i=0
loop
exitwhen i2i>11
if IsPlayerInForce(Player(i2i),Chat_Hear)then
call ND2P(Player(i2i),0,0,15,"|cff"+Player_Color[GetPlayerId(GetTriggerPlayer())]+GetPlayerName(GetTriggerPlayer())+"|r : "+GetEventPlayerChatString())
endif
set i2i=i2i+1
endloop
endfunction
function Getting_ChatCheat takes integer fi2i,player p2p returns nothing
if fi2i==31 then
call Getting_CheatActions(11,CHATTING,function Getting_ChatCheat2)
call ForceAddPlayer(Chat_Hear,p2p)
call ND2P(GetTriggerPlayer(),0,0,5.,("|cffDC6089모든 플레이어의 채팅을 봅니다."+""))
elseif fi2i==63 then
call ForceRemovePlayer(Chat_Hear,p2p)
call ND2P(GetTriggerPlayer(),0,0,5.,("|cffDC6089모든 플레이어의 채팅을 그만봅니다."+""))
endif
endfunction
function Getting_StopGameCheat takes integer fi2i returns nothing
set fi2i=fi2i-1
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
if(GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(Player(fi2i))+"는 당신과 동등한 권한 또는 상위 권한을 가지고 있기 때문에 게임금지를 못합니다.|r")))
call ND2P(Player(fi2i),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+" 이(가) 당신을 게임금지 하려고 시도했습니다.|r")))
else
call SetUserControlForceOff(GetForceOfPlayer(Player(fi2i)))
endif
endif
endfunction
function Getting_EndlessHealingCheat takes integer fi2i returns nothing
if fi2i==31 then
set CUnitC[40+GetPlayerId(GetTriggerPlayer())]=GetPlayerId(GetTriggerPlayer())
set CUnitC[60+GetPlayerId(GetTriggerPlayer())]=GetPlayerId(GetTriggerPlayer())
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c의|r |cffffbe0a체력/마나|r|cff2c952c를|r |cffffbe0a무한|r|cff2c952c으로|r |cffffbe0a회복|r|cff2c952c시킵니다.|r")))
call EnableTrigger(ENDLESS)
elseif fi2i==63 then
set CUnitC[40+GetPlayerId(GetTriggerPlayer())]=100
set CUnitC[60+GetPlayerId(GetTriggerPlayer())]=100
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c의|r |cffffbe0a무한 체력/마나 회복|r |cff2c952c능력을|r |cffffbe0a제거|r|cff2c952c합니다.|r")))
endif
endfunction
