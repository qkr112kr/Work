endfunction
function Fu takes nothing returns nothing
if(Bu())then
call lf(GetAttacker(),GetTriggerUnit(),600000.,1.,1.,ATTACK_TYPE_NORMAL,DAMAGE_TYPE_UNIVERSAL)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0E8',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(550.,ov,Condition(function Nu)),function bu)
call RemoveLocation(ov)
endif
if(cu())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0EB',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call UnitApplyTimedLifeBJ(3.,'BTLF',bj_lastCreatedUnit)
call RemoveLocation(ov)
endif
if(fu())then
if(Du())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0FE',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitAnimation(bj_lastCreatedUnit,"attack")
call SetUnitTimeScale(bj_lastCreatedUnit,.8)
call CreateNUnitsAtLoA(1,'h0FD',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call SetUnitTimeScale(bj_lastCreatedUnit,2.3)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(600.,ov,Condition(function Cu)),function du)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endif
endfunction
function Trig_Legend12_Func002Func002002002001 takes nothing returns boolean
