function cW takes nothing returns boolean
return(GetRandomInt(1,25)==5)
endfunction
function CW takes nothing returns boolean
return(GetUnitAbilityLevelSwapped('A092',GetAttacker())==2)and(GetRandomInt(1,33)==5)
endfunction
function dW takes nothing returns boolean
return(IsPlayerEnemy(GetOwningPlayer(GetFilterUnit()),GetOwningPlayer(GetAttacker())))
endfunction
function DW takes nothing returns nothing
call lf(GetAttacker(),GetEnumUnit(),300000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_NORMAL)
endfunction
function fW takes nothing returns boolean
return(GetRandomInt(1,7)==3)and(GetUnitAbilityLevelSwapped('A092',GetAttacker())==2)
endfunction
function FW takes nothing returns nothing
if(cW())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0AZ',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0ED')
call SetUnitAbilityLevelSwapped('A0ED',bj_lastCreatedUnit,GetUnitAbilityLevelSwapped('A092',GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D0085,GetAttacker())
call RemoveLocation(ov)
endif
if(CW())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0J1',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call CreateNUnitsAtLoA(1,'h0AQ',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitVertexColorBJ(bj_lastCreatedUnit,'d',100.,100.,45.)
call SetUnitAnimation(bj_lastCreatedUnit,"spell throw")
call RemoveLocation(ov)
endif
if(fW())then
set ov=GetUnitLoc(GetTriggerUnit())
set Xe=Dd(ov,GetRandomReal(0,105.),GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0J2',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(GetAttacker()))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(350.,Xe,Condition(function dW)),function DW)
call RemoveLocation(Xe)
call RemoveLocation(ov)
endif
endfunction
function GW takes nothing returns boolean
return(GetSpellAbilityId()=='A0DZ')
endfunction
function hW takes nothing returns boolean
