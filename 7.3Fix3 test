function CS takes nothing returns nothing
if(cS())then
if(BS())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0FA',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(600.,ov,Condition(function NS)),function bS)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0FA',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(600.,ov,Condition(function IS)),function AS)
call RemoveLocation(ov)
endif
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
