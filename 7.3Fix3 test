function VW takes nothing returns nothing
if(aW())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0JS',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
if(oW())then
call UnitDamageTargetBJ(bj_lastCreatedUnit,GetTriggerUnit(),(.06*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
else
call UnitDamageTargetBJ(bj_lastCreatedUnit,GetTriggerUnit(),130000.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
call CreateNUnitsAtLoA(1,'h0GE',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitTimeScalePercent(bj_lastCreatedUnit,140.)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(550.,ov,Condition(function rW)),function iW)
call RemoveLocation(ov)
endif
if(nW())then
call SetUnitAnimation(GetAttacker(),"spell slam")
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0JT',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitTimeScalePercent(bj_lastCreatedUnit,140.)
call IssueTargetOrderById(bj_lastCreatedUnit,$D007F,GetTriggerUnit())
call RemoveLocation(ov)
endif
endfunction
function XW takes nothing returns boolean
return(GetRandomInt(1,$A)==5)
endfunction
function OW takes nothing returns boolean
return(R2I(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker()))=='x')and(GetUnitAbilityLevelSwapped('A0RS',GetAttacker())==1)
