function Fgv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function FAv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function FNv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fbv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function FBv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fcv)))>=1)
endfunction
function FGv takes nothing returns nothing
if(Fgv())then
set re=CreateSound("Sound\\Zoro.mp3",false,false,false,$A,$A,"")
call StartSound(re)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(FIv())then
call CreateNUnitsAtLoA(1,'h08G',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
call CreateNUnitsAtLoA(1,'h08G',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000롤로노아 조로 : |r|cffFA8072악연이든 기연이든 역귀를 태워버린 너희의 운명을 탓해라!|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function FCv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fdv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function FDv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Ffv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function FFv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
