function Giv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function g5v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function g6v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function g7v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function g8v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function g9v)))>=1)
endfunction
function Gav takes nothing returns nothing
if(Giv())then
set re=CreateSound("Sound\\Franky.mp3",false,false,false,$A,$A,"")
call StartSound(re)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(g4v())then
call CreateNUnitsAtLoA(1,'h05G',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
if(g2v())then
call CreateNUnitsAtLoA(1,'h05G',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"fairywing.MDX")
else
call CreateNUnitsAtLoA(1,'h05G',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000커티 프람 : |r|cffFA8072슈퍼 타오르는군!!!!|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Gvv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Gev))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Gxv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Gov))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Grv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
