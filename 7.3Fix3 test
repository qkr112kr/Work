function cH takes nothing returns nothing
if(Wh())then
call AdjustPlayerStateBJ($A,Player(0),PLAYER_STATE_RESOURCE_GOLD)
endif
if(Yh())then
call AdjustPlayerStateBJ($A,Player(1),PLAYER_STATE_RESOURCE_GOLD)
endif
if(Zh())then
call AdjustPlayerStateBJ($A,Player(3),PLAYER_STATE_RESOURCE_GOLD)
endif
if(eH())then
call AdjustPlayerStateBJ($A,Player(4),PLAYER_STATE_RESOURCE_GOLD)
endif
if(oH())then
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',Player(0),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(iH())then
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',Player(1),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(nH())then
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',Player(3),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(EH())then
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',Player(4),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(OH())then
call AdjustPlayerStateBJ(1,Player(0),PLAYER_STATE_RESOURCE_LUMBER)
endif
if(IH())then
call AdjustPlayerStateBJ(1,Player(1),PLAYER_STATE_RESOURCE_LUMBER)
endif
if(NH())then
call AdjustPlayerStateBJ(1,Player(3),PLAYER_STATE_RESOURCE_LUMBER)
endif
if(BH())then
call AdjustPlayerStateBJ(1,Player(4),PLAYER_STATE_RESOURCE_LUMBER)
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|CFFFFFA7820회까지의 세이브 회수의 보상이 지급됩니다. (세이브는 하드부터 클리어 후 가능합니다.)|r")
call DisableTrigger(GetTriggeringTrigger())
endfunction
