return(GetRandomInt(1,25)==16)
endfunction
function Z7 takes nothing returns nothing
set ov=GetUnitLoc(GetAttacker())
set Xe=Dd(ov,GetRandomReal(0,400.),GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0FN',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call UnitApplyTimedLife(bj_lastCreatedUnit,'Bmec',2)
call UnitAddAbility(bj_lastCreatedUnit,'A0P7')
call IssueTargetOrderById(bj_lastCreatedUnit,$D0214,GetTriggerUnit())
call RemoveLocation(Xe)
set Xe=Dd(ov,GetRandomReal(0,400.),GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0FN',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call UnitApplyTimedLife(bj_lastCreatedUnit,'Bmec',2)
call UnitAddAbility(bj_lastCreatedUnit,'A0PA')
call IssueTargetOrderById(bj_lastCreatedUnit,$D0214,GetTriggerUnit())
call RemoveLocation(Xe)
set Xe=Dd(ov,GetRandomReal(0,400.),GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0FN',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call UnitApplyTimedLife(bj_lastCreatedUnit,'Bmec',2)
call UnitAddAbility(bj_lastCreatedUnit,'A0P6')
call IssueTargetOrderById(bj_lastCreatedUnit,$D0214,GetTriggerUnit())
call RemoveLocation(Xe)
call RemoveLocation(ov)
