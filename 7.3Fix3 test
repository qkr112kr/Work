function Gdv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function GRv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function GIv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function GAv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function GNv)))>=1)
endfunction
function GDv takes nothing returns nothing
if(Gdv())then
set re=CreateSound("Sound\\Akainu.mp3",false,false,false,$A,$A,"")
call StartSound(re)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(GOv())then
call CreateNUnitsAtLoA(1,'h03Z',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
if(GEv())then
call CreateNUnitsAtLoA(1,'h03Z',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"fairywing.MDX")
else
call CreateNUnitsAtLoA(1,'h03Z',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000아카이누 : |r|cffFA8072올바르지 못한 해병은 필요없다.|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Gbv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function GBv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Gcv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function GCv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
