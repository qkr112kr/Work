function hsv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hjv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hJv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hkv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hKv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hlv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hLv)))>=1)
endfunction
function hSv takes nothing returns nothing
if(hsv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(hHv())then
call CreateNUnitsAtLoA(1,'h0EE',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"anwing2.mdx")
call AddSpecialEffectTargetUnitBJ("origin",bj_lastCreatedUnit,"wing.mdx")
else
call CreateNUnitsAtLoA(1,'h0EE',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
set re=CreateSound("Sound\\sabo.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000혁명군참모총장-사보 : |r|cffFA8072그녀석의 의지는 우리들이 이어가는거야|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hmv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hMv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hpv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hPv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hqv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hQv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
