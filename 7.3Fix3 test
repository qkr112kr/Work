endfunction
function WM takes nothing returns nothing
if(wM())then
if(uM())then
set ov=GetRectCenter(gr)
call CreateNUnitsAtLoA(1,'e003',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function qM))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function QM))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function sM))))
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF8200"+("☆★         행운의 토큰사용 !!         ★☆|r")))
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cff2040f0"+("☆★         특별함 위습 획득 !!         ★☆|r")))
else
set ov=GetRectCenter(Gr)
call CreateNUnitsAtLoA(1,'e004',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function SM))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function tM))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function TM))))
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF8200"+("☆★         행운의 토큰사용 !!         ★☆|r")))
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffff00ff"+("☆★         희귀함 위습 획득 !!         ★☆|r")))
endif
endif
endfunction
