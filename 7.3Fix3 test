function lvv takes nothing returns nothing
set le=1
loop
exitwhen le>5
exitwhen RectContainsUnit(ke[le],GetDyingUnit())
set le=le+1
endloop
set Xe=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0JX',Player(-1+(le)),Xe,bj_UNIT_FACING)
call RemoveLocation(Xe)
if(KTv())then
set je=1
loop
exitwhen je>5
exitwhen RectContainsUnit(ke[je],GetDyingUnit())
set je=je+1
endloop
call DestroyTimerDialog(He[le])
set ov=GetRectCenter(Ii)
call RemoveLocation(ov)
call PauseTimerBJ(true,he[le])
call AdjustPlayerStateBJ($7D0,Player(-1+(le)),PLAYER_STATE_RESOURCE_GOLD)
call AdjustPlayerStateBJ(2,Player(-1+(le)),PLAYER_STATE_RESOURCE_LUMBER)
call DisplayTimedTextToForce(pd(Player(-1+(le))),5.,"|cffffd700(!)|r |cffff0000스모커를 제압하여 2000골드와 목재 2 획득!|r")
if(KSv())then
if(Ksv())then
call UnitRemoveAbility(Le[le],'A0IC')
call UnitAddAbility(Le[le],'A0IG')
call AdjustPlayerStateBJ(1,Player(-1+(le)),PLAYER_STATE_RESOURCE_LUMBER)
call DisplayTimedTextToForce(pd(Player(-1+(le))),5.,"TRIGSTR_16395")
set ov=GetRectCenter(br)
call CreateNUnitsAtLoA(1,'e001',Player(-1+(le)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
else
call IncUnitAbilityLevel(Le[le],'A0IC')
endif
endif
call CreateNUnitsAtLoA(1,'h0IY',Player(-1+(le)),ov,bj_UNIT_FACING)
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(Ii,Player(8)),function Ktv)
endif
if(Kwv())then
call DestroyTimerDialog(Ae[le])
call PauseTimerBJ(true,Ne[le])
call AdjustPlayerStateBJ(1,Player(-1+(le)),PLAYER_STATE_RESOURCE_LUMBER)
call DisplayTimedTextToForce(pd(Player(-1+(le))),5.,"|cffffd700(!)|r |cffff0000알라바스타의 도적을 처치하여 1의 목재 획득 !|r")
if(Kuv())then
call PlaySoundBJ(aa)
endif
if(KUv())then
call IncUnitAbilityLevel(Le[le],'A04S')
call DisplayTimedTextToForce(pd(Player(-1+(le))),10.,"TRIGSTR_15617")
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',Player(-1+(le)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
endif
if(KYv())then
call DestroyTimerDialog(Ae[le])
call PauseTimerBJ(true,Ne[le])
call AdjustPlayerStateBJ(1,Player(-1+(le)),PLAYER_STATE_RESOURCE_LUMBER)
set ov=GetRectCenter(Fr)
call CreateNUnitsAtLoA(1,'e002',Player(-1+(le)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(Player(-1+(le))),5.,"|cffffd700(!)|r |cffff0000와포루를 처치하여 안흔함 위습 1기와 목재 1 획득!|r")
set ov=GetRectCenter(Ii)
if(KWv())then
call PlaySoundBJ(aa)
endif
call CreateNUnitsAtLoA(1,'h0IY',Player(-1+(le)),ov,bj_UNIT_FACING)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call RemoveLocation(ov)
if(Kyv())then
call DisplayTimedTextToForce(pd(Player(-1+(le))),10.,"TRIGSTR_15616")
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',Player(-1+(le)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
call IncUnitAbilityLevel(Le[le],'A04S')
endif
endif
if(K0v())then
call DestroyTimerDialog(Ae[le])
call PauseTimerBJ(true,Ne[le])
set ov=GetRectCenter(gr)
call CreateNUnitsAtLoA(1,'e003',Player(-1+(le)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(Player(-1+(le))),5.,"|cffffd700(!)|r |cffff0000모리아를 처치하여 특별함 위습 1기 획득 !|r")
if(KZv())then
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',Player(-1+(le)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
if(Kzv())then
call UnitRemoveAbility(Le[le],'A04S')
call UnitAddAbility(Le[le],'A0HA')
call AdjustPlayerStateBJ(2,Player(-1+(le)),PLAYER_STATE_RESOURCE_LUMBER)
call DisplayTimedTextToForce(pd(Player(-1+(le))),10.,"TRIGSTR_15525")
else
call DisplayTimedTextToForce(pd(Player(-1+(le))),10.,"TRIGSTR_15526")
call IncUnitAbilityLevel(Le[le],'A04S')
endif
endif
if(K_v())then
call PlaySoundBJ(aa)
endif
endif
if(K2v())then
call DestroyTimerDialog(Qx[le])
call PauseTimerBJ(true,qx[le])
set ov=GetRectCenter(br)
call CreateNUnitsAtLoA(1,'e001',Player(-1+(le)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(Player(-1+(le))),5.,"|cffffd700(!)|r |cffff0000바제스를 쓰러트린 보상으로 흔함 선택위습 1기 획득 !|r")
if(K1v())then
call PlaySoundBJ(aa)
endif
endif
if(K5v())then
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"TRIGSTR_6533")
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(K3v())then
set ov=GetRectCenter(Fr)
call CreateNUnitsAtLoA(1,'e002',Player(-1+(bj_forLoopAIndex)),ov,bj_UNIT_FACING)
call UnitRemoveAbility(me[bj_forLoopAIndex],'A0HF')
call UnitAddAbility(me[bj_forLoopAIndex],'A040')
call RemoveLocation(ov)
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call SetUnitAbilityLevelSwapped('A0TP',Le[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))],(GetUnitAbilityLevelSwapped('A0TP',Le[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))])+1))
if(K4v())then
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetKillingUnit())),10.,"TRIGSTR_15343")
call UnitRemoveAbility(Le[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))],'A0TP')
call UnitAddAbility(Le[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))],'A0HN')
call AdjustPlayerStateBJ($4E20,GetOwningPlayer(GetKillingUnit()),PLAYER_STATE_RESOURCE_GOLD)
call AdjustPlayerStateBJ(2,GetOwningPlayer(GetKillingUnit()),PLAYER_STATE_RESOURCE_LUMBER)
call PlaySoundBJ(aa)
endif
call PlaySoundBJ(aa)
endif
if(K7v())then
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"TRIGSTR_11698")
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(K6v())then
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',Player(-1+(bj_forLoopAIndex)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
call UnitRemoveAbility(me[bj_forLoopAIndex],'A0T9')
call UnitAddAbility(me[bj_forLoopAIndex],'A0TG')
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call PlaySoundBJ(aa)
endif
if(K9v())then
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"TRIGSTR_2031")
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(K8v())then
set ov=GetRectCenter(br)
call CreateNUnitsAtLoA(1,'e001',Player(-1+(bj_forLoopAIndex)),ov,bj_UNIT_FACING)
call UnitRemoveAbility(me[bj_forLoopAIndex],'A0T9')
call UnitAddAbility(me[bj_forLoopAIndex],'A0TG')
call RemoveLocation(ov)
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call PlaySoundBJ(aa)
endif
endfunction
