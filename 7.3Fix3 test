call RemoveLocation(ov)
call DestroyTrigger(DR)
call ExecuteFunc("i3")
call TriggerRegisterUnitEvent(DR,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
endif
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
