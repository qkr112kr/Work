call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,100000000.,"쉬움모드는 51라운드까지입니다!")
return
endif
endif
endif
call StartTimerBJ(Ce,true,.7)
call PauseTimerBJ(true,T)
call StartTimerBJ(T,false,51.)
call TimerDialogSetTitle(S,("|cffFF0000다음레벨->|r"+(I2S((Y+1))+"|r")))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,8.,"|cffff0000라운드가 끝났습니다.|r")
call StartTimerBJ(ce[1],false,28.)
endif
endif
set Go=1
loop
exitwhen Go>5
if(ok())then
if(xk())then
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function vk))))
else
call CreateNUnitsAtLoA(2,'ewsp',Player(-1+(Go)),fo,bj_UNIT_FACING)
endif
endif
set Go=Go+1
endloop
endfunction
