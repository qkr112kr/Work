function RW takes nothing returns nothing
if(XW())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call SetUnitScalePercent(bj_lastCreatedUnit,100.,100.,100.)
