set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(wm())then
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,5.,"|cffff8200 악몽모드 솔로 플레이 모드입니다. 스토리의 체력이 줄어듭니다. 10골드 추가 획득!|r")
call AdjustPlayerStateBJ($A,Player(0),PLAYER_STATE_RESOURCE_GOLD)
call DisableTrigger(oV)
else
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,5.,"|cffff8200 악몽 모드 파티 플레이 모드입니다. 스토리의 체력이 파티플레이어 수만큼 증가합니다.|r")
call DisableTrigger(oV)
endif
endfunction
