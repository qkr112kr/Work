endfunction
function Dsv takes nothing returns boolean
return(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)>=5)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Dqv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function DQv)))>=1)
endfunction
function DSv takes nothing returns nothing
if(Dsv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set Re=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000크로커다일 : |r|cffFA8072이상 이란 실력이 따르는 자만이 떠들어댈 수 있는 '현실'이야!!|r")
call CreateNUnitsAtLoA(1,'h0AA',GetOwningPlayer(GetTriggerUnit()),Re,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function DMv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Dpv))))
call RemoveLocation(Re)
call AdjustPlayerStateBJ(-5,GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)
if(DPv())then
call UnitAddAbility(bj_lastCreatedUnit,'S01J')
endif
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
