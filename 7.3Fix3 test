function b7 takes nothing returns nothing
if(N7())then
call StartSound(fx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))])
endif
set ov=GetUnitLoc(GetAttacker())
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=3
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set Xe=Dd(ov,(345.*I2R(bj_forLoopBIndex)),GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0KF',GetOwningPlayer(GetAttacker()),Xe,(GetUnitFacing(GetAttacker())-180.))
call SetUnitTimeScale(bj_lastCreatedUnit,3.15)
call RemoveLocation(Xe)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
call CreateNUnitsAtLoA(1,'h041',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call RemoveLocation(ov)
set ov=GetUnitLoc(GetTriggerUnit())
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,ov)
call RemoveLocation(ov)
endfunction
function c7 takes nothing returns boolean
return(GetRandomInt(1,20)==5)and(UnitHasBuffBJ(GetAttacker(),'B03T')==false)and(GetUnitPointValue(GetTriggerUnit())!=$C7)
endfunction
function C7 takes nothing returns nothing
call CameraSetEQNoiseForPlayer(GetOwningPlayer(GetAttacker()),5.)
set Rx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetAttacker()
set Ix[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetUnitLoc(GetTriggerUnit())
set Ax[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0KA',GetOwningPlayer(GetAttacker()),Ix[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))],GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D00CB,GetAttacker())
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
set Nx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=.0
call SetUnitAnimation(GetAttacker(),"spell slam")
call StartTimerBJ(Ox[(1+GetPlayerId(GetOwningPlayer(Rx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))])))],true,.13)
endfunction
function D7 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(Rx[bx])))
endfunction
