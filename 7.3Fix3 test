return GetBooleanAnd((GetUnitTypeId(GetFilterUnit())=='h08U'),(GetOwningPlayer(GetFilterUnit())==GetTriggerPlayer()))
endfunction
function gFv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gIv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gAv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gNv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gbv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gBv)))>=1)
endfunction
function ggv takes nothing returns nothing
if(gFv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(gRv())then
call CreateNUnitsAtLoA(1,'h058',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
if(gXv())then
call CreateNUnitsAtLoA(1,'h058',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH6.mdx")
else
call CreateNUnitsAtLoA(1,'h058',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
endif
set re=CreateSound("Sound\\Brook.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000브룩 : |r|cffFA8072전... 이미 죽어있지만 서도...|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gcv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gCv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gdv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gDv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gfv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
function ghv takes nothing returns boolean
return(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="w_teach")
endfunction
