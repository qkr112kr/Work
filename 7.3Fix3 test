function ftv takes nothing returns boolean
return GetBooleanAnd((GetUnitTypeId(GetFilterUnit())=='h05O'),(GetOwningPlayer(GetFilterUnit())==GetTriggerPlayer()))
endfunction
function fTv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function fLv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function fmv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function fMv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function fpv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function fPv)))>=1)
endfunction
function fuv takes nothing returns nothing
if(fTv())then
set re=CreateSound("Sound\\Aokiji.mp3",false,false,false,$A,$A,"")
call StartSound(re)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(flv())then
call CreateNUnitsAtLoA(1,'h05K',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
if(fkv())then
call CreateNUnitsAtLoA(1,'h05K',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"fairywing.MDX")
else
call CreateNUnitsAtLoA(1,'h05K',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000쿠잔 : |r|cffFA8072이봐 미안한데... 비켜주겠어? 내친구거든..|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function fqv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function fQv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function fsv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function fSv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function ftv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
