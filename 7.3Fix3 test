call CameraClearNoiseForPlayer(GetOwningPlayer(GetTriggerUnit()))
else
if(eUv())then
call ResetUnitLookAt(Se[5])
call CameraClearNoiseForPlayer(GetOwningPlayer(GetTriggerUnit()))
endif
endif
endif
endif
endfunction
