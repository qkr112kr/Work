if(wK())then
if(UK())then
set wv[1]=(wv[1]+1)
call SetPlayerStateBJ(Player(0),PLAYER_STATE_RESOURCE_GOLD,0)
call SetPlayerStateBJ(Player(0),PLAYER_STATE_RESOURCE_LUMBER,0)
set bj_wantDestroyGroup=true
call ForGroupBJ(sf(GetWorldBounds(),Player(0)),function TK)
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function uK)
call DisplayTimedTextToForce(bj_FORCE_PLAYER[0],20.,"TRIGSTR_11192")
else
if(tK())then
call DisplayTimedTextToForce(bj_FORCE_PLAYER[0],3.,"|cffffd700(!)|r |cffff0000라인의 유닛이 70마리 이상입니다 !|r")
if(SK())then
call PlaySoundBJ(oa)
endif
endif
endif
call GroupClear(xv)
else
