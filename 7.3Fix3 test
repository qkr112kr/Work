function Mbv takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='e005')
endfunction
function MBv takes nothing returns boolean
return(bv==1)
endfunction
function Mcv takes nothing returns boolean
return(bv==2)
endfunction
function MCv takes nothing returns boolean
return(bv==3)
endfunction
function Mdv takes nothing returns boolean
return(bv==4)
endfunction
function MDv takes nothing returns boolean
return(StringCase(ae[bj_forLoopBIndex],false)=="taeki")
endfunction
function Mfv takes nothing returns nothing
set bv=0
set bv=GetRandomInt(1,4)
if(MBv())then
call CreateNUnitsAtLoA(1,'h08F',GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
endif
if(Mcv())then
call CreateNUnitsAtLoA(1,'h08E',GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
endif
if(MCv())then
call CreateNUnitsAtLoA(1,'h08D',GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
endif
if(Mdv())then
call CreateNUnitsAtLoA(1,'h08C',GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
endif
call RemoveUnit(GetEnteringUnit())
if(MDv())then
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"fairywing.MDX")
endif
endfunction
function Mgv takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='e000')
endfunction
