call IssueImmediateOrderById(bj_lastCreatedUnit,$D0277)
call RemoveLocation(ov)
endif
if(Gw())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0CU',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h09F',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitTimeScale(bj_lastCreatedUnit,1.3)
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(450.,ov,Condition(function Fw)),function gw)
call RemoveLocation(ov)
endif
