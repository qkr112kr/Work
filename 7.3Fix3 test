function YZ takes nothing returns nothing
set ov=GetUnitLoc(GetAttacker())
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=4
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set Xe=Dd(GetUnitLoc(GetAttacker()),(190.*I2R(bj_forLoopBIndex)),GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h03K',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(GetAttacker()))
call RemoveLocation(Xe)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
call CreateNUnitsAtLoA(1,'h08L',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call SetUnitScalePercent(bj_lastCreatedUnit,360.,360.,360.)
call UnitAddAbility(bj_lastCreatedUnit,'A0HR')
call RemoveLocation(ov)
set ov=GetUnitLoc(GetTriggerUnit())
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,ov)
call RemoveLocation(ov)
endfunction
