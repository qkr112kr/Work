function KG takes nothing returns nothing
set Fv=1
loop
exitwhen Fv>5
set gv[(Fv+20)]=GetPlayerName(Player(-1+(Fv)))
set gv[(Fv+40)]=("("+(GetPlayerName(Player(-1+(Fv)))+")"))
set Fv=Fv+1
endloop
set gv[41]=("|c00ff0202"+(gv[41]+"|r"))
set gv[42]=("|cff0041FF"+(gv[42]+"|r"))
set gv[43]=("|cff1BE6B8"+(gv[43]+"|r"))
set gv[44]=("|cff530080"+(gv[44]+"|r"))
set gv[45]=("|cffFFFC00"+(gv[45]+"|r"))
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
if(jG())then
set Hx[bj_forLoopBIndex]=true
endif
if(JG())then
set Hx[bj_forLoopBIndex]=true
endif
if(kG())then
set Hx[bj_forLoopBIndex]=true
endif
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
endfunction
