set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h04G',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
call SetUnitScalePercent(bj_lastCreatedUnit,70.,70.,70.)
call UnitAddAbility(bj_lastCreatedUnit,'A0AK')
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0079,ov)
call CreateNUnitsAtLoA(1,'h04G',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call SetUnitScalePercent(bj_lastCreatedUnit,70.,70.,70.)
call UnitAddAbility(bj_lastCreatedUnit,'A0AM')
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0079,ov)
call CreateNUnitsAtLoA(1,'h04G',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call SetUnitScalePercent(bj_lastCreatedUnit,70.,70.,70.)
call UnitAddAbility(bj_lastCreatedUnit,'A0AL')
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0079,ov)
call RemoveLocation(ov)
endfunction
