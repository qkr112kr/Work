call CreateNUnitsAtLoA(1,'o02S',Player(8),w[1],270.)
call StartTimerBJ(Ne[1],false,60.)
call CreateTimerDialogBJ(bj_lastStartedTimer,"모리아 퇴치-남은 시간 : ")
set Ae[1]=bj_lastCreatedTimerDialog
call TimerDialogDisplayForPlayerBJ(true,Ae[1],Player(0))
call TimerDialogDisplayForPlayerBJ(false,Ae[1],Player(1))
call TimerDialogDisplayForPlayerBJ(false,Ae[1],Player(3))
call TimerDialogDisplayForPlayerBJ(false,Ae[1],Player(4))
endif
if(KEv())then
call CreateNUnitsAtLoA(1,'o02S',Player(8),w[2],270.)
call StartTimerBJ(Ne[2],false,60.)
call CreateTimerDialogBJ(bj_lastStartedTimer,"모리아 퇴치-남은 시간 : ")
set Ae[2]=bj_lastCreatedTimerDialog
call TimerDialogDisplayForPlayerBJ(false,Ae[2],Player(0))
call TimerDialogDisplayForPlayerBJ(true,Ae[2],Player(1))
call TimerDialogDisplayForPlayerBJ(false,Ae[2],Player(3))
call TimerDialogDisplayForPlayerBJ(false,Ae[2],Player(4))
endif
if(KXv())then
call CreateNUnitsAtLoA(1,'o02S',Player(8),w[4],270.)
call StartTimerBJ(Ne[4],false,60.)
call CreateTimerDialogBJ(bj_lastStartedTimer,"모리아 퇴치-남은 시간 : ")
set Ae[4]=bj_lastCreatedTimerDialog
call TimerDialogDisplayForPlayerBJ(false,Ae[4],Player(0))
call TimerDialogDisplayForPlayerBJ(false,Ae[4],Player(1))
call TimerDialogDisplayForPlayerBJ(true,Ae[4],Player(3))
call TimerDialogDisplayForPlayerBJ(false,Ae[4],Player(4))
endif
if(KOv())then
call CreateNUnitsAtLoA(1,'o02S',Player(8),w[5],270.)
call StartTimerBJ(Ne[5],false,60.)
call CreateTimerDialogBJ(bj_lastStartedTimer,"모리아 퇴치-남은 시간 : ")
set Ae[5]=bj_lastCreatedTimerDialog
call TimerDialogDisplayForPlayerBJ(false,Ae[5],Player(0))
call TimerDialogDisplayForPlayerBJ(false,Ae[5],Player(1))
call TimerDialogDisplayForPlayerBJ(false,Ae[5],Player(3))
call TimerDialogDisplayForPlayerBJ(true,Ae[5],Player(4))
endif
endfunction
