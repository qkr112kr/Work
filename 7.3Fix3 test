return(GetUnitPointValue(GetTriggerUnit())==$C8)
endfunction
function ES takes nothing returns boolean
return(GetRandomInt(1,8)==2)
endfunction
function XS takes nothing returns boolean
return(GetRandomInt(1,$A)==5)
endfunction
function OS takes nothing returns nothing
if(ES())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0GV',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call RemoveLocation(ov)
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),600000.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
if(VS())then
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.01*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
endif
if(XS())then
set ov=GetUnitLoc(GetAttacker())
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=4
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set Xe=Dd(ov,(175.*I2R(bj_forLoopBIndex)),GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h051',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call RemoveLocation(Xe)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
call RemoveLocation(ov)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A07E')
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call RemoveLocation(ov)
endif
endfunction
