endfunction
function GQ takes nothing returns boolean
return(GetRandomInt(1,8)==5)
endfunction
function hQ takes nothing returns nothing
if(gQ())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h02Q',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitTimeScalePercent(bj_lastCreatedUnit,150.)
call CreateNUnitsAtLoA(1,'h08L',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A03O')
call RemoveLocation(ov)
set ov=GetUnitLoc(GetTriggerUnit())
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,ov)
call RemoveLocation(ov)
endif
if(GQ())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0KC',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A03P')
call IssueTargetOrderById(bj_lastCreatedUnit,$D0097,GetTriggerUnit())
call RemoveLocation(ov)
endif
endfunction
function jQ takes nothing returns boolean
return(GetRandomInt(1,$A)==5)
endfunction
function JQ takes nothing returns nothing
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A03T')
set Xe=GetUnitLoc(GetTriggerUnit())
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,Xe)
call RemoveLocation(Xe)
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set Re=Dd(ov,(155.*I2R(bj_forLoopBIndex)),GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h02R',GetOwningPlayer(GetAttacker()),Re,bj_UNIT_FACING)
call RemoveLocation(Re)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
call RemoveLocation(ov)
endfunction
