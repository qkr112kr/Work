function hFv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hIv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hAv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hNv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hbv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function hBv)))>=1)
endfunction
function hgv takes nothing returns nothing
if(hFv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(hRv())then
call CreateNUnitsAtLoA(1,'h0GK',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
call CreateNUnitsAtLoA(1,'h0GK',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000도플라밍고 : |r|cffFA8072승자만이 정의다!|r")
set re=CreateSound("Sound\\DP.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hcv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hCv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hdv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hDv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function hfv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
