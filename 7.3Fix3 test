function dg takes nothing returns nothing
call SetPlayerTechResearchedSwap(GetResearched(),(GetPlayerTechCountSimple(GetResearched(),GetOwningPlayer(GetTriggerUnit()))+1),GetOwningPlayer(GetTriggerUnit()))
if(Ng())then
call SetPlayerTechResearchedSwap('R01P',(GetPlayerTechCountSimple('R01P',GetOwningPlayer(GetTriggerUnit()))+1),GetOwningPlayer(GetTriggerUnit()))
endif
if(bg())then
call SetPlayerTechResearchedSwap('R01Q',(GetPlayerTechCountSimple('R01Q',GetOwningPlayer(GetTriggerUnit()))+1),GetOwningPlayer(GetTriggerUnit()))
endif
if(Bg())then
call SetPlayerTechResearchedSwap('R01R',(GetPlayerTechCountSimple('R01R',GetOwningPlayer(GetTriggerUnit()))+1),GetOwningPlayer(GetTriggerUnit()))
endif
if(cg())then
call SetPlayerTechResearchedSwap('R01S',(GetPlayerTechCountSimple('R01S',GetOwningPlayer(GetTriggerUnit()))+1),GetOwningPlayer(GetTriggerUnit()))
endif
if(Cg())then
call SetPlayerTechResearchedSwap('R01T',(GetPlayerTechCountSimple('R01T',GetOwningPlayer(GetTriggerUnit()))+1),GetOwningPlayer(GetTriggerUnit()))
endif
endfunction
