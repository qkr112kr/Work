endfunction
function egv takes nothing returns nothing
call CameraSetEQNoiseForPlayer(GetOwningPlayer(GetAttacker()),4.)
set ov=GetUnitLoc(GetAttacker())
set Ie=Kd(920.,ov,Condition(function eCv))
set Pe=GroupPickRandomUnit(Ie)
set Xe=GetUnitLoc(Pe)
set qe=AngleBetweenPoints(ov,Xe)
call CreateNUnitsAtLoA(1,'h0JD',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())-7.))
call IssueTargetOrderById(bj_lastCreatedUnit,$D007F,GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0IP',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())-7.))
call CreateNUnitsAtLoA(1,'h0JD',GetOwningPlayer(GetAttacker()),ov,(qe+7.))
call IssueTargetOrderById(bj_lastCreatedUnit,$D007F,Pe)
call CreateNUnitsAtLoA(1,'h0IP',GetOwningPlayer(GetAttacker()),ov,(qe+7.))
call RemoveLocation(Xe)
call RemoveLocation(ov)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0JG',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call RemoveLocation(ov)
call SetUnitAnimationByIndex(GetAttacker(),GetRandomInt(5,$F))
if(eFv())then
set Se[1]=GetAttacker()
call SetUnitLookAt(Se[1],"bone_head",Pe,(CosBJ(qe)*100.),(SinBJ(qe)*100.),.0)
else
if(efv())then
set Se[2]=GetAttacker()
call SetUnitLookAt(Se[2],"bone_head",Pe,(CosBJ(qe)*100.),(SinBJ(qe)*100.),.0)
else
if(eDv())then
set Se[4]=GetAttacker()
call SetUnitLookAt(Se[4],"bone_head",Pe,(CosBJ(qe)*100.),(SinBJ(qe)*100.),.0)
else
if(edv())then
set Se[5]=GetAttacker()
call SetUnitLookAt(Se[5],"bone_head",Pe,(CosBJ(qe)*100.),(SinBJ(qe)*100.),.0)
endif
endif
endif
endif
call DestroyGroup(Ie)
endfunction
