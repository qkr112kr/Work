endfunction
function Mlv takes nothing returns nothing
call CreateNUnitsAtLoA(1,GetUnitTypeId(yc),GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
endfunction
