function ZS takes nothing returns nothing
if(YS())then
if(yS())then
set ov=GetUnitLoc(GetTriggerUnit())
call SetUnitPositionLoc(GetAttacker(),ov)
call RemoveLocation(ov)
endif
call lf(GetAttacker(),GetTriggerUnit(),200000.,.9,9.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
if(zS())then
