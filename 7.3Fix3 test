return(R2I(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker()))==$B9)
endfunction
function tw takes nothing returns boolean
return(GetRandomInt(1,33)==16)
endfunction
function Tw takes nothing returns nothing
if(Pw())then
call lf(GetAttacker(),GetTriggerUnit(),250000.,1.,1.5,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
set ov=Dd(GetUnitLoc(GetTriggerUnit()),170.,GetRandomReal(0,360))
call CreateNUnitsAtLocFacingLocBJ(1,'h0CD',GetOwningPlayer(GetAttacker()),ov,ov)
call SetUnitAnimation(bj_lastCreatedUnit,"spell one")
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(400.,ov,Condition(function Mw)),function pw)
call RemoveLocation(ov)
endif
if(sw())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0CE',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(500.,ov,Condition(function qw)),function Qw)
call DestroyGroup(xv)
call RemoveLocation(ov)
endif
if(Sw())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0C9',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetTriggerUnit()))
call UnitApplyTimedLifeBJ(2.5,'BTLF',bj_lastCreatedUnit)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
if(tw())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0CQ',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D0085,GetAttacker())
call RemoveLocation(ov)
endif
