call TriggerAddAction(SKILLDAM,function Getting_Skilldamage)
call DisableTrigger(SKILLDAM2)
call TriggerRegisterAnyUnitEventBJ(SKILLDAM2,EVENT_PLAYER_UNIT_ATTACKED)
call TriggerAddAction(SKILLDAM2,function Skilldam_Check)
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function cd takes real Cd returns nothing
local real dd
local real st=TimerGetElapsed(Xd)
if st<=0 then
set Xd=CreateTimer()
call TimerStart(Xd,$F4240,false,null)
endif
if(Cd>0)then
loop
set dd=Cd-TimerGetElapsed(Xd)+st
exitwhen dd<=0
if(dd>bj_POLLED_WAIT_SKIP_THRESHOLD)then
call TriggerSleepAction(.1*dd)
else
call TriggerSleepAction(bj_POLLED_WAIT_INTERVAL)
endif
endloop
endif
endfunction
function Dd takes location fd,real Fd,real gd returns location
return Location(GetLocationX(fd)+Fd*Cos(gd*bj_DEGTORAD),GetLocationY(fd)+Fd*Sin(gd*bj_DEGTORAD))
endfunction
function Gd takes nothing returns boolean
local real dx=GetDestructableX(GetFilterDestructable())-Rd
local real dy=GetDestructableY(GetFilterDestructable())-Id
return(dx*dx+dy*dy<=bj_enumDestructableRadius)
endfunction
function hd takes rect r,boolexpr Hd returns group
set Ad=CreateGroup()
call GroupEnumUnitsInRect(Ad,r,Hd)
call DestroyBoolExpr(Hd)
return Ad
endfunction
function jd takes rect r returns group
set Ad=CreateGroup()
call GroupEnumUnitsInRect(Ad,r,bd)
return Ad
endfunction
function Jd takes rect r,player kd returns group
set Ad=CreateGroup()
set bj_groupEnumOwningPlayer=kd
call GroupEnumUnitsInRect(Ad,r,filterGetUnitsInRectOfPlayer)
return Ad
endfunction
function Kd takes real ld,location Ld,boolexpr Hd returns group
set Ad=CreateGroup()
call GroupEnumUnitsInRangeOfLoc(Ad,Ld,ld,Hd)
return Ad
endfunction
function md takes player kd,integer Md returns group
set Ad=CreateGroup()
set bj_groupEnumTypeId=Md
call GroupEnumUnitsOfPlayer(Ad,kd,filterGetUnitsOfPlayerAndTypeId)
return Ad
endfunction
function pd takes player kd returns force
set Nd=CreateForce()
call ForceAddPlayer(Nd,kd)
return Nd
endfunction
function Pd takes itemtype qd,integer Qd returns nothing
local group g
set bj_stockPickedItemType=qd
set bj_stockPickedItemLevel=Qd
set g=CreateGroup()
call GroupEnumUnitsOfType(g,"marketplace",bd)
call ForGroup(g,function UpdateEachStockBuildingEnum)
call DestroyGroup(g)
set g=null
endfunction
function sd takes nothing returns nothing
local integer pickedItemId
local itemtype Sd
local integer td=0
local integer Td=0
local integer Qd
set Qd=1
loop
if(bj_stockAllowedPermanent[Qd])then
set Td=Td+1
if(GetRandomInt(1,Td)==1)then
set Sd=ITEM_TYPE_PERMANENT
set td=Qd
endif
endif
if(bj_stockAllowedCharged[Qd])then
set Td=Td+1
if(GetRandomInt(1,Td)==1)then
set Sd=ITEM_TYPE_CHARGED
set td=Qd
endif
endif
if(bj_stockAllowedArtifact[Qd])then
set Td=Td+1
if(GetRandomInt(1,Td)==1)then
set Sd=ITEM_TYPE_ARTIFACT
set td=Qd
endif
endif
set Qd=Qd+1
exitwhen Qd>$A
endloop
if(Td==0)then
set Sd=null
return
endif
call Pd(Sd,td)
set Sd=null
endfunction
function ud takes nothing returns nothing
call sd()
call TimerStart(bj_stockUpdateTimer,bj_STOCK_RESTOCK_INTERVAL,true,function sd)
endfunction
function wd takes nothing returns boolean
return true
endfunction
function xD takes integer i,trigger a1 returns nothing
set ed=a1
call TriggerEvaluate(ZC[i])
endfunction
function oD takes integer i,trigger a1,string a2,boolean a3 returns nothing
set ed=a1
set Ed=a2
set xd=a3
call TriggerEvaluate(vd[i])
endfunction
