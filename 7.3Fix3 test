function e4v takes nothing returns boolean
return GetBooleanAnd((GetUnitTypeId(GetFilterUnit())=='ewsp'),(GetOwningPlayer(GetFilterUnit())==GetOwningPlayer(GetTriggerUnit())))
endfunction
function e5v takes nothing returns boolean
return(GetUnitAbilityLevelSwapped('A0BO',GetTriggerUnit())==3)
endfunction
function e6v takes nothing returns boolean
return(GetUnitAbilityLevelSwapped('A0BO',GetTriggerUnit())==5)
endfunction
function e7v takes nothing returns boolean
return(GetUnitAbilityLevelSwapped('A0BO',GetTriggerUnit())==7)
endfunction
function e8v takes nothing returns boolean
return(GetUnitAbilityLevelSwapped('A0BO',GetTriggerUnit())==$A)
endfunction
function e9v takes nothing returns boolean
return((90-(GetUnitAbilityLevelSwapped('A0BO',GetTriggerUnit())*9))>=GetRandomInt(1,'d'))
endfunction
function Trig_vivi_upgrade_Func001Func005001001002001 takes nothing returns boolean
return(GetUnitTypeId(GetFilterUnit())=='ewsp')
endfunction
function Trig_vivi_upgrade_Func001Func005001001002002 takes nothing returns boolean
return(GetOwningPlayer(GetFilterUnit())==GetTriggerPlayer())
endfunction
function xvv takes nothing returns boolean
return GetBooleanAnd((GetUnitTypeId(GetFilterUnit())=='ewsp'),(GetOwningPlayer(GetFilterUnit())==GetTriggerPlayer()))
endfunction
function xev takes nothing returns boolean
return(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)>=2)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function xvv)))>=3)
endfunction
function xxv takes nothing returns nothing
if(xev())then
if(e9v())then
call SetPlayerTechResearchedSwap('Rhgb',(GetPlayerTechCountSimple('Rhgb',GetOwningPlayer(GetTriggerUnit()))+1),GetOwningPlayer(GetTriggerUnit()))
call IncUnitAbilityLevel(GetTriggerUnit(),'A0BO')
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function e2v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function e3v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function e4v))))
call AdjustPlayerStateBJ(-2,GetOwningPlayer(GetTriggerUnit()),PLAYER_STATE_RESOURCE_LUMBER)
if(e5v())then
call UnitAddAbility(GetTriggerUnit(),'A0MH')
endif
if(e6v())then
call UnitAddAbility(GetTriggerUnit(),'A09C')
endif
if(e7v())then
call IncUnitAbilityLevel(GetTriggerUnit(),'S00B')
endif
if(e8v())then
call UnitRemoveAbility(GetTriggerUnit(),'A03I')
endif
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,"|cffFA8072강화에 성공하였습니다 !|r")
else
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function e_v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function e0v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function e1v))))
call AdjustPlayerStateBJ(-2,GetOwningPlayer(GetTriggerUnit()),PLAYER_STATE_RESOURCE_LUMBER)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,"|cffFF0000강화에 실패하였습니다!|r")
endif
else
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,"|cffFF0000랜덤위습의 개수나 목재가 부족합니다!|r")
endif
endfunction
function xrv takes nothing returns boolean
return(GetSpellAbilityId()=='A0P9')
endfunction
function xiv takes nothing returns nothing
set Se[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=GetTriggerUnit()
set ox[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=GetUnitLoc(Se[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
set Xe=GetSpellTargetLoc()
call PauseUnit(Se[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],true)
call SetUnitAnimation(GetTriggerUnit(),"morph")
call CreateNUnitsAtLoA(1,'h0BI',GetOwningPlayer(GetTriggerUnit()),ox[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(GetTriggerUnit()))
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0275,Xe)
call RemoveLocation(Xe)
endfunction
function xnv takes nothing returns boolean
return(GetSpellAbilityId()=='A0QP')
endfunction
function xVv takes nothing returns nothing
set nx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=.0
set rx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=GetSpellTargetLoc()
set Xx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=AngleBetweenPoints(ox[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],rx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
call StartTimerBJ(cx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],true,.13)
endfunction
function xXv takes nothing returns boolean
return(GetLocalPlayer()==Player(-1+(ax)))
endfunction
function xOv takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(Se[ax])))
endfunction
function xRv takes nothing returns boolean
return(GetUnitPointValue(Zx[ax])<$C8)
endfunction
function xIv takes nothing returns boolean
return(nx[ax]==12.)
endfunction
function xAv takes nothing returns nothing
set ax=1
loop
exitwhen ax>5
exitwhen GetExpiredTimer()==cx[ax]
set ax=ax+1
endloop
call SetUnitAnimation(Se[ax],"spell slam")
if(xXv())then
call StartSound(Cx[R2I(nx[ax])])
endif
set nx[ax]=(nx[ax]+1)
set Xx[ax]=AngleBetweenPoints(ox[ax],rx[ax])
set Ex[ax]=Dd(ox[ax],45.,(Xx[ax]+(49.*Pow(-1.,nx[ax]))))
set zx[ax]=Dd(ox[ax],GetRandomReal(400.,900.),GetUnitFacing(Se[ax]))
set Kx=Kd(460.,zx[ax],Condition(function xOv))
set vo[ax]=GetUnitLoc(Zx[ax])
set Zx[ax]=GroupPickRandomUnit(Kx)
if(xRv())then
call CreateNUnitsAtLoA(1,'h04W',GetOwningPlayer(Se[ax]),vo[ax],GetUnitFacing(Se[ax]))
call lf(bj_lastCreatedUnit,GroupPickRandomUnit(Kx),(150000.+((.04*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GroupPickRandomUnit(Kx)))*I2R(GetUnitAbilityLevelSwapped('A0BO',Se[ax])))),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
else
call CreateNUnitsAtLoA(1,'h04W',GetOwningPlayer(Se[ax]),vo[ax],GetUnitFacing(Se[ax]))
call lf(bj_lastCreatedUnit,GroupPickRandomUnit(Kx),(150000.+(50000.*I2R(GetUnitAbilityLevelSwapped('A0BO',Se[ax])))),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
call DestroyGroup(Kx)
call RemoveLocation(zx[ax])
call RemoveLocation(vo[ax])
if(xIv())then
call CreateNUnitsAtLoA(1,'h0JN',GetOwningPlayer(Se[ax]),Ex[ax],GetUnitFacing(Se[ax]))
call SetUnitAbilityLevelSwapped('ACbf',bj_lastCreatedUnit,GetPlayerTechCountSimple('Rhgb',Player(-1+(ax))))
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0250,Ex[ax])
call RemoveLocation(ox[ax])
call RemoveLocation(Ex[ax])
call RemoveLocation(rx[ax])
call PauseUnit(Se[ax],false)
call PauseTimerBJ(true,cx[ax])
call SetUnitAnimation(Se[ax],"stand")
else
call CreateNUnitsAtLoA(1,'h0JN',GetOwningPlayer(Se[ax]),Ex[ax],GetUnitFacing(Se[ax]))
call SetUnitAbilityLevelSwapped('ACbf',bj_lastCreatedUnit,GetPlayerTechCountSimple('Rhgb',Player(-1+(ax))))
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0250,Ex[ax])
call RemoveLocation(Ex[ax])
call RemoveLocation(Vx[ax])
endif
endfunction
function xbv takes nothing returns boolean
return(GetSpellAbilityId()=='A084')
endfunction
