endfunction
function Gu takes nothing returns boolean
return(GetBooleanAnd((GetOwningPlayer(GetFilterUnit())==GetOwningPlayer(GetAttacker())),(IsUnitType(GetFilterUnit(),UNIT_TYPE_UNDEAD))))!=null
endfunction
function hu takes nothing returns boolean
return(CountUnitsInGroup(xv)>=25)
endfunction
function Hu takes nothing returns nothing
call UnitAddAbility(bj_lastCreatedUnit,'A07T')
call IssueTargetOrderById(bj_lastCreatedUnit,$D0206,GetEnumUnit())
endfunction
function ju takes nothing returns boolean
return(GetUnitPointValue(GetTriggerUnit())<$C8)
endfunction
function Ju takes nothing returns boolean
return(GetRandomInt(1,$A)==5)
endfunction
function ku takes nothing returns boolean
return(GetRandomInt(1,9)==5)
endfunction
function Ku takes nothing returns nothing
if(Ju())then
set ov=GetUnitLoc(GetAttacker())
set xv=hd(bj_mapInitialPlayableArea,Condition(function Gu))
if(hu())then
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A07U')
call SetUnitAbilityLevelSwapped('A07U',bj_lastCreatedUnit,25)
call IssueTargetOrderById(bj_lastCreatedUnit,$D0062,GetAttacker())
else
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A07U')
call SetUnitAbilityLevelSwapped('A07U',bj_lastCreatedUnit,CountUnitsInGroup(xv))
call IssueTargetOrderById(bj_lastCreatedUnit,$D0062,GetAttacker())
endif
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function Hu)
call DestroyGroup(xv)
call RemoveLocation(ov)
if(ju())then
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.09*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
endif
if(ku())then
set ov=GetUnitLoc(GetAttacker())
set Xe=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0IX',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,Xe)
call RemoveLocation(Xe)
call RemoveLocation(ov)
endif
endfunction
