function P8 takes nothing returns nothing
if(M8())then
call RemoveUnit(GetAttacker())
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLocFacingLocBJ(1,'h0H5',GetOwningPlayer(GetAttacker()),ov,ov)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call CreateNUnitsAtLocFacingLocBJ(1,'h0FW',GetOwningPlayer(GetAttacker()),ov,ov)
call SetUnitAnimation(bj_lastCreatedUnit,"spell one")
call SetUnitTimeScale(bj_lastCreatedUnit,.9)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
set ov=GetUnitLoc(GetTriggerUnit())
set Xe=Dd(ov,400.,GetRandomReal(0,360))
call SetUnitPositionLoc(GetAttacker(),Xe)
call RemoveLocation(ov)
call RemoveLocation(Xe)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLocFacingLocBJ(1,'h0FK',GetOwningPlayer(GetAttacker()),ov,ov)
call CreateNUnitsAtLocFacingLocBJ(1,'h0FM',GetOwningPlayer(GetAttacker()),ov,ov)
call CreateNUnitsAtLocFacingLocBJ(1,'h0FL',GetOwningPlayer(GetAttacker()),ov,ov)
call RemoveLocation(ov)
set ov=GetUnitLoc(GetAttacker())
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(650.,ov,Condition(function L8)),function m8)
call RemoveLocation(ov)
call IssueTargetOrderById(GetAttacker(),$D000F,GetTriggerUnit())
endif
if(p8())then
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.35*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
else
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.01*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())),ATTACK_TYPE_NORMAL,DAMAGE_TYPE_UNIVERSAL)
endif
