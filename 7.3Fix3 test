call RemoveLocation(ov)
if(ey())then
set ov=GetRectCenter(ei)
call CreateNUnitsAtLoA(1,'h0A2',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call RemoveLocation(ov)
endif
if(xy())then
call AdjustPlayerStateBJ(1,GetOwningPlayer(GetTriggerUnit()),PLAYER_STATE_RESOURCE_LUMBER)
endif
endfunction
