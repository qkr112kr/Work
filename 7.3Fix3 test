call RemoveLocation(Xe)
set Xe=Dd(ov,300.,300.)
call CreateNUnitsAtLoA(1,'h09N',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call RemoveLocation(Xe)
set Xe=Dd(ov,300.,240.)
