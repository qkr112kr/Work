return(StringCase(GetPlayerName(Player(0)),false)=="실")
endfunction
function lh takes nothing returns nothing
set ae[1]=GetPlayerName(Player(0))
set ae[2]=GetPlayerName(Player(1))
set ae[4]=GetPlayerName(Player(3))
set ae[5]=GetPlayerName(Player(4))
if(Kh())then
set ae[1]="Sil"
call SetPlayerName(Player(0),ae[1])
else
if(kh())then
set ae[2]="Sil"
call SetPlayerName(Player(1),ae[2])
else
if(Jh())then
set ae[4]="Sil"
call SetPlayerName(Player(3),ae[4])
else
if(jh())then
set ae[5]="Sil"
call SetPlayerName(Player(4),ae[5])
endif
endif
endif
endif
endfunction
function mh takes nothing returns nothing
