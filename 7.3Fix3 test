return(GetUnitTypeId(bj_lastCreatedUnit)=='h092')
endfunction
function MJv takes nothing returns nothing
call CreateNUnitsAtLoA(1,GetUnitTypeId(GroupPickRandomUnit(Nv)),GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
