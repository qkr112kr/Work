call CreateNUnitsAtLoA(1,'h0JV',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call SetUnitScalePercent(bj_lastCreatedUnit,500.,500.,500.)
call CreateNUnitsAtLoA(1,'h0JV',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+GetRandomReal(25.,200.)))
call IssueTargetOrderById(bj_lastCreatedUnit,$D007F,GetTriggerUnit())
call SetUnitScalePercent(bj_lastCreatedUnit,400.,400.,400.)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(550.,ov,Condition(function Y1)),function Z1)
call RemoveLocation(ov)
else
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0EZ',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h0JU',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call SetUnitTimeScale(bj_lastCreatedUnit,2.25)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(550.,ov,Condition(function U1)),function W1)
call RemoveLocation(ov)
