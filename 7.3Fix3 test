function Jdv takes nothing returns nothing
if(JCv())then
if(Jcv())then
set re=CreateSound("Sound\\bugi.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000천냥광대 버기 : |r|cffFA8072사나이라면 나와 함께 꾸지 않겠나!|r")
call CreateNUnitsAtLoA(1,'h0E7',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function JIv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function JAv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function JNv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Jbv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function JBv))))
call AdjustPlayerStateBJ(-5,GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)
else
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),10.,("|cffFF8200"+("클리어회수가 부족합니다.|r")))
endif
endif
endfunction
