function uH takes nothing returns nothing
if(DH())then
set ov=GetRectCenter(br)
call CreateNUnitsAtLoA(1,'e001',Player(0),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(FH())then
set ov=GetRectCenter(br)
call CreateNUnitsAtLoA(1,'e001',Player(1),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(GH())then
set ov=GetRectCenter(br)
call CreateNUnitsAtLoA(1,'e001',Player(3),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(HH())then
set ov=GetRectCenter(br)
call CreateNUnitsAtLoA(1,'e001',Player(4),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(JH())then
set ov=GetRectCenter(Fr)
call CreateNUnitsAtLoA(1,'e002',Player(0),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(KH())then
set ov=GetRectCenter(Fr)
call CreateNUnitsAtLoA(1,'e002',Player(1),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(LH())then
set ov=GetRectCenter(Fr)
call CreateNUnitsAtLoA(1,'e002',Player(3),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(MH())then
set ov=GetRectCenter(Fr)
call CreateNUnitsAtLoA(1,'e002',Player(4),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(PH())then
set ov=GetRectCenter(Lo)
call CreateNUnitsAtLoA(1,'h0A2',Player(0),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(QH())then
set ov=GetRectCenter(mo)
call CreateNUnitsAtLoA(1,'h0A2',Player(1),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(SH())then
