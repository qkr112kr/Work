function jK takes multiboard JK,integer x1,integer y1,real w1,boolean kK,boolean KK,string z1,string d1 returns nothing
local multiboarditem lK=MultiboardGetItem(JK,y1,x1)
call MultiboardSetItemWidth(lK,w1)
call MultiboardSetItemStyle(lK,KK,kK)
call MultiboardSetItemIcon(lK,z1)
call MultiboardSetItemValue(lK,d1)
call MultiboardReleaseItem(lK)
set lK=null
endfunction
function LK takes nothing returns nothing
local string gK="|c00ffb0ff유닛 카운트 = |cFF00FF00 80 |c00ffb0ff<- 패배 "
call MultiboardSetTitleText(uC,gK)
call MultiboardSetColumnCount(uC,2)
call MultiboardSetRowCount(uC,5)
call jK(uC,0,0,.1,false,true,"ReplaceableTextures\\CommandButtons\\BTNSkillz.tga",("|cff00ffff난이도 :|r"+(Q+"|cff00ffff모드|r")))
call jK(uC,1,0,.1,true,true,"ReplaceableTextures\\CommandButtons\\BTNSkillz.tga","|c0000ff00남은 라운드 유닛 수")
set UC[0]=MultiboardGetItem(uC,1,0)
call MultiboardSetItemWidth(UC[0],.1)
call MultiboardSetItemValue(UC[0],"|c00ff0202"+GetPlayerName(Player(0)))
call MultiboardSetItemIcon(UC[0],"ReplaceableTextures\\CommandButtons\\BTNPatrol.tga")
set wC[0]=MultiboardGetItem(uC,1,1)
call MultiboardSetItemWidth(wC[0],.1)
call MultiboardSetItemStyle(wC[0],true,false)
call MultiboardSetItemValue(wC[0],"0")
set UC[1]=MultiboardGetItem(uC,2,0)
call MultiboardSetItemWidth(UC[1],.1)
call MultiboardSetItemValue(UC[1],"|cff0041FF"+GetPlayerName(Player(1)))
call MultiboardSetItemIcon(UC[1],"ReplaceableTextures\\CommandButtons\\BTNPatrol.tga")
set wC[1]=MultiboardGetItem(uC,2,1)
call MultiboardSetItemWidth(wC[1],.1)
call MultiboardSetItemStyle(wC[1],true,false)
call MultiboardSetItemValue(wC[1],"0")
set UC[3]=MultiboardGetItem(uC,3,0)
call MultiboardSetItemWidth(UC[3],.1)
call MultiboardSetItemValue(UC[3],"|cff530080"+GetPlayerName(Player(3)))
call MultiboardSetItemIcon(UC[3],"ReplaceableTextures\\CommandButtons\\BTNPatrol.tga")
set wC[3]=MultiboardGetItem(uC,3,1)
call MultiboardSetItemWidth(wC[3],.1)
call MultiboardSetItemStyle(wC[3],true,false)
call MultiboardSetItemValue(wC[3],"0")
set UC[4]=MultiboardGetItem(uC,4,0)
call MultiboardSetItemWidth(UC[4],.1)
call MultiboardSetItemValue(UC[4],"|cffFFFC00"+GetPlayerName(Player(4)))
call MultiboardSetItemIcon(UC[4],"ReplaceableTextures\\CommandButtons\\BTNPatrol.tga")
set wC[4]=MultiboardGetItem(uC,4,1)
call MultiboardSetItemWidth(wC[4],.1)
call MultiboardSetItemStyle(wC[4],true,false)
call MultiboardSetItemValue(wC[4],"0")
endfunction
function mK takes nothing returns nothing
set uC=CreateMultiboard()
call LK()
call MultiboardDisplay(uC,true)
call MultiboardMinimize(uC,false)
endfunction
