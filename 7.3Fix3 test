function gnv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function F7v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function F8v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function F9v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gvv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gev)))>=1)
endfunction
function gVv takes nothing returns nothing
if(gnv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(F6v())then
call CreateNUnitsAtLoA(1,'h03J',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
if(F4v())then
call CreateNUnitsAtLoA(1,'h03J',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH6.mdx")
else
call CreateNUnitsAtLoA(1,'h03J',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000나미 : |r|cffFA8072하여튼, 이 녀석이고 저 녀석이고 성장하지 않았다니까.|r")
set re=CreateSound("Sound\\Nami.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gxv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gov))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function grv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function giv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gav))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
