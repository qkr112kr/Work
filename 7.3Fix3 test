endfunction
function Trig_Shyrahosi_Eternal_Func001Func013001001002002 takes nothing returns boolean
return(GetOwningPlayer(GetFilterUnit())==GetTriggerPlayer())
endfunction
function FVv takes nothing returns boolean
return GetBooleanAnd((GetUnitTypeId(GetFilterUnit())=='h05O'),(GetOwningPlayer(GetFilterUnit())==GetTriggerPlayer()))
endfunction
function FEv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fev)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fxv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fov)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Frv)))>=1)
endfunction
function FXv takes nothing returns nothing
if(FEv())then
set re=CreateSound("Sound\\Shira.mp3",false,false,false,$A,$A,"")
call StartSound(re)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(Fvv())then
call CreateNUnitsAtLoA(1,'h03B',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
if(f8v())then
call CreateNUnitsAtLoA(1,'h03B',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH6.mdx")
else
call CreateNUnitsAtLoA(1,'h03B',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000시라호시 : |r|cffFA8072죄송해요...! 사실은 다 알고 있었어요|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fiv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fav))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fnv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function FVv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
function FRv takes nothing returns boolean
return(GetPlayerName(GetTriggerPlayer())=="마야")or(GetPlayerName(GetTriggerPlayer())=="플머")or(GetPlayerName(GetTriggerPlayer())=="브로콜리")or(GetPlayerName(GetTriggerPlayer())=="마왕병")or(GetPlayerName(GetTriggerPlayer())=="루르")or(GetPlayerName(GetTriggerPlayer())=="렘")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="dontworry")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="kalyptus")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="nct")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="taeyeon")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="broly")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="sil")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="tory_drd")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="lody")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="free.ic")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="moo...")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="d88")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="hanayo")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="ap4o")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="esp.adol")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="nezsi")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="santa")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="camellia")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="300")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="rev.")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="kruse")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="teastory")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="hardgamelifer")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="alzip")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="opcr")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="neptune")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="kimeve")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="anby")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="taeki")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="achernar")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="praha")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="theriver")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="leal")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="no30")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="bj.despair")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="[md]_senugma")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="moria")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="bj.mild")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="no25")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="ryunian")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="iloveluna")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="hisins")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="bed")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="no31")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="oftenprized")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="hucuro.ac")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="coramdeo")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="linen")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="galmegi")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="w_amdor")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="shirai")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="w_orosung")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="not_byeon")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="minijung")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="j.savior")or(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="w_teach")
endfunction
