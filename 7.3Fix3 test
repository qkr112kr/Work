function rF takes nothing returns nothing
set Gx=md(GetTriggerPlayer(),'h000')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("루피 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
set Gx=md(GetTriggerPlayer(),'h003')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("조로 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
set Gx=md(GetTriggerPlayer(),'h001')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("나미 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
set Gx=md(GetTriggerPlayer(),'h006')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("우솝 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
set Gx=md(GetTriggerPlayer(),'h004')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("상디 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
set Gx=md(GetTriggerPlayer(),'h005')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("쵸파 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
set Gx=md(GetTriggerPlayer(),'h008')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("해군 칼병 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
set Gx=md(GetTriggerPlayer(),'h007')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("해군 총병 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
set Gx=md(GetTriggerPlayer(),'h002')
set Wv=CountUnitsInGroup(Gx)
call DisplayTimedTextToForce(pd(GetTriggerPlayer()),3.,("버기 : "+(I2S(Wv)+"개")))
call DestroyGroup(Gx)
endfunction
function aF takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function nF takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function VF takes nothing returns boolean
return(GetTriggerPlayer()==Player(0))
endfunction
function EF takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function XF takes nothing returns boolean
return(GetTriggerPlayer()==Player(1))
endfunction
function OF takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function RF takes nothing returns boolean
return(GetTriggerPlayer()==Player(3))
endfunction
function IF takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function AF takes nothing returns boolean
return(GetTriggerPlayer()==Player(4))
endfunction
function NF takes nothing returns nothing
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,5.,(("|cffFF8200"+GetPlayerName(GetTriggerPlayer()))+"|r |cffff0000님이 게임에서 나가셨습니다. 스토리 체력이 인원수에 맞게 감소합니다. |r"))
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(ii,GetTriggerPlayer()),function aF)
if(VF())then
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(Lo,Player(6)),function nF)
set wv[1]=(wv[1]+1)
endif
if(XF())then
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(mo,Player(6)),function EF)
set wv[2]=(wv[2]+1)
endif
if(RF())then
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(Mo,Player(6)),function OF)
set wv[4]=(wv[4]+1)
endif
if(AF())then
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(po,Player(6)),function IF)
set wv[5]=(wv[5]+1)
endif
call SetPlayerTechResearchedSwap('R00T',(GetPlayerTechCountSimple('R00T',Player(8))+1),Player(8))
endfunction
function BF takes nothing returns boolean
return(GetOwningPlayer(GetEnteringUnit())!=Player(7))
endfunction
function cF takes nothing returns nothing
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,"|Cffff8200조합식들어가기 없기|r")
call SetUnitPositionLoc(GetTriggerUnit(),Fe)
endfunction
function dF takes nothing returns boolean
return(GetUnitTypeId(GetTriggerUnit())!='hgyr')
endfunction
function DF takes nothing returns nothing
set Xe=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'hgyr',GetOwningPlayer(GetTriggerUnit()),Xe,bj_UNIT_FACING)
call RemoveLocation(Xe)
call SetUnitPositionLoc(GetTriggerUnit(),Ho[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
endfunction
function FF takes nothing returns boolean
return(S2I(SubStringBJ(GetEventPlayerChatString(),9,$B))>=50)and(S2I(SubStringBJ(GetEventPlayerChatString(),9,$B))<=$96)
endfunction
function gF takes nothing returns nothing
call SetCameraFieldForPlayer(GetTriggerPlayer(),CAMERA_FIELD_TARGET_DISTANCE,(20.*S2R(SubStringBJ(GetEventPlayerChatString(),9,$B))),1.)
endfunction
function hF takes nothing returns boolean
return(S2I(SubStringBJ(GetEventPlayerChatString(),8,$A))>=50)and(S2I(SubStringBJ(GetEventPlayerChatString(),8,$A))<=$96)
endfunction
function HF takes nothing returns nothing
call SetCameraFieldForPlayer(GetTriggerPlayer(),CAMERA_FIELD_TARGET_DISTANCE,(20.*S2R(SubStringBJ(GetEventPlayerChatString(),8,$A))),1.)
endfunction
function JF takes nothing returns boolean
return(Y>51)
endfunction
function kF takes nothing returns nothing
set Iv=GetRandomInt(1,$C)
call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,Vo[Iv])
if(JF())then
call PauseTimerBJ(true,de)
call DisableTrigger(GetTriggeringTrigger())
call DestroyTrigger(GetTriggeringTrigger())
endif
endfunction
function lF takes nothing returns boolean
return(GetPlayerSlotState(Player(-1+(bj_forLoopAIndex)))==PLAYER_SLOT_STATE_PLAYING)
endfunction
