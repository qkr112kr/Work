function EQv takes nothing returns nothing
if(VJv())then
if(Vjv())then
set Re=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000해적 : |r|cffFA8072\"막았다! 세계제일의 참격을! 3번대 대장 다이아몬드 죠즈! |r")
call CreateNUnitsAtLoA(1,'h08T',GetOwningPlayer(GetTriggerUnit()),Re,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Vgv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function VGv))))
call RemoveLocation(Re)
endif
endif
if(VMv())then
if(Vmv())then
set Re=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000도플라밍고 : |r|cffFA8072네놈들과의 거래가 재미없어지면 난 언제든지 칠무해를 그만두겠어 ㅋㅋ|r")
call CreateNUnitsAtLoA(1,'h01P',GetOwningPlayer(GetTriggerUnit()),Re,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Vkv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function VKv))))
call RemoveLocation(Re)
endif
endif
