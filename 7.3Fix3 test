set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000블루노 : |r|cffFA8072나는 문문열매 능력자.|r")
call CreateNUnitsAtLoA(1,'h00G',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function rKv))))
call RemoveLocation(ov)
endif
endfunction
