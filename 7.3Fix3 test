function xCv takes nothing returns nothing
if(xcv())then
call RemoveUnit(GetSpellTargetUnit())
else
if(xBv())then
call RemoveUnit(GetSpellTargetUnit())
else
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
call UnitRemoveAbility(GetTriggerUnit(),'A084')
call UnitAddAbility(GetTriggerUnit(),'A084')
call SetUnitManaBJ(GetTriggerUnit(),(GetUnitStateSwap(UNIT_STATE_MANA,GetTriggerUnit())+100.))
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),5.,"|cffff00ff보스,스토리, 특수유닛에게는 사용불가합니다!|r")
endif
endif
endfunction
