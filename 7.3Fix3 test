endfunction
function Vt takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),(1600000.+((GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())-GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit()))*.01)),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endfunction
function Et takes nothing returns nothing
if(at())then
else
return
endif
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetEventDamageSource()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0BF')
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call CreateNUnitsAtLoA(1,'h051',GetOwningPlayer(GetEventDamageSource()),ov,bj_UNIT_FACING)
call SetUnitScalePercent(bj_lastCreatedUnit,250.,250.,250.)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(800.,ov,Condition(function nt)),function Vt)
call RemoveLocation(ov)
endfunction
