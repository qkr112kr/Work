function bt takes nothing returns nothing
if(At())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0HC')
set ov=GetUnitLoc(GetTriggerUnit())
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,ov)
call RemoveLocation(Xe)
set Xe=Dd(ov,-200.,(GetUnitFacing(GetAttacker())+-360.))
call CreateNUnitsAtLocFacingLocBJ(1,'h09G',GetOwningPlayer(GetAttacker()),Xe,GetUnitLoc(GetTriggerUnit()))
call CreateNUnitsAtLoA(1,'h02Q',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(bj_lastCreatedUnit))
call SetUnitVertexColorBJ(bj_lastCreatedUnit,'d',.0,.0,0)
call SetUnitTimeScalePercent(bj_lastCreatedUnit,150.)
call RemoveLocation(Xe)
set Xe=Dd(ov,-200.,(GetUnitFacing(GetAttacker())+-70.))
call CreateNUnitsAtLocFacingLocBJ(1,'h09G',GetOwningPlayer(GetAttacker()),Xe,GetUnitLoc(GetTriggerUnit()))
call CreateNUnitsAtLoA(1,'h02Q',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(bj_lastCreatedUnit))
call SetUnitVertexColorBJ(bj_lastCreatedUnit,'d',.0,.0,0)
call SetUnitTimeScalePercent(bj_lastCreatedUnit,150.)
call RemoveLocation(Xe)
set Xe=Dd(ov,-200.,(GetUnitFacing(GetAttacker())+-290.))
call CreateNUnitsAtLocFacingLocBJ(1,'h09G',GetOwningPlayer(GetAttacker()),Xe,GetUnitLoc(GetTriggerUnit()))
call CreateNUnitsAtLoA(1,'h02Q',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(bj_lastCreatedUnit))
call SetUnitVertexColorBJ(bj_lastCreatedUnit,'d',.0,.0,0)
call SetUnitTimeScalePercent(bj_lastCreatedUnit,150.)
call RemoveLocation(Xe)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
if(Nt())then
call lf(GetAttacker(),GetTriggerUnit(),150000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
endfunction
