if(eHv())then
call UnitDamageTargetBJ(bj_lastCreatedUnit,GetSpellTargetUnit(),(((I2R(GetPlayerTechCountSimple('Rhgb',GetOwningPlayer(GetTriggerUnit())))*25000.)+(.15*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetSpellTargetUnit())))+(GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetSpellTargetUnit())*(I2R(GetPlayerTechCountSimple('Rhgb',GetOwningPlayer(GetTriggerUnit())))*.01))),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
else
