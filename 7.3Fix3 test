function uW takes nothing returns nothing
if(sW())then
if(QW())then
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.35*GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
endif
