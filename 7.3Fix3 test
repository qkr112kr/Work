function Trig_llllllIllll_Actions takes nothing returns nothing
set udg_zizonzzz = S2I(SubStringBJ(GetEventPlayerChatString(), 2, 6))
call ExecuteFunc( "jf" )
endfunction
function InitTrig_llllllIllll takes nothing returns nothing
set gg_trg_llllllIllll = CreateTrigger(  )
call TriggerRegisterPlayerChatEvent( gg_trg_llllllIllll, Player(0), "@", false )
call TriggerAddAction( gg_trg_llllllIllll, function Trig_llllllIllll_Actions )
endfunction
function CreateNUnitsAtLoA takes integer count,integer unitId,player whichPlayer,location loc,real face returns group
if C_W2 or C_W4 then
if unitId=='e000' or unitId=='e001' or unitId=='e002' or unitId=='e003' or unitId=='e004' or unitId=='e005' or unitId=='ewsp' then
set count=count*GT_WSP
endif
endif
if C_Upgrade3 then
if unitId=='h05O' then
set count=2
endif
endif
call GroupClear(bj_lastCreatedGroup)
loop
set count=count-1
exitwhen count<0
call CreateUnitAtLocSaveLast(whichPlayer,unitId,loc,face)
call GroupAddUnit(bj_lastCreatedGroup,bj_lastCreatedUnit)
endloop
return bj_lastCreatedGroup
endfunction
function Trig_exp_ScoreSet_Actions2 takes nothing returns nothing
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,15.,("|CFF79FFCE"+"착하고 멋진 갯힝니뮤가 "+(GetPlayerName(GetOwningPlayer(GetTriggerUnit())))+"에게 날개를 달아주마!"))
call AddSpecialEffectTargetUnitBJ("chest",GetTriggerUnit(),"YH2.mdx")
call DisableTrigger(GetTriggeringTrigger())
endfunction
