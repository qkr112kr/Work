function Func0040 takes nothing returns nothing
set integer004=1
loop
exitwhen integer004>10
set integers003[integer004]=GetRandomInt(0,9)
set integer004=integer004+1
endloop
set integer001=(integer001+1)
call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,("◎ "+(I2S(integer001)+"|r |cff646eff회 로또 추첨번호")))
call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,((I2S(integers003[1])+(" "+(I2S(integers003[2])+" ")))+((I2S(integers003[3])+" ")+(I2S(integers003[4])+(" "+I2S(integers003[5]))))))
call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,((I2S(integers003[6])+(" "+(I2S(integers003[7])+" ")))+((I2S(integers003[8])+" ")+(I2S(integers003[9])+(" "+I2S(integers003[10]))))))
set integer003=(1+GetPlayerId(Player(0)))
loop
exitwhen integer003>(1+GetPlayerId(Player(11)))
set players001[integer003]=Player(-1+(integer003))
set forces001[integer003]=Func0010(players001[integer003])
set integers002[integer003]=0
if(((booleans002[integer003])))then
set integers001[integer003]=GetRandomInt(0,9)
endif
if(((((GetPlayerController(players001[integer003])==MAP_CONTROL_USER)and(GetPlayerSlotState(players001[integer003])==PLAYER_SLOT_STATE_PLAYING)))))then
call DisplayTextToForce(forces001[integer003],(GetPlayerName(players001[integer003])+"|r |cff646eff님이 찍은 번호"))
call DisplayTextToForce(forces001[(1+GetPlayerId(players001[integer003]))],I2S(integers001[(1+GetPlayerId(players001[integer003]))]))
set integer002=1
loop
exitwhen integer002>10
if(((integers001[integer003]==integers003[integer002])))then
set ov=GetRectCenter(br)
call CreateNUnitsAtLoA(1*GT_WSP,'e001',players001[integer003],ov,bj_UNIT_FACING)
call RemoveLocation(ov)
set integers002[integer003]=(integers002[integer003]+1)
endif
set integer002=integer002+1
endloop
if(((integers002[integer003]==3)))then
call AdjustPlayerStateBJ(1,players001[integer003],PLAYER_STATE_RESOURCE_LUMBER)
set ov=GetRectCenter(gr)
call CreateNUnitsAtLoA(1*GT_WSP,'e003',players001[integer003],ov,bj_UNIT_FACING)
call RemoveLocation(ov)
call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,(GetPlayerName(players001[integer003])+"|r - |cffffA500님이 3개를 맞추어 나무 1개와 특별함 1개를 받습니다!"))
endif
if(((integers002[integer003]==4)))then
call AdjustPlayerStateBJ(3,players001[integer003],PLAYER_STATE_RESOURCE_LUMBER)
set ov=GetRectCenter(Gr)
call CreateNUnitsAtLoA(1*GT_WSP,'e004',players001[integer003],ov,bj_UNIT_FACING)
call RemoveLocation(ov)
call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,(GetPlayerName(players001[integer003])+"|r - |cffffA500님이 4개를 맞추어 나무 3개와 희귀함랜덤 1기를 받습니다!"))
endif
if(((integers002[integer003]==5)))then
call AdjustPlayerStateBJ(10000,players001[integer003],PLAYER_STATE_RESOURCE_GOLD)
call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,(GetPlayerName(players001[integer003])+"|r - |cffffA500님이 5개를 맞추어 10000골드와 경품당첨의 보너스를 받습니다!"))
if(((integers004[integer003]==1)))then
call AdjustPlayerStateBJ(20000,players001[integer003],PLAYER_STATE_RESOURCE_GOLD)
call AdjustPlayerStateBJ(15,players001[integer003],PLAYER_STATE_RESOURCE_LUMBER)
else
if(((integers004[integer003]==2)))then
set ov=GetRectCenter(Jr)
call CreateNUnitsAtLoA(1*GT_WSP,'e005',players001[integer003],ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
endif
endif
if(((integers002[integer003]>=6)))then
call AdjustPlayerStateBJ(50000,players001[integer003],PLAYER_STATE_RESOURCE_GOLD)
set ov=GetRectCenter(Wr)
call CreateNUnitsAtLoA(1*GT_WSP,'e000',players001[integer003],ov,bj_UNIT_FACING)
call RemoveLocation(ov)
call DisplayTextToForce(bj_FORCE_ALL_PLAYERS,(GetPlayerName(players001[integer003])+"|r - |cffffA500님이 미친듯이 맞춰서 5만골드와 전설유닛 또는 히든유닛의 혜택을 받습니다!"))
endif
endif
call DisplayTextToForce(forces001[integer003],(("|cffFF2118당첨된 갯수 : "+"|CFFF7DEF7")+(I2S(integers002[integer003])+"개")))
set integer003=integer003+1
endloop
endfunction
