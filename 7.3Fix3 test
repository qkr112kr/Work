endfunction
function E3 takes nothing returns boolean
return(IsUnitAliveBJ(GetTriggerUnit()))
endfunction
function Trig_SN_Func013001003001001 takes nothing returns boolean
return(IsUnitAliveBJ(GetFilterUnit()))
endfunction
function Trig_SN_Func013001003001002 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(GetEventDamageSource())))
endfunction
function Trig_SN_Func013001003001 takes nothing returns boolean
return GetBooleanAnd((IsUnitAliveBJ(GetFilterUnit())),(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(GetEventDamageSource()))))
endfunction
function Trig_SN_Func013001003002 takes nothing returns boolean
return(IsUnitInRangeLoc(GetFilterUnit(),ux,700.))
endfunction
function X3 takes nothing returns boolean
return GetBooleanAnd((GetBooleanAnd((IsUnitAliveBJ(GetFilterUnit())),(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(GetEventDamageSource()))))),(IsUnitInRangeLoc(GetFilterUnit(),ux,700.)))
endfunction
function O3 takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),(3000000.+((GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())-GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit()))*.04)),1.,2.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endfunction
function R3 takes nothing returns nothing
if(E3())then
else
return
endif
call DestroyTrigger(GetTriggeringTrigger())
set ux=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetEventDamageSource()),ux,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0BF')
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call CreateNUnitsAtLoA(1,'h051',GetOwningPlayer(GetEventDamageSource()),ux,bj_UNIT_FACING)
call SetUnitScalePercent(bj_lastCreatedUnit,250.,250.,250.)
call CreateNUnitsAtLoA(1,'h0CM',GetOwningPlayer(GetEventDamageSource()),ux,bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h0CO',GetOwningPlayer(GetEventDamageSource()),ux,bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h0CN',GetOwningPlayer(GetEventDamageSource()),ux,bj_UNIT_FACING)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(700.,ux,Condition(function X3)),function O3)
call RemoveLocation(ux)
endfunction
