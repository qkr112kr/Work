function gzv takes nothing returns boolean
return GetBooleanAnd((GetUnitTypeId(GetFilterUnit())=='h05O'),(GetOwningPlayer(GetFilterUnit())==GetTriggerPlayer()))
endfunction
function gZv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gUv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gwv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function gWv)))>=1)
endfunction
function g_v takes nothing returns nothing
if(gZv())then
set re=CreateSound("Sound\\Choppa.mp3",false,false,false,$A,$A,"")
call StartSound(re)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(guv())then
call CreateNUnitsAtLoA(1,'h06M',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
if(gtv())then
call CreateNUnitsAtLoA(1,'h06M',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"fairywing.MDX")
else
call CreateNUnitsAtLoA(1,'h06M',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000쵸파 : |r|cffFA8072모두 건강할까나~ 빨리 만나고 싶다 으헤헤|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gyv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gYv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function gzv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
function g1v takes nothing returns boolean
return(StringCase(GetPlayerName(GetTriggerPlayer()),false)=="w_teach")
endfunction
