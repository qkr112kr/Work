call SelectUnitRemove(GetTriggerUnit())
call RemoveLocation(ov)
endif
if(iM())then
set ov=GetRectCenter(Ur)
call SetUnitPositionLoc(GetTriggerUnit(),ov)
call SelectUnitRemove(GetTriggerUnit())
call RemoveLocation(ov)
endif
if(aM())then
set ov=GetRectCenter(Sr)
