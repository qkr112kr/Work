set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000슈가 : |r|cffFA8072기분 나빠.|r")
call CreateNUnitsAtLoA(1,'h05R',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function R4v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function R5v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function R6v))))
call RemoveLocation(ov)
endif
endfunction
