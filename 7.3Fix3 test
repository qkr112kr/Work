endfunction
function lEv takes nothing returns boolean
return(GetSpellAbilityId()=='A0GV')
endfunction
function lXv takes nothing returns nothing
if(lrv())then
if(lov())then
set ov=GetUnitLoc(GetTriggerUnit())
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-25))
call CreateNUnitsAtLoA(1,'h06P',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
else
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF8200"+("목재가 부족합니다!|r")))
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
endif
endif
if(lnv())then
if(lav())then
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-8))
if(liv())then
call CreateNUnitsAtLoA(1,GetUnitTypeId(GroupPickRandomUnit(vv)),GetOwningPlayer(GetTriggerUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,((("|cffff8200"+GetPlayerName(GetOwningPlayer(GetTriggerUnit())))+("|r |cffff8200님이 다른세계 도박으로|r |cffFF0000"))+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
else
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,((("|cffff8200"+GetPlayerName(GetOwningPlayer(GetTriggerUnit())))+("|r |cffff8200님이 다른세계 유닛 도박을|r |cffFF0000"))+"실패 하셨습니다!|r"))
endif
call RemoveUnit(GetTriggerUnit())
else
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF8200"+("목재가 부족합니다!|r")))
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
endif
endif
if(lEv())then
if(lVv())then
set ov=GetUnitLoc(GetTriggerUnit())
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-25))
call CreateNUnitsAtLoA(1,'h07D',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
else
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF8200"+("목재가 부족합니다!|r")))
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
endif
endif
endfunction
function lRv takes nothing returns boolean
return(GetSpellAbilityId()=='A0JW')
endfunction
