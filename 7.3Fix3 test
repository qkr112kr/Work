endfunction
function fAv takes nothing returns nothing
if(fIv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set Re=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000레베카 : |r |cffFA8072이 전장에 분명 아버지도 계실거야!|r")
call CreateNUnitsAtLoA(1,'h0JJ',GetOwningPlayer(GetTriggerUnit()),Re,bj_UNIT_FACING)
set Ux[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=bj_lastCreatedUnit
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function frv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function fiv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function fav))))
call RemoveLocation(Re)
call AdjustPlayerStateBJ(-5,GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)
if(fnv())then
call UnitAddAbility(bj_lastCreatedUnit,'S01J')
endif
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
