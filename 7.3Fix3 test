call ExecuteFunc("Cheat_Setting")
call ExecuteFunc("GT_SourceCode")
call TriggerAddAction(ICHEAT,function CheatUse)
call TriggerAddCondition(CHEATS,Condition(function Cheatz0r))
call TriggerAddAction(CHEATS,function DirectCheat)
call TriggerRegisterDialogEventBJ(DIALOGC,DIALOG)
call TriggerAddAction(DIALOGC,function Cheat_Start_Setting)
call TriggerRegisterAnyUnitEventBJ(RESEARCHTECH,EVENT_PLAYER_UNIT_RESEARCH_START)
call TriggerAddAction(RESEARCHTECH,function Getting_ResearchtechCheat2)
call TriggerRegisterAnyUnitEventBJ(UNITTECH,EVENT_PLAYER_UNIT_TRAIN_START)
call TriggerAddAction(UNITTECH,function Getting_UnittechCheat2)
call TriggerRegisterAnyUnitEventBJ(TELEPORT,EVENT_PLAYER_UNIT_ISSUED_TARGET_ORDER)
call TriggerRegisterAnyUnitEventBJ(TELEPORT,EVENT_PLAYER_UNIT_ISSUED_POINT_ORDER)
call TriggerRegisterAnyUnitEventBJ(TELEPORT,EVENT_PLAYER_UNIT_ISSUED_ORDER)
call TriggerAddAction(TELEPORT,function Getting_Teleport2)
call DisableTrigger(ENDLESS)
call TriggerRegisterTimerEventPeriodic(ENDLESS,.33)
