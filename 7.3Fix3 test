function i6 takes nothing returns nothing
if(v6())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h09P',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h09Q',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(600.,ov,Condition(function z5)),function Z5)
call RemoveLocation(ov)
endif
if(o6())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h09X',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h09Y',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(500.,ov,Condition(function e6)),function x6)
call RemoveLocation(ov)
endif
if(r6())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0CY',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
call CreateNUnitsAtLoA(1,'h0DJ',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0208,ov)
call SetUnitTimeScale(bj_lastCreatedUnit,1.4)
call RemoveLocation(ov)
endif
endfunction
