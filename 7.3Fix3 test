call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
call CreateNUnitsAtLoA(1,'h05W',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+135.))
call CreateNUnitsAtLoA(1,'h05W',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())-45.))
call CreateNUnitsAtLoA(1,'h05W',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())-135.))
call CreateNUnitsAtLoA(1,'h0CH',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h04J',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call RemoveLocation(ov)
if(uZ())then
call PlaySoundBJ(Va)
endif
else
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h04K',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+90.))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call CreateNUnitsAtLoA(1,'h04K',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+180.))
call CreateNUnitsAtLoA(1,'h04K',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+270.))
call CreateNUnitsAtLoA(1,'h04K',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+360.))
call RemoveLocation(ov)
endif
