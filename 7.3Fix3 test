call GroupAddUnit(W,xC)
call GroupAddUnit(W,vC)
call GroupAddUnit(W,XC)
call GroupAddUnit(W,eC)
endfunction
function yH takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='ewsp')
endfunction
function YH takes nothing returns boolean
return(GetRandomInt(1,489)==$9B)
endfunction
function zH takes nothing returns nothing
if(YH())then
call CreateNUnitsAtLoA(1,'h02J',GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
else
call CreateNUnitsAtLoA(1,GetUnitTypeId(GroupPickRandomUnit(W)),GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
endif
endfunction
function vj takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='ewsp')
endfunction
function ej takes nothing returns nothing
set y=GetRandomInt((U*$A),(U*$F))
call AdjustPlayerStateBJ(y,GetOwningPlayer(GetEnteringUnit()),PLAYER_STATE_RESOURCE_GOLD)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((I2S(y)+"원 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
endfunction
function oj takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='ewsp')
endfunction
function rj takes nothing returns boolean
return(GetRandomReal(0,'d')<=66.)
endfunction
function ij takes nothing returns nothing
if(rj())then
call AdjustPlayerStateBJ(1,GetOwningPlayer(GetEnteringUnit()),PLAYER_STATE_RESOURCE_LUMBER)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+("목재 1 획득 !|r")))
call RemoveUnit(GetEnteringUnit())
else
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+("실패 !|r")))
call RemoveUnit(GetEnteringUnit())
endif
endfunction
function nj takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='ewsp')
endfunction
function Vj takes nothing returns nothing
call SetUnitManaBJ(se[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],(GetUnitStateSwap(UNIT_STATE_MANA,se[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])+(25.+(I2R(U)*1.5))))
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,(""+(("도움소의 현재마나 : "+("|cffff0000"+R2S(GetUnitStateSwap(UNIT_STATE_MANA,se[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))))+"|r")))
call RemoveUnit(GetEnteringUnit())
endfunction
