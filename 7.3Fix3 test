return(Hm())
endfunction
function Jm takes nothing returns nothing
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(hm())then
call SetPlayerTechResearchedSwap('R00S',(GetPlayerTechCountSimple('R00S',Player(8))+1),Player(8))
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(jm())then
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,5.,"|cffff8200 하드모드 솔로 플레이 모드입니다. 스토리의 체력이 줄어듭니다. 10골드 추가 획득!|r")
call AdjustPlayerStateBJ($A,Player(0),PLAYER_STATE_RESOURCE_GOLD)
call DisableTrigger(eV)
else
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,5.,"|cffff8200 어려움 파티 플레이 모드입니다. 스토리 특정 구간의 체력이 파티플레이어 수만큼 증가합니다.|r")
call DisableTrigger(eV)
endif
endfunction
