function emv takes nothing returns boolean
return(UnitHasBuffBJ(GetAttacker(),'B017'))
endfunction
function eMv takes nothing returns nothing
if(emv())then
set ix[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetAttacker()
set oo[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetTriggerUnit()
set ov=GetUnitLoc(GetAttacker())
set Xe=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0KN',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D00CB,GetAttacker())
call CreateNUnitsAtLoA(1,'h0KN',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
call CreateNUnitsAtLoA(1,'h0KN',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0275,Xe)
call RemoveLocation(Xe)
call RemoveLocation(ov)
call SetUnitAnimation(ix[ax],"death")
call SetUnitTimeScale(ix[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))],(1.5*(I2R(GetPlayerTechCountSimple('R01J',GetOwningPlayer(GetAttacker())))*.05)))
else
if(eLv())then
set ix[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetAttacker()
set ov=GetUnitLoc(GetAttacker())
set Xe=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0KN',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D00CB,GetAttacker())
call CreateNUnitsAtLoA(1,'h0KN',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
call CreateNUnitsAtLoA(1,'h0KN',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0275,Xe)
call RemoveLocation(Xe)
call RemoveLocation(ov)
endif
endif
endfunction
