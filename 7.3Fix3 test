endfunction
function Jy takes nothing returns nothing
set ov=GetUnitLoc(GetTriggerUnit())
set xv=Kd(850.,ov,Condition(function hy))
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function Hy)
call DestroyGroup(xv)
if(jy())then
call SetUnitAnimation(GetAttacker(),"attack slam")
set Xe=Dd(ov,50.,(GetUnitFacing(GetAttacker())+90.))
call CreateNUnitsAtLoA(1,'h0J3',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D0278)
call RemoveLocation(Xe)
set Xe=Dd(ov,50.,(GetUnitFacing(GetAttacker())-90.))
call CreateNUnitsAtLoA(1,'h0J3',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D0278)
call RemoveLocation(Xe)
else
call CreateNUnitsAtLoA(1,'h0J3',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D0278)
endif
call RemoveLocation(ov)
endfunction
