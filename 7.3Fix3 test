endfunction
function j4 takes nothing returns boolean
return(GetLocalPlayer()==GetOwningPlayer(GetAttacker()))
endfunction
function J4 takes nothing returns boolean
return(UnitHasBuffBJ(GetAttacker(),'B00C')==false)
endfunction
function k4 takes nothing returns nothing
if(J4())then
set io=GetUnitLoc(GetAttacker())
set Lx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetAttacker()
call PauseUnit(Lx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))],true)
set mx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetTriggerUnit()
set Mx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetUnitLoc(mx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))])
call SetUnitAnimation(Lx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))],"spell one")
call CreateNUnitsAtLoA(1,'h0KV',GetOwningPlayer(GetAttacker()),io,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
call CreateNUnitsAtLoA(1,'h0J9',GetOwningPlayer(GetAttacker()),io,GetUnitFacing(GetAttacker()))
call RemoveLocation(io)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call SetUnitTimeScale(bj_lastCreatedUnit,2.)
if(j4())then
call PlaySoundBJ(Da)
endif
endif
endfunction
