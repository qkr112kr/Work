function LCv takes nothing returns boolean
return(tx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]==3)
endfunction
function Ldv takes nothing returns nothing
set tx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=(tx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]+1)
call RemoveUnit(GetTriggerUnit())
if(LCv())then
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),4.,("누적 3포인트|cffFF8200"+("를 획득하여 |cffffd700 1기의 랜덤위습 획득!|r")))
set tx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=0
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
if(Lcv())then
call AdjustPlayerStateBJ(1,GetOwningPlayer(GetTriggerUnit()),PLAYER_STATE_RESOURCE_LUMBER)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),4.,("|cff20b2aa1개의 추가목재 획득!"))
endif
else
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),4.,(""+(I2S(tx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])+"|cffFF8200 포인트 적립!")))
endif
endfunction
