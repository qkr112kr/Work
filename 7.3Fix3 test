return(CountUnitsInGroup(xv)>=65)
endfunction
function xm takes nothing returns nothing
set xv=sf(Lo,Player(6))
set q[0]=CountUnitsInGroup(xv)
call MultiboardSetItemValue(wC[0],I2S(q[0]))
if(uL())then
set wv[1]=(wv[1]+1)
call SetPlayerStateBJ(Player(0),PLAYER_STATE_RESOURCE_GOLD,0)
call SetPlayerStateBJ(Player(0),PLAYER_STATE_RESOURCE_LUMBER,0)
set bj_wantDestroyGroup=true
call ForGroupBJ(sf(GetWorldBounds(),Player(0)),function tL)
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function TL)
call DisplayTimedTextToForce(bj_FORCE_PLAYER[0],20.,"TRIGSTR_15443")
endif
call GroupClear(xv)
call DestroyGroup(xv)
set xv=sf(mo,Player(6))
set q[1]=CountUnitsInGroup(xv)
call MultiboardSetItemValue(wC[1],I2S(q[1]))
if(WL())then
set wv[2]=(wv[2]+1)
call SetPlayerStateBJ(Player(1),PLAYER_STATE_RESOURCE_GOLD,0)
call SetPlayerStateBJ(Player(1),PLAYER_STATE_RESOURCE_LUMBER,0)
set bj_wantDestroyGroup=true
call ForGroupBJ(sf(GetWorldBounds(),Player(1)),function UL)
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function wL)
call DisplayTimedTextToForce(bj_FORCE_PLAYER[1],20.,"TRIGSTR_15444")
endif
call GroupClear(xv)
call DestroyGroup(xv)
set xv=sf(Mo,Player(6))
set q[3]=CountUnitsInGroup(xv)
call MultiboardSetItemValue(wC[3],I2S(q[3]))
if(zL())then
set wv[4]=(wv[4]+1)
call SetPlayerStateBJ(Player(3),PLAYER_STATE_RESOURCE_GOLD,0)
call SetPlayerStateBJ(Player(3),PLAYER_STATE_RESOURCE_LUMBER,0)
set bj_wantDestroyGroup=true
call ForGroupBJ(sf(GetWorldBounds(),Player(3)),function yL)
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function YL)
call DisplayTimedTextToForce(bj_FORCE_PLAYER[3],20.,"TRIGSTR_15445")
endif
call GroupClear(xv)
call DestroyGroup(xv)
set xv=sf(po,Player(6))
set q[4]=CountUnitsInGroup(xv)
call MultiboardSetItemValue(wC[4],I2S(q[4]))
if(em())then
set wv[5]=(wv[5]+1)
call SetPlayerStateBJ(Player(4),PLAYER_STATE_RESOURCE_GOLD,0)
call SetPlayerStateBJ(Player(4),PLAYER_STATE_RESOURCE_LUMBER,0)
set bj_wantDestroyGroup=true
call ForGroupBJ(sf(GetWorldBounds(),Player(4)),function ZL)
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function vm)
call DisplayTimedTextToForce(bj_FORCE_PLAYER[4],20.,"TRIGSTR_15446")
endif
call GroupClear(xv)
call DestroyGroup(xv)
endfunction
function om takes nothing returns nothing
local timer rm=CreateTimer()
call TimerStart(rm,.7,true,function xm)
set rm=null
endfunction
function am takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function nm takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function Vm takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function Em takes nothing returns boolean
return(IsUnitType(GetFilterUnit(),UNIT_TYPE_ANCIENT))!=null
endfunction
function Xm takes nothing returns boolean
return(CountUnitsInGroup(hd(ke[Ke],Condition(function Em)))>=1)
endfunction
function Om takes nothing returns nothing
set Ke=1
loop
exitwhen Ke>5
if(Xm())then
set wv[Ke]=(wv[Ke]+1)
call SetPlayerStateBJ(Player(-1+(Ke)),PLAYER_STATE_RESOURCE_GOLD,0)
call SetPlayerStateBJ(Player(-1+(Ke)),PLAYER_STATE_RESOURCE_LUMBER,0)
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(bj_mapInitialPlayableArea,Player(-1+(Ke))),function am)
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(ke[Ke],Player(8)),function nm)
set bj_wantDestroyGroup=true
call ForGroupBJ(Jd(ke[Ke],Player(6)),function Vm)
call DisplayTimedTextToForce(pd(Player(-1+(Ke))),20.,"TRIGSTR_2872")
endif
set Ke=Ke+1
endloop
endfunction
