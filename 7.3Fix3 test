call lf(bj_lastCreatedUnit,GetEnumUnit(),(GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetEnumUnit())*.01),1.5,1.5,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_NORMAL)
endfunction
function g4 takes nothing returns boolean
return(GetLocalPlayer()==GetOwningPlayer(GetTriggerUnit()))
endfunction
function G4 takes nothing returns nothing
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0HH',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call CreateNUnitsAtLoA(1,'h0E2',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call SetUnitTimeScale(bj_lastCreatedUnit,2.)
call CreateNUnitsAtLoA(1,'h0FF',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call SetUnitTimeScale(bj_lastCreatedUnit,2.)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(700.,ov,Condition(function f4)),function F4)
call CameraClearNoiseForPlayer(GetOwningPlayer(GetTriggerUnit()))
call RemoveLocation(ov)
if(g4())then
call PlaySoundBJ(na)
endif
endfunction
function H4 takes nothing returns boolean
