call SetUnitPositionLoc(GetTriggerUnit(),ov)
call CreateNUnitsAtLoA(1,'h0AY',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call RemoveLocation(ov)
endif
endif
