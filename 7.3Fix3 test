function r3 takes nothing returns nothing
if(o3())then
if(x3())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0CW',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
