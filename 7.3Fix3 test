else
if(((S2I(SubStringBJ(GetEventPlayerChatString(),7,8))==2)))then
call DisplayTextToForce(Func0010(GetTriggerPlayer()),(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 원하는 경품번호 2번"))
call DisplayTextToForce(Func0010(GetTriggerPlayer()),(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 님 2번 경품은 초월 랜덤 유닛 입니다"))
endif
endif
endfunction
