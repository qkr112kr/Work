if IsUnitType(GetTriggerUnit(),UNIT_TYPE_STRUCTURE)==true then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cff2c952c선택 |r|cffffbe0a건물|r|cff2c952c을 완성시켰습니다.|r"+" "))
call UnitSetConstructionProgress(GetTriggerUnit(),100)
call UnitSetUpgradeProgress(GetTriggerUnit(),100)
else
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 건물이 아닙니다!|r"+" "))
endif
if GetTriggerPlayer()==Selected_Check[(20*15)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*5)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*15)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
