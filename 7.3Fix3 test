function fvv takes nothing returns boolean
return(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)>=5)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function D6v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function D7v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function D8v)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function D9v)))>=1)
endfunction
function fev takes nothing returns nothing
if(fvv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set Re=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000패트릭 레드필드 : |r|cffFA8072지금부터 단 혼자서 '해적왕'이 되어보이겠다!|r")
call CreateNUnitsAtLoA(1,'h0IH',GetOwningPlayer(GetTriggerUnit()),Re,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function D2v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function D3v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function D4v))))
call RemoveLocation(Re)
call AdjustPlayerStateBJ(-5,GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)
if(D5v())then
call UnitAddAbility(bj_lastCreatedUnit,'S01J')
endif
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
