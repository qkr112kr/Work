call DisableTrigger(REENDLESS)
call TriggerRegisterTimerEventPeriodic(REENDLESS,.50)
call TriggerAddAction(REENDLESS,function Getting_REEndless)
call DisableTrigger(ADDDAMAGE)
call TriggerRegisterAnyUnitEventBJ(ADDDAMAGE,EVENT_PLAYER_UNIT_ATTACKED)
call TriggerAddAction(ADDDAMAGE,function Getting_BasicAttack)
call DisableTrigger(REDUCE)
