function US takes nothing returns boolean
return(R2I(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker()))==$96)
endfunction
function wS takes nothing returns nothing
if(tS())then
set ov=GetUnitLoc(GetTriggerUnit())
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=3
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set Xe=Dd(ov,200.,(120.*I2R(bj_forLoopAIndex)))
call CreateNUnitsAtLocFacingLocBJ(1,'h02M',GetOwningPlayer(GetAttacker()),Xe,ov)
call CreateNUnitsAtLocFacingLocBJ(1,'h07N',GetOwningPlayer(GetAttacker()),Xe,ov)
call RemoveLocation(Xe)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(400.,ov,Condition(function sS)),function SS)
call RemoveLocation(ov)
endif
if(US())then
call SetUnitManaBJ(GetAttacker(),.0)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0F0',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0FP',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(600.,ov,Condition(function TS)),function uS)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
