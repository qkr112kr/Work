call DialogAddButtonBJ(M,"|cffee82ee보통|r 모드")
set P[2]=bj_lastCreatedButton
call DialogAddButtonBJ(M,"|cffff0000어려움|r 모드")
set P[3]=bj_lastCreatedButton
call DialogAddButtonBJ(M,"|cff9400d3지옥|r 모드")
set P[4]=bj_lastCreatedButton
call DialogAddButtonBJ(M,"|cffffd700신|r 모드")
set P[5]=bj_lastCreatedButton
call DialogAddButtonBJ(M,"|CFF834683악몽|r 모드")
