endif
endif
set re=CreateSound("Sound\\Law.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function f0v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function f1v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function f2v))))
