function J0v takes nothing returns nothing
if(J_v())then
if(JZv())then
set re=CreateSound("Sound\\vivi.mp3",false,false,false,$A,$A,"")
call StartSound(re)
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000 비비-ver 해적: |r|cffFA8072이가람! 나! 해적여왕이 될거야!|r")
call CreateNUnitsAtLoA(1,'h08N',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function JUv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Jwv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function JWv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Jyv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function JYv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Jzv))))
call AdjustPlayerStateBJ(-5,GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)
endif
endif
