function FQv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function FJv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Fkv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function FKv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function Flv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function FLv)))>=1)
endfunction
function Fsv takes nothing returns nothing
if(FQv())then
set re=CreateSound("Sound\\Usop.mp3",false,false,false,$A,$A,"")
call StartSound(re)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
if(Fjv())then
call CreateNUnitsAtLoA(1,'h05H',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
call CreateNUnitsAtLoA(1,'h05H',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
set vx[(1+GetPlayerId(GetTriggerPlayer()))]=bj_lastCreatedUnit
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000우솝 : |r|cffFA8072옛날엔 허풍을 펑펑 날릴 수 있었는데 지금은 그게 진짜로 되버린단 말이지!|r")
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fmv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function FMv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fpv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function FPv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function Fqv))))
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
function Ftv takes nothing returns boolean
