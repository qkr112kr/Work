call AddSpecialEffectTargetUnitBJ("origin",bj_lastCreatedUnit,"t_judgement6.mdx")
call UnitRemoveAbility(bj_lastCreatedUnit,'A02D')
else
call CreateNUnitsAtLoA(1,'h0GS',GetTriggerPlayer(),Fe,bj_UNIT_FACING)
endif
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function jiv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function jav))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function jnv))))
call AdjustPlayerStateBJ(-$A,GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)
call cd(.12)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
