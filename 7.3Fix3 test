function Getting_PrimitiveCreateCheat takes integer fi2i returns nothing
local integer i2i=(15*(fi2i-1))+1
loop
exitwhen i2i>(15*fi2i)
call CreateItemLoc(udg_rcItemType[i2i-1],Location(0,0))
call ND2P(GetTriggerPlayer(),0,0,15.,(("|cffffbe0a"+I2S(i2i)+" - "))+(GetItemName(GetLastCreatedItem())))
call RemoveItem(GetLastCreatedItem())
set i2i=i2i+1
endloop
endfunction
function Getting_AllStateCheat takes unit fu2u,integer fi2i returns nothing
call ModifyHeroStat(bj_HEROSTAT_STR,fu2u,bj_MODIFYMETHOD_ADD,fi2i)
call ModifyHeroStat(bj_HEROSTAT_AGI,fu2u,bj_MODIFYMETHOD_ADD,fi2i)
call ModifyHeroStat(bj_HEROSTAT_INT,fu2u,bj_MODIFYMETHOD_ADD,fi2i)
endfunction
function Getting_MindcontrolCheat2 takes nothing returns nothing
if Selected_Check[(20*6)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다. 귀속할 수 없습니다.|r")))
call ND2P(Player(GetPlayerId(GetOwningPlayer(GetTriggerUnit()))),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+" 이(가) 당신의 유닛을 귀속하려고 했습니다!|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+("|r |cff2c952c을(를)|r |cffff4500"+(GetPlayerName(udg_rcPlayer[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c님에게로|r |cffffbe0a귀속|r|cff2c952c시킵니다.|r |cffff0000(색상 변경)|r")))))
if CUnitC[120+GetPlayerId(GetTriggerPlayer())]<=15 and CUnitC[120+GetPlayerId(GetTriggerPlayer())]>=0 then
call SetUnitOwner(GetTriggerUnit(),Player(CUnitC[120+GetPlayerId(GetTriggerPlayer())]),true)
else
call SetUnitOwner(GetTriggerUnit(),GetTriggerPlayer(),true)
endif
endif
if GetTriggerPlayer()==Selected_Check[(20*16)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*6)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*16)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_MindcontrolCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*6)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
elseif fi2i==63 then
set Selected_Check[(20*6)+GetPlayerId(GetTriggerPlayer())]=null
else
set Selected_Check[(20*6)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set Selected_Check[(20*16)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set CUnitC[120+GetPlayerId(GetTriggerPlayer())]=fi2i-1
endif
endfunction
function Getting_InvincibleCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
call SetUnitInvulnerable(fu2u,true)
elseif fi2i==63 then
call SetUnitInvulnerable(fu2u,false)
endif
endfunction
function Getting_WOLFList takes integer fi2i returns nothing
local integer i2i=(15*(fi2i-1))+1
if fi2i==0 then
call ND2P(GetTriggerPlayer(),0,0,15.00,("0이상의 숫자를 입력해주세요"))
else
loop
exitwhen i2i>15*fi2i
call ND2P(GetTriggerPlayer(),0,0,15.00,(I2S(i2i)+(" : |cffffbe0a"+Player_Color[20+i2i])))
set i2i=i2i+1
endloop
endif
endfunction
function Getting_WOLFListCreate2 takes integer fi2i returns nothing
local integer i2i=(15*(fi2i-1))+1
if fi2i==0 then
call ND2P(GetTriggerPlayer(),0,0,15.00,("0이상의 숫자를 입력해주세요"))
else
loop
exitwhen i2i>15*fi2i
call CreateItemLoc(CopyI[40+i2i],GetUnitLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))
call ND2P(GetTriggerPlayer(),0,0,7.00,("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+("|r |cff2c952c님의|r |cffff0000"+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+("|r |cff2c952c에게|r |cffffbe0a"+(Player_Color[20+i2i]+"|r|cff2c952c을|r |cffff0000 생성시켜|r |cff2c952c줍니다.")))))))
set i2i=i2i+1
endloop
endif
endfunction
function Getting_WOLFListPart takes string fs2s returns nothing
local integer i2i=1
local integer i3i=0
local integer i4i=0
set i2i=1
loop
exitwhen i2i>20
set i3i=(((40*(i2i-1))+0)+1)
loop
exitwhen i3i>(i2i*40)
set i4i=0
loop
exitwhen i4i>50
if(SubStringBJ(fs2s,15,99)==SubStringBJ(Part_STR[i3i],(0+i4i),((StringLength(SubStringBJ(fs2s,15,99))-1)+i4i)))then
call CreateItemLoc(CopyI[40+i3i],GetUnitLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))
call ND2P(GetTriggerPlayer(),0,0,7.00,("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+("|r |cff2c952c님의|r |cffff0000"+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+("|r |cff2c952c에게|r |cffffbe0a"+(GetItemName(GetLastCreatedItem())+"|r|cff2c952c을|r |cffff0000 생성시켜|r |cff2c952c줍니다.")))))))
endif
set i4i=i4i+1
endloop
set i3i=i3i+1
endloop
call TriggerSleepAction(0.00)
set i2i=i2i+1
endloop
endfunction
function Getting_WOLFCreate takes string fs2s returns nothing
local integer i2i=0
loop
exitwhen i2i>470
if(SubStringBJ(fs2s,10,99)==Part_STR[i2i])then
call CreateItemLoc(CopyI[40+i2i],GetUnitLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))
call ND2P(GetTriggerPlayer(),0,0,7.00,("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+("|r |cff2c952c님의|r |cffff0000"+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+("|r |cff2c952c에게|r |cffffbe0a"+(GetItemName(GetLastCreatedItem())+"|r|cff2c952c을|r |cffff0000 생성시켜|r |cff2c952c줍니다.")))))))
return
endif
set i2i=i2i+1
endloop
endfunction
