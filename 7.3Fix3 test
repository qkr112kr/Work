function m7v takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='e002')
endfunction
function m8v takes nothing returns nothing
call CreateNUnitsAtLoA(1,GetUnitTypeId(GroupPickRandomUnit(rv)),GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
endfunction
function Mvv takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='e003')
endfunction
function Mev takes nothing returns nothing
call CreateNUnitsAtLoA(1,GetUnitTypeId(GroupPickRandomUnit(iv)),GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
endfunction
function Mov takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='e004')
endfunction
function Mrv takes nothing returns boolean
return(GetRandomInt(1,48)==1)
endfunction
function Miv takes nothing returns nothing
if(Mrv())then
call CreateNUnitsAtLoA(1,GetUnitTypeId(GroupPickRandomUnit(Vv)),GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
else
call CreateNUnitsAtLoA(1,GetUnitTypeId(GroupPickRandomUnit(av)),GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
endif
endfunction
function Mnv takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='e005')
endfunction
function MVv takes nothing returns nothing
set y=GetRandomInt($C,20)
call AdjustPlayerStateBJ(y,GetOwningPlayer(GetEnteringUnit()),PLAYER_STATE_RESOURCE_LUMBER)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000목재 "+((I2S(y)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
endfunction
function MXv takes nothing returns boolean
return(GetUnitTypeId(GetEnteringUnit())=='e005')
endfunction
function MOv takes nothing returns nothing
call CreateNUnitsAtLoA(1,'h02J',GetOwningPlayer(GetEnteringUnit()),GetRectCenter(ei),bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h03T',GetOwningPlayer(GetEnteringUnit()),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetEnteringUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetEnteringUnit())
endfunction
