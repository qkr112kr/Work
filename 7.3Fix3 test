function Hm takes nothing returns boolean
return(GetPlayerSlotState(Player(0))==PLAYER_SLOT_STATE_PLAYING)and(GetPlayerSlotState(Player(1))==PLAYER_SLOT_STATE_EMPTY)and(GetPlayerSlotState(Player(3))==PLAYER_SLOT_STATE_EMPTY)and(GetPlayerSlotState(Player(4))==PLAYER_SLOT_STATE_EMPTY)
endfunction
