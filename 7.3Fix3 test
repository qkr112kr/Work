function aS takes nothing returns nothing
call lf(GetAttacker(),GetTriggerUnit(),12000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
if(eS())then
call SetUnitAnimation(GetAttacker(),"spell")
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0JD',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
if(vS())then
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.15*GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
call RemoveLocation(ov)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h05Q',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueTargetOrderById(bj_lastCreatedUnit,$D007F,GetTriggerUnit())
call RemoveLocation(ov)
endif
if(iS())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0II',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(375.,ov,Condition(function xS)),function oS)
call RemoveLocation(ov)
if(rS())then
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.12*GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
endif
endfunction
