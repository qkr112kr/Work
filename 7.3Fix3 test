function M6v takes nothing returns boolean
call nD(ed,xd)
return true
endfunction
function M7v takes nothing returns boolean
call ED(ed,od)
return true
endfunction
function M8v takes nothing returns boolean
call OD(ed,rd,id,ad)
return true
endfunction
function M9v takes nothing returns boolean
call ND(ed,nd,Vd)
return true
endfunction
function pvv takes nothing returns boolean
call cD(ed)
return true
endfunction
function pev takes nothing returns boolean
call CD(ed)
return true
endfunction
function pxv takes nothing returns boolean
call dD(ed)
return true
endfunction
function pov takes nothing returns boolean
call DD(ed)
return true
endfunction
function prv takes nothing returns boolean
call fD(ed)
return true
endfunction
function piv takes nothing returns boolean
call FD(ed,Ed,xd)
return true
endfunction
function M5v takes nothing returns nothing
set WC[1]=CreateTrigger()
call TriggerAddAction(WC[1],function M6v)
call TriggerAddCondition(WC[1],Condition(function M6v))
set yC[1]=CreateTrigger()
call TriggerAddAction(yC[1],function M7v)
call TriggerAddCondition(yC[1],Condition(function M7v))
set YC[1]=CreateTrigger()
call TriggerAddAction(YC[1],function M8v)
call TriggerAddCondition(YC[1],Condition(function M8v))
set zC[1]=CreateTrigger()
call TriggerAddAction(zC[1],function M9v)
call TriggerAddCondition(zC[1],Condition(function M9v))
set ZC[1]=CreateTrigger()
call TriggerAddAction(ZC[1],function pvv)
call TriggerAddCondition(ZC[1],Condition(function pvv))
set ZC[2]=CreateTrigger()
call TriggerAddAction(ZC[2],function pev)
call TriggerAddCondition(ZC[2],Condition(function pev))
set ZC[3]=CreateTrigger()
call TriggerAddAction(ZC[3],function pxv)
call TriggerAddCondition(ZC[3],Condition(function pxv))
set ZC[4]=CreateTrigger()
call TriggerAddAction(ZC[4],function pov)
call TriggerAddCondition(ZC[4],Condition(function pov))
set ZC[5]=CreateTrigger()
call TriggerAddAction(ZC[5],function prv)
call TriggerAddCondition(ZC[5],Condition(function prv))
set vd[1]=CreateTrigger()
call TriggerAddAction(vd[1],function piv)
call TriggerAddCondition(vd[1],Condition(function piv))
call ExecuteFunc("Vf")
call InitTrig_llllllIllll(  )
endfunction
