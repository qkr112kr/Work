endfunction
function m4 takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),3000000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call lf(bj_lastCreatedUnit,GetEnumUnit(),(I2R(UnitCountBuffsExBJ(2,2,Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],false,true))*500000.),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_NORMAL)
endfunction
function M4 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(GetTriggerUnit())))
endfunction
function p4 takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),2300000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call lf(bj_lastCreatedUnit,GetEnumUnit(),(I2R(UnitCountBuffsExBJ(2,2,Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],false,true))*500000.),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_NORMAL)
endfunction
function P4 takes nothing returns boolean
return(IsUnitAliveBJ(mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
endfunction
function q4 takes nothing returns boolean
return(GetLocalPlayer()==GetOwningPlayer(GetTriggerUnit()))
endfunction
function Q4 takes nothing returns nothing
call RemoveUnit(GetTriggerUnit())
call CameraSetEQNoiseForPlayer(GetOwningPlayer(GetTriggerUnit()),9.)
if(P4())then
call RemoveLocation(Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
set Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=GetUnitLoc(mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
set io=GetUnitLoc(GetTriggerUnit())
call SetUnitAnimation(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],"spell slam")
call CreateNUnitsAtLoA(1,'h0H1',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call SetUnitTimeScale(bj_lastCreatedUnit,1.2)
call CreateNUnitsAtLoA(1,'h0KX',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call SetUnitTimeScale(bj_lastCreatedUnit,2.2)
call CreateNUnitsAtLoA(1,'h0F2',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call CreateNUnitsAtLoA(1,'h0HQ',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],(180.+GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])))
call SetUnitTimeScale(bj_lastCreatedUnit,3.)
call CreateNUnitsAtLoA(1,'h0KV',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,io)
call CreateNUnitsAtLoA(1,'h0KW',GetOwningPlayer(GetTriggerUnit()),io,GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call RemoveLocation(io)
call SetUnitVertexColorBJ(bj_lastCreatedUnit,'d','d','d',35.)
call SetUnitAnimation(bj_lastCreatedUnit,"spell slam")
call SetUnitTimeScale(bj_lastCreatedUnit,.8)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(700.,Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],Condition(function M4)),function p4)
call RemoveLocation(Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
else
set io=GetUnitLoc(GetTriggerUnit())
call SetUnitAnimation(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],"spell slam")
call CreateNUnitsAtLoA(1,'h0H1',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call SetUnitTimeScale(bj_lastCreatedUnit,1.2)
call CreateNUnitsAtLoA(1,'h0KX',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call SetUnitTimeScale(bj_lastCreatedUnit,2.2)
call CreateNUnitsAtLoA(1,'h0F2',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call CreateNUnitsAtLoA(1,'h0HQ',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],(180.+GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])))
call SetUnitTimeScale(bj_lastCreatedUnit,3.)
call CreateNUnitsAtLoA(1,'h0KV',GetOwningPlayer(GetTriggerUnit()),Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,io)
call CreateNUnitsAtLoA(1,'h0KW',GetOwningPlayer(GetTriggerUnit()),io,GetUnitFacing(Lx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]))
call RemoveLocation(io)
call SetUnitVertexColorBJ(bj_lastCreatedUnit,'d','d','d',35.)
call SetUnitAnimation(bj_lastCreatedUnit,"spell slam")
call SetUnitTimeScale(bj_lastCreatedUnit,.8)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(700.,Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],Condition(function L4)),function m4)
call RemoveLocation(Mx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
endif
if(q4())then
call PlaySoundBJ(da)
endif
endfunction
