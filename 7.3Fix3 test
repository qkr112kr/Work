return(GetSpellAbilityId()=='A0PO')
endfunction
function eqv takes nothing returns nothing
set ox[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=GetUnitLoc(GetTriggerUnit())
set rx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=GetSpellTargetLoc()
set nx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=.0
set Xx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=AngleBetweenPoints(ox[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],rx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
call CameraSetEQNoiseForPlayer(GetOwningPlayer(GetTriggerUnit()),3)
call StartTimerBJ(xx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],true,.12)
endfunction
function esv takes nothing returns boolean
return(GetUnitPointValue(oo[ax])<$C8)
endfunction
function eSv takes nothing returns boolean
return(nx[ax]==3.)
endfunction
function etv takes nothing returns nothing
set ax=1
loop
exitwhen ax>5
exitwhen GetExpiredTimer()==xx[ax]
set ax=ax+1
endloop
if(esv())then
call lf(ix[ax],oo[ax],((150000.+(GetUnitStateSwap(UNIT_STATE_MAX_LIFE,oo[ax])*.15))+((.03*GetUnitStateSwap(UNIT_STATE_MAX_LIFE,oo[ax]))*I2R(GetUnitAbilityLevelSwapped('A0BO',ix[ax])))),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
else
call lf(ix[ax],oo[ax],(150000.+(30000.*I2R(GetUnitAbilityLevelSwapped('A0BO',ix[ax])))),1.,1.5,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
set nx[ax]=(nx[ax]+1)
set Xx[ax]=AngleBetweenPoints(ox[ax],rx[ax])
set Ex[ax]=Dd(ox[ax],32.,(Xx[ax]+(41.*Pow(-1.,nx[ax]))))
set Vx[ax]=Dd(rx[ax],GetRandomReal(25.,75.),GetRandomReal(0,360))
if(eSv())then
call CreateNUnitsAtLoA(1,'h0JN',GetOwningPlayer(ix[ax]),Ex[ax],GetUnitFacing(ix[ax]))
call CreateNUnitsAtLoA(1,'h04W',GetOwningPlayer(ix[ax]),Vx[ax],GetUnitFacing(ix[ax]))
call PauseTimerBJ(true,xx[ax])
call RemoveLocation(ox[ax])
call RemoveLocation(Ex[ax])
call RemoveLocation(rx[ax])
call RemoveLocation(Vx[ax])
call CameraClearNoiseForPlayer(GetOwningPlayer(ix[ax]))
call UnitRemoveAbility(ix[ax],'B05L')
call UnitRemoveAbility(ix[ax],'B017')
else
call CreateNUnitsAtLoA(1,'h0JN',GetOwningPlayer(ix[ax]),Ex[ax],GetUnitFacing(ix[ax]))
call CreateNUnitsAtLoA(1,'h04W',GetOwningPlayer(ix[ax]),Vx[ax],GetUnitFacing(ix[ax]))
call RemoveLocation(Ex[ax])
call RemoveLocation(Vx[ax])
endif
endfunction
