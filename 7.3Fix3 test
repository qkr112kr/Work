function KNv takes nothing returns nothing
call RemoveItem(GetSoldItem())
call CreateNUnitsAtLoA(1,'o032',Player(8),w[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],270.)
call StartTimerBJ(qx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],false,60.)
call CreateTimerDialogBJ(bj_lastStartedTimer,"바제스 퇴치-남은 시간 : ")
set Qx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))]=bj_lastCreatedTimerDialog
set Do=1
loop
exitwhen Do>5
call TimerDialogDisplayForPlayerBJ(false,Qx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],Player(-1+(Do)))
set Do=Do+1
endloop
call TimerDialogDisplayForPlayerBJ(true,Qx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],GetOwningPlayer(GetTriggerUnit()))
endfunction
