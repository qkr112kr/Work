call CreateNUnitsAtLoA(1,'h0A2',Player(3),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(TH())then
set ov=GetRectCenter(po)
call CreateNUnitsAtLoA(1,'h0A2',Player(4),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|CFFFFFA7830회 이상 세이브회수에 따른 보상이 지급됩니다. (세이브는 하드부터 클리어 후 가능합니다.)|r")
call DisableTrigger(GetTriggeringTrigger())
call TriggerClearActions(GetTriggeringTrigger())
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function wH takes nothing returns nothing
call GroupAddUnit(W,yc)
call GroupAddUnit(W,Zc)
call GroupAddUnit(W,Yc)
call GroupAddUnit(W,zc)
