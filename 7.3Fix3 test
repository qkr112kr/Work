call CreateNUnitsAtLoA(1,'h075',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h075',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h075',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h076',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
if(V5())then
call lf(GetAttacker(),GetTriggerUnit(),(GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit())*.4),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
