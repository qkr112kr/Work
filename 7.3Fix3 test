return(GetRandomInt(1,3)==1)
endfunction
function lNv takes nothing returns boolean
return(GetRandomReal(0,'d')<=40.)
endfunction
function lbv takes nothing returns boolean
return(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)>=30)
endfunction
function lBv takes nothing returns nothing
if(lbv())then
call SetPlayerStateBJ(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER,(GetPlayerState(GetTriggerPlayer(),PLAYER_STATE_RESOURCE_LUMBER)-30))
if(lNv())then
if(lAv())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h07C',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
else
if(lIv())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0AU',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveLocation(ov)
call RemoveUnit(GetTriggerUnit())
else
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h07E',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF0000"+((GetUnitName(bj_lastCreatedUnit)+" 획득 ! ")+"|r")))
call RemoveUnit(GetTriggerUnit())
call RemoveLocation(ov)
endif
endif
call RemoveUnit(Ov[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))])
else
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,((("|cffff8200"+GetPlayerName(GetOwningPlayer(GetTriggerUnit())))+("|r |cffff8200님이 고대병기 획득을|r |cffFF0000"))+"실패 하셨습니다!|r"))
call UnitRemoveAbility(GetTriggerUnit(),'A0JW')
endif
else
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),10.,("|cffFF8200"+("목재가 부족합니다!|r")))
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
endif
endfunction
