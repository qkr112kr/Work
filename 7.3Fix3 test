function evv takes nothing returns nothing
if(v9v())then
set ov=GetUnitLoc(GetTriggerUnit())
call KillUnit(GetAttacker())
call CreateNUnitsAtLocFacingLocBJ(1,'h0FM',GetOwningPlayer(GetAttacker()),ov,ov)
call SetUnitScalePercent(bj_lastCreatedUnit,150.,150.,150.)
call CreateNUnitsAtLoA(1,'h0IM',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+120.))
call lf(bj_lastCreatedUnit,GetTriggerUnit(),1000000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call CreateNUnitsAtLoA(1,'h0IM',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+240.))
call CreateNUnitsAtLoA(1,'h0IM',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call CreateNUnitsAtLoA(1,'h0I2',GetOwningPlayer(GetAttacker()),ov,GetRandomReal(0,360))
call SetUnitTimeScale(bj_lastCreatedUnit,.85)
set xv=Kd(650.,ov,Condition(function v7v))
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function v8v)
call DestroyGroup(xv)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
set ov=GetUnitLoc(GetTriggerUnit())
set Xe=Dd(ov,200.,GetRandomReal(0,360))
call SetUnitPositionLoc(GetAttacker(),Xe)
call RemoveLocation(Xe)
call RemoveLocation(ov)
call IssueTargetOrderById(GetAttacker(),$D000F,GetTriggerUnit())
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0HU',GetOwningPlayer(GetAttacker()),ov,GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0I1',GetOwningPlayer(GetAttacker()),ov,GetRandomReal(0,360))
call lf(bj_lastCreatedUnit,GetTriggerUnit(),350000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call RemoveLocation(ov)
endif
endfunction
