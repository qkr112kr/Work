function FD takes trigger iD,string gD,boolean GD returns nothing
local integer i=0
loop
call TriggerRegisterPlayerChatEvent(iD,Player(i),gD,GD)
set i=i+1
exitwhen i>=5
endloop
endfunction
function hD takes string s returns string
local integer i=0
local string HD=null
local string jD
local string JD
local integer kD=StringLength(s)
loop
set JD=SubString(s,i,i+2)
if JD=="|c" or JD=="|C" then
set jD=null
set i=i+$A
elseif JD=="|r" or JD=="|R" then
set jD=null
set i=i+2
else
set jD=SubString(s,i,i+1)
set i=i+1
endif
set HD=HD+jD
exitwhen i>=kD
endloop
return HD
endfunction
function KD takes player p,string lD,integer C,string LD returns nothing
local integer RI=GetRandomInt(0,$F4240)
if(GetLocalPlayer()==p)then
call PreloadGenClear()
call PreloadGenStart()
call Preload("----------------- Save Code ------------------")
call Preload("클리어 유저 : "+GetPlayerName(p))
call Preload("Code : "+hD(LD)+" ")
call Preload("클리어 횟수 : "+I2S(C))
call Preload("---------------- Chat Message ----------------")
call Preload(lD)
call Preload("원랜디 카페 : http://cafe.naver.com/clanopcr")
call Preload("도트짤의 카페 : http://cafe.naver.com/dtzrpg")
call PreloadGenEnd("ORD7\\"+GetPlayerName(p)+"_"+I2S(C)+"_"+I2S(RI)+".txt")
call PreloadEnd(3.)
call DisplayTimedTextToPlayer(p,.0,.0,10.,"|cfffffc01워크 폴더 \\ ORD7 \\ "+ae[(1+GetPlayerId(p))]+"_"+I2S(C)+"_"+I2S(RI)+"로 저장되었습니다.|r ")
endif
endfunction
function mD takes string MD returns boolean
local integer pD
set pD=0
loop
exitwhen pD>9
if(MD==I2S(pD))then
return true
endif
set pD=pD+1
endloop
return false
endfunction
function PD takes string MD returns boolean
local string qD=A
local integer pD
set pD=0
loop
exitwhen pD>StringLength(qD)
if(MD==SubString(qD,pD,pD+1))then
return true
endif
set pD=pD+1
endloop
return false
endfunction
function QD takes string MD returns boolean
local string qD=N
local integer pD
set pD=0
loop
exitwhen pD>StringLength(qD)
if(MD==SubString(qD,pD,pD+1))then
return true
endif
set pD=pD+1
endloop
return false
endfunction
function sD takes string MD returns boolean
local string qD=B
local integer pD
set pD=0
loop
exitwhen pD>StringLength(qD)
if(MD==SubString(qD,pD,pD+1))then
return true
endif
set pD=pD+1
endloop
return false
endfunction
function SD takes string MD returns string
local string tD=""
local string TD
local boolean uD=false
local boolean UD=false
local boolean wD=false
local boolean WD=false
local integer pD
set pD=0
loop
exitwhen pD>StringLength(MD)
set TD=SubString(MD,pD,pD+1)
if(mD(TD))then
if(WD)then
set tD=tD+TD
else
set tD=tD+h+TD
endif
set WD=true
set wD=false
set uD=false
set UD=false
endif
if(PD(TD))then
if(wD)then
set tD=tD+TD
else
set tD=tD+G+TD
endif
set WD=false
set wD=true
set uD=false
set UD=false
endif
if(QD(TD))then
if(uD)then
set tD=tD+TD
else
set tD=tD+D+TD
endif
set WD=false
set wD=false
set uD=true
set UD=false
endif
if(sD(TD))then
if(UD)then
set tD=tD+TD
else
set tD=tD+F+TD
endif
set WD=false
set wD=false
set uD=false
set UD=true
endif
set pD=pD+1
endloop
set tD=tD+"|r"
return tD
endfunction
function yD takes string YD returns integer
local string qD="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890"
local string zD
local integer ZD=StringLength(qD)
local integer vf=StringLength(YD)
local integer cutLength
local integer tD=0
local integer pD
local integer ef
set pD=0
loop
exitwhen pD>vf
set zD=SubString(YD,pD,pD+1)
set ef=0
loop
exitwhen ef>ZD
if(zD==SubString(qD,ef,ef+1))then
set tD=tD+(ef*pD)
endif
set ef=ef+1
endloop
set pD=pD+1
endloop
return tD
endfunction
function xf takes integer MD returns string
local string inputStr=I2S(MD)
local string tD=null
local integer of
loop
exitwhen MD<=0
set of=ModuloInteger(MD,x)
set tD=SubString(e,of,of+1)+tD
set MD=MD/ x
endloop
return tD
endfunction
function rf takes string MD returns integer
local integer af=StringLength(MD)
local string nf=null
local integer tD=0
local integer pD
local integer ef
set pD=0
loop
exitwhen pD>=af
set tD=tD*x
set nf=SubString(MD,pD,pD+1)
set ef=0
loop
exitwhen(ef>=x)or(nf==SubString(e,ef,ef+1))
set ef=ef+1
endloop
if(ef<=x)then
set tD=tD+ef
endif
set pD=pD+1
endloop
return tD
endfunction
function Vf takes nothing returns nothing
local integer pD=0
local integer Ef=I-1
local integer ef=1
local integer Xf=StringLength(V)
set pD=1
set Ef=62
loop
exitwhen pD>Ef
set n[pD]=SubString(e,pD-1,pD)
set o[pD]=""
set pD=pD+1
endloop
set E=yD(V)
set X=E
set o[1]=SD(xf(E))
endfunction
function Of takes string Rf,integer If returns string
local integer a=1
local integer Af=0
local integer Nf=1
local integer kD
local integer af=StringLength(Rf)
local string bf
if If<1 then
return""
endif
loop
exitwhen Af==(If-1)or a>af
if SubStringBJ(Rf,a,a)=="-" then
set Af=Af+1
endif
set Nf=a+1
set a=a+1
endloop
if a>af then
return""
endif
set a=1
set Af=0
loop
exitwhen Af==If or a>(af+1)
if SubStringBJ(Rf,a,a)=="-" then
set Af=Af+1
endif
set kD=a-1
set a=a+1
endloop
set bf=SubStringBJ(Rf,Nf,kD)
return bf
endfunction
function Bf takes nothing returns nothing
local integer pD=1
local integer loopE=62
local integer ef=1
local integer Xf
local integer PI=(1+GetPlayerId(GetTriggerPlayer()))
local integer cf=0
local string Cf=GetEventPlayerChatString()
local string array df
local integer array Df
if j[(1+GetPlayerId(GetTriggerPlayer()))]==false then
set O=ae[PI]
set Xf=StringLength(O)
set cf=yD(O)
set Cf=SubString(Cf,6,0)
set df[1]=Of(Cf,1)
set df[2]=Of(Cf,2)
set df[3]=Of(Cf,3)
set Df[1]=rf(df[1])
set Df[2]=rf(df[2])
set Df[3]=rf(df[3])
set Df[4]=Df[2]/(cf*9)
set Df[5]=Df[3]/ Df[4]
set Df[6]=Df[5]-cf-X
set Df[7]=((Df[2]/ 9)/ Df[4])
set Df[8]=Df[5]*Df[4]
set Df[9]=Df[8]/ Df[5]
set Df[$A]=Df[7]*Df[9]*9
if X!=Df[1]then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,$A,"|cffFF0000Error )|r |cffFFE400Load 실패 ! 코드를 재확인해보세요 !|r")
return
elseif cf!=Df[7]then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,$A,"|cffFF0000Error )|r |cffFFE400Load 실패 ! 코드를 재확인해보세요 !|r")
return
elseif Df[8]!=Df[3]then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,$A,"|cffFF0000Error )|r |cffFFE400Load 실패 ! 코드를 재확인해보세요 !|r")
return
elseif Df[2]!=Df[$A]then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,$A,"|cffFF0000Error )|r |cffFFE400Load 실패 ! 코드를 재확인해보세요 !|r")
return
elseif Df[6]>$FA or Df[6]<=0 then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,$A,"|cffFF0000Error )|r |cffFFE400Load 실패 ! 코드를 재확인해보세요 !|r")
return
else
set R[PI]=Df[6]
set xe[PI]=R[PI]
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,$A,"|cffFFE400Load Success ) |r  |cff00FF00현재 클리어횟수는 "+I2S(R[PI])+"회 입니다.|r")
set j[PI]=true
endif
elseif j[PI]then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,60,"|cffFF8200이미 로드를 하셨습니다.|r")
endif
endfunction
function ff takes nothing returns nothing
local timer t=GetExpiredTimer()
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20,"|cffFFE400더 이상 로드가 불가능합니다.|r")
call DestroyTrigger(K)
call DestroyTimer(t)
set t=null
endfunction
function Load_init3 takes nothing returns nothing
local integer pD=0
local integer Ef=I
local timer t=CreateTimer()
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|cffffd70025초동안|r |cffff0000로드가|r |cffffd700가능합니다.|r")
set K=CreateTrigger()
loop
exitwhen pD>Ef
call TriggerRegisterPlayerChatEvent(K,Player(pD),"-load",false)
set j[pD]=false
set pD=pD+1
endloop
call TriggerAddAction(K,function Bf)
call TimerStart(t,L,false,function ff)
set t=null
endfunction
function Ff takes nothing returns nothing
local integer pD=0
local integer Ef=I
local timer t=CreateTimer()
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|cff52E25225초동안 로드가 가능합니다.|r")
set K=CreateTrigger()
loop
exitwhen pD>Ef
call TriggerRegisterPlayerChatEvent(K,Player(pD),"-load",false)
set j[pD]=false
set pD=pD+1
endloop
call TriggerAddAction(K,function Bf)
call TimerStart(t,L,false,function ff)
set t=null
endfunction
function gf takes nothing returns nothing
local integer pD=1
local integer loopE=62
local integer ef=0
local integer Xf
local integer cf=0
local integer Gf=0
local string array OutputCode
local integer hf=GetRandomInt(1,62)
local string Hf=""
local integer PI=(1+GetPlayerId(GetTriggerPlayer()))
if wv[PI]==0 then
if H[PI]==false then
set xe[PI]=xe[PI]+udg_zizonzzz
set o[2]=""
set R[PI]=xe[PI]
if J==3 then
if R[PI]>$F then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,60,"|cffFF00FF해당 모드에서 가능한 세이브 횟수(15회)를 초과하셨습니다.|r")
return
endif
elseif J==4 then
if R[PI]>50 then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,60,"|cffFF00FF해당 모드에서 가능한 세이브 횟수(50회)를 초과하셨습니다.|r")
return
endif
elseif J==5 then
if R[PI]>$FA then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,60,"|cffFF00FF해당 모드에서 가능한 세이브 횟수(250회)를 초과하셨습니다.|r")
return
endif
endif
set O=ae[PI]
set Xf=StringLength(O)
set cf=yD(O)
set Gf=(cf+R[PI]+X)*hf
set o[2]=SD(xf(cf*hf*9))
set o[3]=SD(xf(Gf))
set Hf=o[1]+"-"+o[2]+"-"+o[3]
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,60,Hf)
set H[PI]=true
call KD(GetTriggerPlayer(),"",xe[PI],Hf)
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,$A,"|cffFFE400Save Success ) |r|cff00ff00현재 클리어횟수는 "+I2S(R[PI])+"회 입니다.|r")
elseif H[PI]then
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,60,"|cffFF00FF이미 세이브를 하셨습니다.|r")
endif
else
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,60,"|cffFF8200패배한 유저는 세이브할 수 없습니다.|r")
endif
endfunction
function jf takes nothing returns nothing
local integer pD=0
local integer Ef=I
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20,"|cffFF8200이제부터 세이브가 가능합니다.|r")
set m=CreateTrigger()
loop
exitwhen pD>Ef
call TriggerRegisterPlayerChatEvent(m,Player(pD),"-save",true)
set H[pD]=false
set pD=pD+1
endloop
call TriggerAddAction(m,function gf)
endfunction
