function hh takes nothing returns nothing
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=5
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
if(qG())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cffff7f50[견습선원]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(sG())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cff20b2aa[신참해적]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(tG())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cffffd700[루키]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(uG())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cff4169e1[초신성]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(wG())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cff00ff7f[영웅]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(yG())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cff1FBF00[대해적]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(zG())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cff7b68ee[칠무해]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(vh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cff7b68ee[조커]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(xh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|CFFC15AF4[사황]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(rh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cffFF0000[전설적인]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(ah())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c0080ffff[패왕]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(Vh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c00bc4346[신화적인]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(Xh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c004b4b4b[지존]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(Rh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c00a4d1ff[오로성]|r"+gv[(bj_forLoopBIndex+40)]))
endif
if(Ah())then
set xe[bj_forLoopBIndex]=0
endif
set Fv=1
loop
exitwhen Fv>5
set gv[(Fv+20)]=GetPlayerName(Player(-1+(Fv)))
set gv[(Fv+40)]=GetPlayerName(Player(-1+(Fv)))
set Fv=Fv+1
endloop
set gv[41]=("|c00ff0202"+(gv[41]+"|r"))
set gv[42]=("|cff0041FF"+(gv[42]+"|r"))
set gv[43]=("|cff1BE6B8"+(gv[43]+"|r"))
set gv[44]=("|cff530080"+(gv[44]+"|r"))
set gv[45]=("|cffFFFC00"+(gv[45]+"|r"))
if(Nh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cffff00ff[조이보이]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(bh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c008080c0[패기의]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(Bh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c008080c0[패기의]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(ch())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c008080c0[패기의]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(Ch())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c008080c0[패기의]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(dh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cffffd700[명왕]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(Dh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cff9400D3[천왕]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(fh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|cffff00ff[원랜디NPC]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(Fh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c00ff59ff[그랜드슬램]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(gh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c008080ff[선|r|c00ff59ff샹전갑니다]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
if(Gh())then
call SetPlayerName(Player(-1+(bj_forLoopBIndex)),("|c00a4d1ff[압축해제]|r"+gv[((1+GetPlayerId(Player(-1+(bj_forLoopBIndex))))+40)]))
endif
set Fv=1
loop
exitwhen Fv>5
set gv[(Fv+20)]=GetPlayerName(Player(-1+(Fv)))
set gv[(Fv+40)]=GetPlayerName(Player(-1+(Fv)))
set Fv=Fv+1
endloop
set gv[41]=("|c00ff0202"+(gv[41]+"|r"))
set gv[42]=("|cff0041FF"+(gv[42]+"|r"))
set gv[43]=("|cff1BE6B8"+(gv[43]+"|r"))
set gv[44]=("|cff530080"+(gv[44]+"|r"))
set gv[45]=("|cffFFFC00"+(gv[45]+"|r"))
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
endfunction
function jh takes nothing returns boolean
