endfunction
function up takes nothing returns boolean
return(Tp())
endfunction
function Up takes nothing returns nothing
if(up())then
call SetUnitAbilityLevelSwapped('S00Y',GetSpellTargetUnit(),(GetUnitAbilityLevelSwapped('S00Y',GetSpellTargetUnit())+1))
call SetUnitAbilityLevelSwapped('A0NC',GetSpellTargetUnit(),(GetUnitAbilityLevelSwapped('A0NC',GetSpellTargetUnit())+1))
call UnitRemoveAbility(GetSpellTargetUnit(),'A0KR')
call UnitAddAbility(GetSpellTargetUnit(),'A0DN')
call SetPlayerAbilityAvailableBJ(false,'A03W',GetOwningPlayer(GetTriggerUnit()))
call RemoveUnit(GetTriggerUnit())
else
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),4.,("|cffFF8200"+("자신소유의 조로 초월함에게만 시전 가능합니다!|r")))
endif
endfunction
function Wp takes nothing returns boolean
return(GetSpellAbilityId()=='A0NT')
endfunction
function yp takes nothing returns boolean
return(GetLocalPlayer()==GetOwningPlayer(GetTriggerUnit()))
endfunction
function Yp takes nothing returns nothing
if(yp())then
call CameraClearNoiseForPlayer(GetOwningPlayer(GetTriggerUnit()))
endif
endfunction
function Zp takes nothing returns boolean
return(GetSpellAbilityId()=='A0OO')
endfunction
function vP takes nothing returns boolean
return(GetUnitPointValue(GetTriggerUnit())==$C8)
endfunction
function eP takes nothing returns nothing
set ov=GetUnitLoc(GetSpellTargetUnit())
call CreateNUnitsAtLoA(1,'h0GV',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call RemoveLocation(ov)
if(vP())then
call UnitDamageTargetBJ(bj_lastCreatedUnit,GetTriggerUnit(),(.02*GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
else
call UnitDamageTargetBJ(bj_lastCreatedUnit,GetTriggerUnit(),100000.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
endfunction
function oP takes nothing returns boolean
return(GetSpellAbilityId()=='A0HE')
endfunction
function rP takes nothing returns boolean
return('o02T'==GetUnitTypeId(GetSpellTargetUnit()))or('h0L8'==GetUnitTypeId(GetSpellTargetUnit()))or('o034'==GetUnitTypeId(GetSpellTargetUnit()))
endfunction
function iP takes nothing returns boolean
return(rP())
endfunction
function aP takes nothing returns boolean
return(GetUnitPointValue(GetSpellTargetUnit())<$C8)
endfunction
function nP takes nothing returns nothing
if(aP())then
call RemoveUnit(GetSpellTargetUnit())
set ov=GetUnitLoc(GetSpellTargetUnit())
call CreateNUnitsAtLoA(1,'h0AC',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h02K',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call UnitApplyTimedLife(bj_lastCreatedUnit,'Bmec',30)
call RemoveLocation(ov)
else
if(iP())then
call KillUnit(GetSpellTargetUnit())
set ov=GetUnitLoc(GetSpellTargetUnit())
call CreateNUnitsAtLoA(1,'h0AC',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call CreateNUnitsAtLoA(1,'h02K',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call UnitApplyTimedLife(bj_lastCreatedUnit,'Bmec',30)
call RemoveLocation(ov)
else
call IssueImmediateOrderById(GetTriggerUnit(),$D0004)
call UnitRemoveAbility(GetTriggerUnit(),'A0HE')
call UnitAddAbility(GetTriggerUnit(),'A0HE')
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),5.,"|cffff00ff이 유닛에게는 사용불가합니다!|r")
endif
endif
endfunction
function EP takes nothing returns boolean
return(GetUnitPointValue(GetTriggerUnit())==3)
endfunction
function XP takes nothing returns boolean
return(IsPlayerEnemy(GetOwningPlayer(GetFilterUnit()),GetOwningPlayer(GetTriggerUnit())))
endfunction
function OP takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetTriggerUnit()),1.,1.,ATTACK_TYPE_NORMAL,DAMAGE_TYPE_UNIVERSAL)
endfunction
function RP takes nothing returns boolean
return(IsPlayerEnemy(GetOwningPlayer(GetFilterUnit()),GetOwningPlayer(GetTriggerUnit())))
endfunction
function IP takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),400000.,1.,1.,ATTACK_TYPE_NORMAL,DAMAGE_TYPE_UNIVERSAL)
endfunction
function AP takes nothing returns boolean
return(GetRandomInt(1,37)==1)
endfunction
function NP takes nothing returns boolean
return(GetUnitTypeId(GetTriggerUnit())=='h0KZ')
endfunction
function bP takes nothing returns nothing
if(NP())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'n000',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call SetUnitScalePercent(bj_lastCreatedUnit,315.,315.,315.)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(750.,ov,Condition(function RP)),function IP)
call RemoveLocation(ov)
if(AP())then
call CreateNUnitsAtLoA(1,'h04M',GetOwningPlayer(GetTriggerUnit()),Yx[(1+GetPlayerId(GetOwningPlayer(GetTriggerUnit())))],bj_UNIT_FACING)
call SetUnitTimeScale(bj_lastCreatedUnit,.0)
endif
else
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'n000',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(500.,ov,Condition(function XP)),function OP)
call RemoveLocation(ov)
endif
endfunction
function cP takes nothing returns boolean
return(GetSpellAbilityId()=='A0PZ')
endfunction
function CP takes nothing returns nothing
call CameraClearNoiseForPlayer(GetOwningPlayer(GetTriggerUnit()))
endfunction
function DP takes nothing returns boolean
return(GetRandomInt(1,$A)==5)
endfunction
function fP takes nothing returns nothing
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A004')
call RemoveLocation(ov)
set ov=GetUnitLoc(GetTriggerUnit())
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,GetUnitLoc(GetTriggerUnit()))
call RemoveLocation(ov)
endfunction
function gP takes nothing returns boolean
return(GetRandomInt(1,$A)==5)
endfunction
