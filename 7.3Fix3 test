function p7 takes nothing returns nothing
if(M7())then
call CameraSetEQNoiseForPlayer(GetOwningPlayer(GetAttacker()),8.)
call SetUnitManaBJ(GetAttacker(),.0)
call IssueImmediateOrderById(GetAttacker(),$D005C)
set ov=GetUnitLoc(GetAttacker())
set Xe=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0KO',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0270,Xe)
call RemoveLocation(ov)
call RemoveLocation(Xe)
if(m7())then
call StartSound(gx[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))])
endif
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
function q7 takes nothing returns boolean
return(GetSpellAbilityId()=='A0NI')
endfunction
function Q7 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(GetTriggerUnit())))
endfunction
function s7 takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),(4750000.+((GetUnitStateSwap(UNIT_STATE_MAX_LIFE,GetEnumUnit())-GetUnitStateSwap(UNIT_STATE_LIFE,GetEnumUnit()))*.05)),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endfunction
function S7 takes nothing returns nothing
call CameraClearNoiseForPlayer(GetOwningPlayer(GetTriggerUnit()))
set ov=GetSpellTargetLoc()
call CreateNUnitsAtLoA(1,'h09A',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetTriggerUnit()))
call SetUnitScalePercent(bj_lastCreatedUnit,300.,300.,300.)
call SetUnitTimeScale(bj_lastCreatedUnit,1.5)
call CreateNUnitsAtLoA(1,'h0KI',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call SetUnitTimeScale(bj_lastCreatedUnit,3.8)
set Xe=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0KP',GetOwningPlayer(GetTriggerUnit()),Xe,GetUnitFacing(GetTriggerUnit()))
call CreateNUnitsAtLoA(1,'h0K8',GetOwningPlayer(GetTriggerUnit()),Xe,GetUnitFacing(GetTriggerUnit()))
call CreateNUnitsAtLoA(1,'h0CU',GetOwningPlayer(GetTriggerUnit()),Xe,GetUnitFacing(GetTriggerUnit()))
call SetUnitScalePercent(bj_lastCreatedUnit,270.,270.,270.)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(800.,ov,Condition(function Q7)),function s7)
call RemoveLocation(ov)
call RemoveLocation(Xe)
endfunction
function T7 takes nothing returns boolean
return(GetRandomInt(1,5)==5)
