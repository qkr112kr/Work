return(GetRandomInt(1,25)==5)
endfunction
function y7 takes nothing returns nothing
if(u7())then
if(T7())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0EK',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
call CreateNUnitsAtLoA(1,'h0G2',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call SetUnitTimeScale(bj_lastCreatedUnit,2.2)
set Re=GetUnitLoc(GetTriggerUnit())
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,Re)
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=3
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set Xe=Dd(ov,(275.*I2R(bj_forLoopBIndex)),GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0G3',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(GetAttacker()))
call RemoveLocation(Xe)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
call RemoveLocation(Re)
call RemoveLocation(ov)
else
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0EK',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueTargetOrderById(bj_lastCreatedUnit,$D0101,GetAttacker())
set Re=GetUnitLoc(GetTriggerUnit())
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,Re)
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=3
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set Xe=Dd(ov,(310.*I2R(bj_forLoopBIndex)),GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0EQ',GetOwningPlayer(GetAttacker()),Xe,GetUnitFacing(GetAttacker()))
call SetUnitAnimation(bj_lastCreatedUnit,"birth")
call SetUnitTimeScale(bj_lastCreatedUnit,1.75)
call RemoveLocation(Xe)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
call RemoveLocation(Re)
call RemoveLocation(ov)
endif
endif
if(W7())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0DH',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0EO',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D0080)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(520.,ov,Condition(function U7)),function w7)
call RemoveLocation(ov)
endif
endfunction
