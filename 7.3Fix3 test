call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
set Xe=Dd(ov,-65.,GetUnitFacing(GetAttacker()))
call lf(GetAttacker(),GetTriggerUnit(),12000.,1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call SetUnitPositionLoc(GetAttacker(),Xe)
call IssueImmediateOrderById(GetAttacker(),$D0278)
call RemoveLocation(Xe)
call RemoveLocation(ov)
endfunction
