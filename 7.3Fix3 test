if(KKv())then
call RemoveUnit(GroupPickRandomUnit(hd(ke[Je],Condition(function Kfv))))
call RemoveUnit(GroupPickRandomUnit(hd(ke[Je],Condition(function KFv))))
call RemoveUnit(GroupPickRandomUnit(hd(ke[Je],Condition(function Kgv))))
call RemoveUnit(GroupPickRandomUnit(hd(ke[Je],Condition(function KGv))))
call CreateNUnitsAtLoA(2,'h0I3',Player(-1+(Je)),Fe,bj_UNIT_FACING)
call DisplayTimedTextToForce(pd(Player(-1+(Je))),5.,"|cffffd700(!)|r |cffff0000타임어택에 실패하여 다음 2라운드간 랜덤위습을 받지 못합니다!|r")
