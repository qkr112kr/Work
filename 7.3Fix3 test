call CreateNUnitsAtLoA(1,'h0GI',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,Re)
call RemoveLocation(Re)
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=3
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set Xe=Dd(ov,(250.*I2R(bj_forLoopBIndex)),GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h051',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call RemoveLocation(Xe)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
call RemoveLocation(ov)
