call CreateNUnitsAtLoA(1,'h0AS',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D011D)
call CreateNUnitsAtLoA(1,'h07N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
if(LT())then
call AdjustPlayerStateBJ(40,GetOwningPlayer(GetAttacker()),PLAYER_STATE_RESOURCE_GOLD)
endif
endfunction
