call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@시야공유|r |cff646eff(숫)|r |cff646464 해당 플레이어 시야 공유|r  |cff288C28@시야공유ALL|r |cff646464 모든 플레이어 시야 공유|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@텔|r |cff646464 지정한 장소로 선택 영웅 1회 텔레포트|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@텔ON|r |cff646464 지정한 장소로 선택 영웅 텔레포트|r  |cff288C28@텔OFF|r |cff646464 텔레포트 능력 해제|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@맵핵ON|r |cff646464 맵핵 가동|r  |cff288C28@맵핵OFF|r |cff646464 맵핵 가동 중단|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@무적ON|r |cff646464 선택 유닛 무적|r  |cff288C28@무적OFF|r |cff646464 선택 유닛 무적 해제|r"+" "))
elseif fi2i==4 then
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@점수삭제|r |cff646464 플레이어의 점수 제거|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@게임금지 (숫)|r |cff646464 플레이어 게임금지|r  |cff288C28@게임금지해제 (숫)|r |cff646464 플레이어 게임금지해제|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@인구무제한ON|r |cff646464 인구 무제한|r  |cff288C28@인구무제한OFF|r |cff646464 인구 다시 사용|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@무한힐ON|r |cff646464 선택 영웅 체/마나 소모안됨|r  |cff288C28@무한힐OFF|r |cff646464 무한 힐 능력 해제|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@무한쿨탐ON|r |cff646464 선택 유닛 쿨타임 제거|r  |cff288C28@무한쿨탐OFF|r |cff646464 무한 쿨타임 해제|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@목록 (숫)|r |cff646464 전 아이템 목록을 볼수있음|r  |cff288C28@목록생성 (숫)|r |cff646464 목록에 있는 아이템 생성|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@칸목록 (숫)|r - |cffffA500|r |cff646464 목록(숫) 에 있는 아이템 15개 생성.|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@부분생성 (아이템이름)|r - |cffffA500|r |cff646464 EX) @부분생성 검 // 검으로시작되는 아이템 전부 생성|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@전체쿨탐on,off|r - |cffffA500|r |cff646464 적.아군 플레이어의  쿨타임을 제로로만듭니다 (지속)|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@그룹쿨탐ON|r |cff646464 모든 유닛 쿨탐 무한|r  |cff288C28@그룹쿨탐OFF|r |cff646464 모든 유닛 쿨탐 유한|r"+" "))
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cff288C28@그룹체젠|r |cff646eff(숫)|r |cff646464 모든 유닛 체젠 추가|r  |cff288C28@그룹마젠|r |cff646eff(숫)|r |cff646464 모든 유닛 마젠 추가|r"+" "))
