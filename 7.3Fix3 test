endfunction
function dfv takes nothing returns boolean
return(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function ddv)))>=1)and(CountUnitsInGroup(hd(bj_mapInitialPlayableArea,Condition(function dDv)))>=1)and(GetPlayerState(GetOwningPlayer(GetTriggerUnit()),PLAYER_STATE_RESOURCE_LUMBER)>=3)
endfunction
function dFv takes nothing returns nothing
if(dfv())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set ie[bj_forLoopAIndex]=GetPlayerName(Player(-1+(bj_forLoopAIndex)))
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ae[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set Re=GetUnitLoc(GetTriggerUnit())
if(dBv())then
call CreateNUnitsAtLoA(1,'h068',GetOwningPlayer(GetTriggerUnit()),Re,bj_UNIT_FACING)
call AddSpecialEffectTargetUnitBJ("chest",bj_lastCreatedUnit,"YH2.mdx")
else
call CreateNUnitsAtLoA(1,'h068',GetOwningPlayer(GetTriggerUnit()),Re,bj_UNIT_FACING)
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000카르가라 : |r|cffFA8072샨도라의 등불을 밝혀라 !!|r")
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function dcv))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function dCv))))
call AdjustPlayerStateBJ(-3,GetOwningPlayer(GetTriggerUnit()),PLAYER_STATE_RESOURCE_LUMBER)
call RemoveLocation(Re)
call TriggerSleepAction(.01)
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerName(Player(-1+(bj_forLoopAIndex)),ie[bj_forLoopAIndex])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
endfunction
