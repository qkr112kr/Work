function MP takes nothing returns nothing
if(mP())then
set ov=GetUnitLoc(GetTriggerUnit())
call lf(GetAttacker(),GetTriggerUnit(),3000.,.8,1.5,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call CreateNUnitsAtLoA(1,'h00S',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call SetUnitTimeScalePercent(bj_lastCreatedUnit,.0)
call CreateNUnitsAtLoA(1,'h00S',GetOwningPlayer(GetAttacker()),ov,120.)
call SetUnitTimeScalePercent(bj_lastCreatedUnit,.0)
call CreateNUnitsAtLoA(1,'h00S',GetOwningPlayer(GetAttacker()),ov,240.)
call SetUnitTimeScalePercent(bj_lastCreatedUnit,.0)
call RemoveLocation(ov)
endif
endfunction
