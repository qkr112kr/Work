endfunction
function C6 takes nothing returns nothing
if(c6())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0B4',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0KA')
call IssueTargetOrderById(bj_lastCreatedUnit,$D007F,GetTriggerUnit())
set Xe=Dd(ov,300.,GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0B4',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0KB')
call IssueTargetOrderById(bj_lastCreatedUnit,$D007F,GetTriggerUnit())
call RemoveLocation(Xe)
set Xe=Dd(ov,300.,GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0B4',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0KC')
call IssueTargetOrderById(bj_lastCreatedUnit,$D007F,GetTriggerUnit())
call RemoveLocation(Xe)
call RemoveLocation(ov)
call TriggerRegisterUnitEvent(nI,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
call EnableTrigger(nI)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
