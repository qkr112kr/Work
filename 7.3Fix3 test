function GT_UpgradeComplete_Func001 takes integer techid returns boolean
return((techid=='R004')or(techid=='R005')or(techid=='R00I')or(techid=='R00X')or(techid=='R02F')or(techid=='R01V')or(techid=='R02E')or(techid=='R02D')or(techid=='R00K')or(techid=='R00N')or(techid=='R010')or(techid=='R00P')or(techid=='R01U')or(techid=='R00E')or(techid=='R00S')or(techid=='R02G')or(techid=='R00O')or(techid=='R015')or(techid=='R00Z')or(techid=='R01E')or(techid=='R01C')or(techid=='R01B')or(techid=='R01D')or(techid=='R018')or(techid=='R01A')or(techid=='R003'))
endfunction
function GT_UpgradeComplete_Func002 takes integer techid returns boolean
return((techid=='R004')or(techid=='R005')or(techid=='R00I')or(techid=='R00X')or(techid=='R02F')or(techid=='R01V')or(techid=='R02E')or(techid=='R02D')or(techid=='R00K')or(techid=='R00N')or(techid=='R010')or(techid=='R00P')or(techid=='R01U')or(techid=='R00E')or(techid=='R00S')or(techid=='R02G')or(techid=='R00O')or(techid=='R015')or(techid=='R00Z')or(techid=='R01E')or(techid=='R01C')or(techid=='R01B')or(techid=='R01D')or(techid=='R018')or(techid=='R01A')or(techid=='R003')or(techid=='R01K')or(techid=='R01L')or(techid=='R007')or(techid=='R01M'))
endfunction
function GT_Start_Func001 takes nothing returns boolean
return(GetPlayerSlotState(Player(-1+(bj_forLoopAIndex)))==PLAYER_SLOT_STATE_PLAYING)and(GetPlayerController(Player(-1+(bj_forLoopAIndex)))!=MAP_CONTROL_COMPUTER)
endfunction
function GTDalg_Select_Actions takes nothing returns nothing
local integer techid='R000'
if(GTDalg_Effect_W2())then
if(C2_W2())then
set C_W2=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 위습 2배 모드를 ON 하셨습니다.")
else
set C_W2=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 위습 2배 모드를 OFF 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_W4())then
if(C2_W4())then
set C_W4=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 위습 4배 모드를 ON 하셨습니다.")
else
set C_W4=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 위습 4배 모드를 OFF 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_Gold())then
if(C2_Gold())then
set C_Gold=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 돈무한 모드를 ON 하셨습니다.")
else
set C_Gold=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 돈무한 모드를 OFF 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_Lotto())then
if(C2_Lotto())then
set C_Lotto=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 로또 모드를 ON 하셨습니다.")
else
set C_Lotto=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 로또 모드를 OFF 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_Skip())then
if(C2_Skip())then
set C_Skip=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 스킵 모드를 ON 하셨습니다.")
else
set C_Skip=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 스킵 모드를 OFF 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_Upgrade())then
if(C2_Upgrade())then
set C_Upgrade=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 업글 맥스 모드를 ON 하셨습니다.")
else
set C_Upgrade=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 업글 맥스 모드를 OFF 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_Upgrade1())then
if(C2_Upgrade1())then
set C_Upgrade1=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 업글 맥스 모드(히든제외)를 ON 하셨습니다.")
else
set C_Upgrade1=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 업글 맥스 모드(히든제외)를 OFF 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_Upgrade2())then
if(C2_Upgrade2())then
set C_Upgrade2=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 갓적선 모드를 ON 하셨습니다.(해적선이 하나씩 지급됩니다.)")
else
set C_Upgrade2=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 갓적선 모드를 ON 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_Upgrade3())then
if(C2_Upgrade3())then
set C_Upgrade3=true
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF0080"+"방장이 쿠마 2배 모드를 ON 하셨습니다.")
else
set C_Upgrade3=false
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFF0080FF"+"방장이 쿠마 2배 모드를 OFF 하셨습니다.")
endif
call TriggerExecute(GTDalg_Creats_T)
else
if(GTDalg_Effect_Complete())then
if(C3_W2())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(GT_Start_Func001())then
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoc(5*GT_WSP,'ewsp',Player(-1+(bj_forLoopAIndex)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set GT_WSP=2
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF8000"+"처음에 받지못한 위습 5마리를 지급합니다.")
endif
if(C3_W4())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(GT_Start_Func001())then
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoc(15*GT_WSP,'ewsp',Player(-1+(bj_forLoopAIndex)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set GT_WSP=4
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF8000"+"처음에 받지못한 위습 15마리를 지급합니다.")
endif
if(C3_Gold())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=7
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerStateBJ(Player(-1+(bj_forLoopAIndex)),PLAYER_STATE_RESOURCE_GOLD,10000000)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
if(C3_Lotto())then
call EnableTrigger(trigger002)
call EnableTrigger(trigger003)
call EnableTrigger(trigger004)
call EnableTrigger(trigger006)
call EnableTrigger(trigger007)
call PolledWait(5.)
call TriggerExecute(trigger005)
else
call DisableTrigger(trigger002)
call DisableTrigger(trigger003)
call DisableTrigger(trigger004)
call DisableTrigger(trigger005)
call DisableTrigger(trigger006)
call DisableTrigger(trigger007)
endif
if(C3_Skip())then
call EnableTrigger(GTDiag_Skip_T)
else
call DisableTrigger(GTDiag_Skip_T)
endif
if(C3_Upgrade())then
loop
exitwhen techid>'R034'
if(not(GT_UpgradeComplete_Func001(techid)))then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerTechResearched(Player(-1+bj_forLoopAIndex),techid,GetPlayerTechCount(Player(-1+(bj_forLoopAIndex)),techid,true)+30)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
if(techid=='R009')then
set techid='R00A'
elseif(techid=='R00Z')then
set techid='R010'
elseif(techid=='R019')then
set techid='R01A'
elseif(techid=='R01Z')then
set techid='R020'
elseif(techid=='R029')then
set techid='R02A'
elseif(techid=='R02Z')then
set techid='R030'
else
set techid=techid+1
endif
endloop
endif
if(C3_Upgrade1())then
loop
exitwhen techid>'R03Z'
if(not(GT_UpgradeComplete_Func002(techid)))then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerTechResearched(Player(-1+bj_forLoopAIndex),techid,GetPlayerTechCount(Player(-1+(bj_forLoopAIndex)),techid,true)+30)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
if(techid=='R009')then
set techid='R00A'
elseif(techid=='R00Z')then
set techid='R010'
elseif(techid=='R019')then
set techid='R01A'
elseif(techid=='R01Z')then
set techid='R020'
elseif(techid=='R029')then
set techid='R02A'
elseif(techid=='R02Z')then
set techid='R030'
elseif(techid=='R039')then
set techid='R03A'
else
set techid=techid+1
endif
endloop
endif
if(C3_Upgrade2())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(GT_Start_Func001())then
call CreateNUnitsAtLoA(1,'h02J',Player(-1+bj_forLoopAIndex),Qe,bj_UNIT_FACING)
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000 해적선이 스토리에 생성 되었습니다.|r")
endif
if(C3_Upgrade3())then
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000 초월 쿠마를 뽑을시 2마리가 생성됩니다.|r")
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20,"|c00ff1700설|r|c00ff2e00정|r|c00ff4600이|r|c00ff5d00 |r|c00ff7400완|r|c00ff8b00료|r|c00ffa200되|r|c00ffb900었|r|c00ffd100습|r|c00ffe800니|r|c00ffff00다.|r")
call DestroyTrigger(GTDalg_Creats_T)
endif
endif
endif
endif
endif
endif
endif
endif
endif
endif
endfunction
function InitTrig_GTES_T takes nothing returns nothing
set GTDiag_Select_T=CreateTrigger()
call TriggerRegisterDialogEvent(GTDiag_Select_T,GT_Dalg_Base)
call TriggerAddAction(GTDiag_Select_T,function GTDalg_Select_Actions)
endfunction
function GTDalg_Skip_Actions takes nothing returns nothing
if U<=30 then
call StartTimerBJ(ce[1],false,.01)
call StartTimerBJ(T,false,.01)
else
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20,"|c00ff1700 31라운드 이상은 사용이 불가능합니다.|r")
endif
endfunction
function InitTrig_GTSC_T takes nothing returns nothing
set GTDiag_Skip_T=CreateTrigger()
call DisableTrigger(GTDiag_Skip_T)
call TriggerRegisterPlayerChatEvent(GTDiag_Skip_T,Player(0),"-스킵",true)
call TriggerAddAction(GTDiag_Skip_T,function GTDalg_Skip_Actions)
endfunction
function GT_Display_Actions takes nothing returns nothing
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFE1FFE1"+"-----------------------------------------------"))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFF79FFCE"+"갯힝의 치트 에디션에 오신것을 환영합니다."))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFE1FFE1"+"이 치트 모드는 갯힝이 만들었습니다."))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFE1FFE1"+"▽ 현재 치트 모드에 있는 치트 목록 ▽ "))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFE1FFE1"+"위습 2배 / 위습 4배 / 돈무한 / 로또 / 갓적선 / 쿠마 2배 / 스킵 / 업글 맥스"))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFE1FFE1"+"방장만 설정가능하오니 서로 타협하여 모드를 선택하여 주세요."))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFE1FFE1"+"기회는 한번 뿐이니 신중하게 해주시길 바랍니다."))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFE1FFE1"+"시작하려면 '-치트모드'를 입력하세요"))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFE1FFE1"+"------------------------------------------------"))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFF4AAAA"+"제작자 블로그 http://cheatmap.tistory.com"))
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20.,("|CFFF4AAAA"+"플레이어 여러분의 행운을 기원합니다."))
endfunction
function InitTrig_GTDD_T takes nothing returns nothing
set GTDiag_Display_T=CreateTrigger()
call TriggerRegisterTimerEventSingle(GTDiag_Display_T,3.)
call TriggerAddAction(GTDiag_Display_T,function GT_Display_Actions)
endfunction
function ND2P takes player toPlayer,real x,real y,real duration,string message returns nothing
if(DIALOG3[GetPlayerId(toPlayer)]==true)then
call DisplayTimedTextToPlayer(toPlayer,x,y,duration,message)
endif
endfunction
function Getting_CheatEvent takes integer i2i,trigger trig,player p2p returns nothing
set Selected_Check[(20*i2i)+GetPlayerId(p2p)]=p2p
if CheatActionCheck[(20*i2i)+GetPlayerId(p2p)]!=p2p then
set CheatActionCheck[(20*i2i)+GetPlayerId(p2p)]=p2p
call TriggerRegisterPlayerUnitEvent(trig,p2p,EVENT_PLAYER_UNIT_SELECTED,null)
endif
endfunction
function Getting_CheatActions takes integer i2i,trigger whichTrigger,code actionFunc returns nothing
if CheatActionCheck[i2i]!=ConvertedPlayer(i2i)then
set CheatActionCheck[i2i]=ConvertedPlayer(i2i)
call TriggerAddAction(whichTrigger,actionFunc)
endif
endfunction
function ONnOFF takes string s2s returns integer
local integer fi2i=0
if((SubStringBJ(s2s,1,2)=="ON")or(SubStringBJ(s2s,1,2)=="on")or(SubStringBJ(s2s,1,3)=="온"))then
set fi2i=31
return fi2i
elseif((SubStringBJ(s2s,1,3)=="OFF")or(SubStringBJ(s2s,1,3)=="off")or(SubStringBJ(s2s,1,6)=="오프"))then
set fi2i=63
return fi2i
elseif((SubStringBJ(s2s,1,3)=="초"))then
set fi2i=3
return fi2i
elseif(SubStringBJ(s2s,1,3)=="ALL")or(SubStringBJ(s2s,1,3)=="all")or(SubStringBJ(s2s,1,3)=="올")then
set fi2i=4
return fi2i
else
set fi2i=S2I(SubStringBJ(s2s,1,7))
return fi2i
endif
endfunction
function KickcheatP takes player p returns nothing
if GetLocalPlayer()==p then
call DisplayTextToPlayer(p,0,0,GetPlayerName(Player(-1)))
endif
endfunction
function ItemRangeClear takes nothing returns nothing
call RemoveItem(GetEnumItem())
endfunction
function NS2U takes player fp2p,string message returns unit
if(udg_rcUnit[GetPlayerId(fp2p)]==null)then
call ND2P(fp2p,0,0,5.00,("|cffff0000주의! 선택된 유닛이 없습니다.|r"+""))
return null
elseif(IsUnitType(udg_rcUnit[GetPlayerId(fp2p)],UNIT_TYPE_HERO)==false and udg_rcUnit[GetPlayerId(fp2p)]!=null)then
call ND2P(fp2p,0,0,5.00,(("|cffff4500"+"현재 "+"|cffffbe0a선택|r"+"되어있는 유닛은"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(fp2p)]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(fp2p)])+"|r |cff2c952c이(가)|r |cffffbe0a선택|r|cff2c952c되어 있습니다.|r")))
call ND2P(fp2p,0,0,5.00,("|cffff0000주의! 선택된 유닛은 영웅이 아닙니다.|r"+""))
return udg_rcUnit[GetPlayerId(fp2p)]
else
call ND2P(fp2p,0,0,5.00,message)
return udg_rcUnit[GetPlayerId(fp2p)]
endif
endfunction
function NS3U takes player fp2p,integer fi2i,string message,string message2,string message3 returns unit
if(udg_rcUnit[GetPlayerId(fp2p)]==null)then
call ND2P(fp2p,0,0,5.00,("|cffff0000주의! 선택된 유닛이 없습니다.|r"+""))
return null
elseif(IsUnitType(udg_rcUnit[GetPlayerId(fp2p)],UNIT_TYPE_HERO)==false)then
call ND2P(fp2p,0,0,5.00,("|cffff0000주의! 선택된 유닛은 영웅이 아닙니다.|r"+""))
return udg_rcUnit[GetPlayerId(fp2p)]
elseif fi2i==31 then
call ND2P(fp2p,0,0,5.00,message)
return udg_rcUnit[GetPlayerId(fp2p)]
elseif fi2i==63 then
call ND2P(fp2p,0,0,5.00,message2)
return udg_rcUnit[GetPlayerId(fp2p)]
else
call ND2P(fp2p,0,0,5.00,message3)
return udg_rcUnit[GetPlayerId(fp2p)]
endif
endfunction
function Getting_SpecialList takes integer i2i returns boolean
local player p2p=Player(i2i)
return(GetPlayerName(p2p)=="qkr112kr")
endfunction
function Cheat_Start_Setting takes nothing returns nothing
if(GetClickedButtonBJ()==DIALOG2[1])then
if(GT_Privilege[GetPlayerId(GetTriggerPlayer())]>50)then
set DIALOG3[GetPlayerId(GetTriggerPlayer())]=true
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cffFF0000당신은 특권자 입니다!|r"))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("|r*|r |cffFFB1BCThanks to used |cffFFFA78Getting Cheat Maps Welcome! |r|r*"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("              |cffEF904C불만있으면 블로그로 찾아와라 씨발놈아 킄킄..닝겐.. 어리석다|r"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("1 . |cffCD0000Cafe Address|r |cffEE84EA갯힝's 티스토리 블로그|r -|cff6482FF http://cheatmap.tistory.com|r"+" "))
else
set DIALOG3[GetPlayerId(GetTriggerPlayer())]=true
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("|r*|r |cffFFB1BCThanks to used |cffFFFA78Getting Cheat Maps Welcome! |r|r*"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("              |cffEF904C갯힝의 카페 목록|r"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("1 . |cffCD0000Cafe Address|r |cffEE84EA갯힝's 네이버 카페|r -|cff6482FF http://cafe.naver.com/wsmc|r"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("2 . |cffCD0000Cafe Address|r |cffEE84EA갯힝's 네이버 블로그|r -|cff6482FF http://blog.naver.com/zzzwnsn|r"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("3 . |cffCD0000Cafe Address|r |cffEE84EA갯힝's 티스토리 블로그|r -|cff6482FF http://cheatmap.tistory.com|r"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("4 . |cffCD0000Cafe Address|r |cffEE84EA딩지's 티스토리 블로그|r -|cff6482FF http://cheatfactory.tistory.com|r"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("5 . |cffCD0000자동으로 리플레이가|r |cffEE84EA삭제 됩니다|r |cff6482FF"+" "))
endif
else
if(GetClickedButtonBJ()==DIALOG2[2])then
set DIALOG3[GetPlayerId(GetTriggerPlayer())]=false
endif
endif
endfunction
function Getting_Select takes nothing returns nothing
if Selected_Check[(20*1)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if Player(GetPlayerId(GetOwningPlayer(GetTriggerUnit())))==Player(GetPlayerId(GetTriggerPlayer()))then
set udg_rcUnit[GetPlayerId(GetTriggerPlayer())]=GetTriggerUnit()
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c이(가)|r |cffffbe0a선택|r|cff2c952c되었습니다.|r")))
call TriggerRegisterUnitEvent(REDUCE,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
call TriggerRegisterUnitEvent(BLOOD,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
else
if GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>=GT_Privilege[GetPlayerId(GetTriggerPlayer())]then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 해당 유닛은 특권 치트 소유자의 유닛입니다. 당신은 선택할 수 없습니다.|r"+""))
else
set udg_rcUnit[GetPlayerId(GetTriggerPlayer())]=GetTriggerUnit()
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c이(가)|r |cffffbe0a선택|r|cff2c952c되었습니다.|r")))
call TriggerRegisterUnitEvent(REDUCE,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
call TriggerRegisterUnitEvent(BLOOD,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
endif
endif
endif
set Selected_Check[(20*1)+GetPlayerId(GetTriggerPlayer())]=null
endfunction
function Getting_hyphen takes integer hyphen returns string
local integer hyphen2=1
local string hyphen3=""
loop
exitwhen hyphen2>hyphen
set hyphen3=hyphen3+"-"
set hyphen2=hyphen2+1
endloop
return hyphen3
endfunction
function Getting_at takes integer fat returns string
local integer fat2=1
local string fat3=""
loop
exitwhen fat2>fat
set fat3=fat3+"A"
set fat2=fat2+1
endloop
return fat3
endfunction
function Getting_hyphenInsert takes string inst returns string
return inst+Getting_at(Before_Time)+Getting_hyphen(After_Time)
endfunction
function Getting_atInsert takes string inst returns string
return inst+Getting_at(Before_Time)
endfunction
function Getting_Shadow2 takes nothing returns boolean
return(IsUnitIllusionBJ(GetFilterUnit())and Selected_Check[(20*24)+EndlessI]==GetLocalPlayer())
endfunction
function Getting_Shadow3 takes nothing returns nothing
call SetUnitVertexColorBJ(GetEnumUnit(),10.,38.,100,.0)
endfunction
function Getting_Regen1 takes nothing returns boolean
return(GetFilterUnit()!=udg_rcUnit[REEndlessI])
endfunction
function Getting_GroupCoolTime1 takes nothing returns boolean
return(GetFilterUnit()!=udg_rcUnit[CUnitC[100+EndlessI]])
endfunction
function Getting_GroupCoolTime2 takes nothing returns nothing
call UnitResetCooldown(GetEnumUnit())
endfunction
function Getting_Regen2 takes nothing returns nothing
call SetUnitLifeBJ(GetEnumUnit(),(GetUnitStateSwap(UNIT_STATE_LIFE,GetEnumUnit())+EndlessR[40+REEndlessI]))
call SetUnitManaBJ(GetEnumUnit(),(GetUnitStateSwap(UNIT_STATE_MANA,GetEnumUnit())+EndlessR[60+REEndlessI]))
endfunction
function Getting_REEndless takes nothing returns nothing
set bj_wantDestroyGroup=true
set REEndlessI=0
loop
exitwhen REEndlessI>11
call SetUnitLifeBJ(udg_rcUnit[REEndlessI],(GetUnitStateSwap(UNIT_STATE_LIFE,udg_rcUnit[REEndlessI])+EndlessR[REEndlessI]))
call SetUnitManaBJ(udg_rcUnit[REEndlessI],(GetUnitStateSwap(UNIT_STATE_MANA,udg_rcUnit[REEndlessI])+EndlessR[20+REEndlessI]))
call ForGroupBJ(GetUnitsOfPlayerMatching(udg_rcPlayer[REEndlessI],Condition(function Getting_Regen1)),function Getting_Regen2)
set REEndlessI=REEndlessI+1
endloop
endfunction
function Getting_Endless takes nothing returns nothing
set bj_wantDestroyGroup=true
set EndlessI=0
loop
exitwhen EndlessI>11
call UnitResetCooldown(udg_rcUnit[CUnitC[EndlessI]])
call SetPlayerStateBJ(udg_rcPlayer[CUnitC[20+EndlessI]],PLAYER_STATE_RESOURCE_FOOD_USED,0)
call SetPlayerStateBJ(udg_rcPlayer[CUnitC[20+EndlessI]],PLAYER_STATE_RESOURCE_FOOD_CAP,GetPlayerState(udg_rcPlayer[CUnitC[20+EndlessI]],PLAYER_STATE_FOOD_CAP_CEILING))
call SetUnitLifePercentBJ(udg_rcUnit[CUnitC[40+EndlessI]],100)
call SetUnitManaPercentBJ(udg_rcUnit[CUnitC[60+EndlessI]],100)
call SetUnitMoveSpeed(udg_rcUnit[CUnitC[80+EndlessI]],EndlessR[80+CUnitC[80+EndlessI]])
call ForGroupBJ(GetUnitsInRectMatching(GetPlayableMapRect(),Condition(function Getting_Shadow2)),function Getting_Shadow3)
call ForGroupBJ(GetUnitsOfPlayerMatching(udg_rcPlayer[CUnitC[100+EndlessI]],Condition(function Getting_GroupCoolTime1)),function Getting_GroupCoolTime2)
set EndlessI=EndlessI+1
endloop
endfunction
function Getting_Shaodw takes integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*24)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
call ND2P(GetTriggerPlayer(),0,0,5.,("|cffffbe0a분신|r|cff2c952c을 |cffff0000구별|r |cff2c952c해줍니다.|r |cffff0000(다른 플레이어는 구분되지 않습니다.)|r"+" "))
call EnableTrigger(ENDLESS)
elseif fi2i==63 then
set Selected_Check[(20*24)+GetPlayerId(GetTriggerPlayer())]=null
call ND2P(GetTriggerPlayer(),0,0,5.,("|cffffbe0a분신 구별|r|cff2c952c을|r |cffff0000중단|r |cff2c952c합니다.|r"+" "))
endif
endfunction
function Getting_Speed takes integer fi2i returns nothing
local player fp2p=GetTriggerPlayer()
if fi2i==3 then
set CUnitC[80+GetPlayerId(fp2p)]=100
call SetUnitMoveSpeed(NS2U(fp2p,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(fp2p)]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(fp2p)])+("|r |cff2c952c의|r |cffffbe0a이동 속도|r|cff2c952c를|r |cffff0000초기값 ("+(I2S(R2I(GetUnitDefaultMoveSpeed(udg_rcUnit[GetPlayerId(fp2p)])))+")|r |cff2c952c(으)로 변경합니다.|r"))))),GetUnitDefaultMoveSpeed(udg_rcUnit[GetPlayerId(fp2p)]))
else
call ND2P(fp2p,0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(fp2p)]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(fp2p)])+("|r |cff2c952c의|r |cffffbe0a이동 속도|r|cff2c952c를|r |cffff0000"+I2S(fi2i)+"|r |cff2c952c로 변경합니다.|r"))))
set EndlessR[80+GetPlayerId(fp2p)]=fi2i
set CUnitC[80+GetPlayerId(fp2p)]=GetPlayerId(fp2p)
call EnableTrigger(ENDLESS)
endif
endfunction
function Getting_Healing takes unit fu2u,integer fi2i returns nothing
if fi2i<=1 then
call SetUnitLifePercentBJ(fu2u,100)
call SetUnitManaPercentBJ(fu2u,100)
else
call SetUnitLifePercentBJ(fu2u,fi2i)
call SetUnitManaPercentBJ(fu2u,fi2i)
endif
endfunction
function Getting_Check takes nothing returns nothing
local integer i2i=0
loop
exitwhen i2i>11
if GT_Privilege[i2i]>=99 then
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+" |cff0000ff(특권 치트 사용자)|r")))))))))))))
elseif GT_Privilege[i2i]>=50 then
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+" |cffff0000(치트 사용자)|r")))))))))))))
elseif GT_Privilege[i2i]>=25 then
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+" |cffff4500(제한 치트 사용자)|r")))))))))))))
elseif GT_Privilege[i2i]==0 then
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+" |cff808000(치트 금지자)|r")))))))))))))
else
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+"")))))))))))))
endif
set i2i=i2i+1
endloop
endfunction
function Getting_GivingCheat takes integer fi2i returns nothing
set fi2i=fi2i-1
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<=49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
if udg_rcPlayer[fi2i]!=Player(fi2i)then
set udg_rcPlayer[fi2i]=Player(fi2i)
set GT_Privilege[fi2i]=25
call ForceAddPlayer(CHEATER,Player(fi2i))
call TriggerRegisterPlayerChatEvent(CHEATS,Player(fi2i),"",false)
set DIALOG3[fi2i]=true
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000"+(GetPlayerName(Player(fi2i))+"|r |cff2c952c에게|r |cffffbe0a치트 권한|r|cff2c952c을 주었습니다.|r")))
call ND2P(Player(fi2i),0,0,15.00,("|cffff0000>>>|r |cff646464갯힝|r |cff646eff의|r |cff288C28치트맵입니다.|r |cffffa500고마우면 블로그좀들려주셈|r |cffff0000<<<|r"+""))
call ND2P(Player(fi2i),0,0,5.00,("|cffff0000"+(GetPlayerName(GetTriggerPlayer())+"|r |cff2c952c님께서 당신에게|r |cffffbe0a치트 권한|r|cff2c952c을 주었습니다.|r |cffff0000(주의! 모든 치트를 사용할 수는 없습니다.)|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(Player(fi2i))+"는 이미 치트 권한을 가지고 있습니다.|r")))
endif
endif
endfunction
function Getting_BanCheat takes nothing returns nothing
local integer i2i=0
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<=49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
loop
exitwhen i2i>11
if(GT_Privilege[i2i]>=GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(Player(i2i))+"는 이미 치트를 사용하고 있습니다. 당신과 동등한 권한 또는 상위 권한을 가지고 있기 때문에 빼앗지 못합니다.|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cff"+Player_Color[i2i]+(GetPlayerName(Player(i2i))+"|r |cff2c952c가|r |cffffbe0a치트|r|cff2c952c를 사용하지 못하게 했습니다.|r")))
call ForceRemovePlayer(CHEATER,Player(i2i))
set GT_Privilege[i2i]=0
endif
set i2i=i2i+1
endloop
endif
endfunction
function Getting_Explosion takes nothing returns nothing
local integer i2i=0
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<=49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
loop
exitwhen i2i>11
if(GT_Privilege[i2i]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(Player(i2i),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+" 이(가) 이 방을 방폭하려고 시도합니다.|r")))
return
endif
set i2i=i2i+1
endloop
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cff646eff이 방은|r |cffffbe0a3초 후|r|cff646eff에 저절로 |cffffbe0a폭파|r|cff646eff됩니다.|r |cffff0000(취소따윈 못합니다. 이미 늦었어!)|r"+""))
call PolledWait(1.00)
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000 3 |r"))
call PolledWait(1.00)
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000 2 |r"))
call PolledWait(1.00)
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000 1 |r"))
call PolledWait(1.00)
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000Game Over!!|r"+""))
call PolledWait(1.00)
call EndGame(false)
call ClearTextMessagesBJ(GetPlayersAll())
endif
endfunction
