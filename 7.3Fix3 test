endfunction
function HU takes nothing returns nothing
call RemoveUnit(GetEnumUnit())
endfunction
function jU takes nothing returns boolean
return(GetRandomInt(1,40)==5)and(GetUnitPointValue(GetTriggerUnit())<$C7)
endfunction
function JU takes nothing returns nothing
if(jU())then
call PlaySoundBJ(xa)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h02X',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D024C)
set xv=Kd(900.,ov,Condition(function hU))
set bj_wantDestroyGroup=true
call ForGroupBJ(xv,function HU)
call DestroyGroup(xv)
call CreateNUnitsAtLoA(1,'h02Y',GetOwningPlayer(GetAttacker()),GetUnitLoc(GetAttacker()),bj_UNIT_FACING)
call RemoveUnit(GetAttacker())
call RemoveLocation(ov)
endif
endfunction
function KU takes nothing returns boolean
return(IsPlayerEnemy(GetOwningPlayer(GetFilterUnit()),GetOwningPlayer(GetAttacker())))
endfunction
function lU takes nothing returns nothing
call lf(GetAttacker(),GetEnumUnit(),130000.,1.,2.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endfunction
function LU takes nothing returns boolean
return(GetRandomInt(1,$A)==5)
endfunction
function mU takes nothing returns boolean
return(GetUnitPointValue(GetTriggerUnit())<$C8)
endfunction
function MU takes nothing returns boolean
return(GetRandomInt(1,$C)==5)
endfunction
function pU takes nothing returns boolean
return(R2I(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker()))=='}')
endfunction
function PU takes nothing returns nothing
if(LU())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h0C4',GetOwningPlayer(GetAttacker()),ov,GetUnitFacing(GetAttacker()))
call CreateNUnitsAtLoA(1,'h0E5',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(450.,ov,Condition(function KU)),function lU)
call RemoveLocation(ov)
endif
if(MU())then
if(mU())then
set ov=GetUnitLoc(GetTriggerUnit())
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),(.25*GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())),ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call CreateNUnitsAtLocFacingLocBJ(1,'h0C5',GetOwningPlayer(GetAttacker()),ov,ov)
call RemoveLocation(ov)
else
call UnitDamageTargetBJ(GetAttacker(),GetTriggerUnit(),200000.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endif
endif
if(pU())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0LK')
call IssueImmediateOrderById(bj_lastCreatedUnit,$D026C)
call RemoveLocation(ov)
set ov=GetRectCenter(ai)
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0UT')
call IssueImmediateOrderById(bj_lastCreatedUnit,$D026C)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
function QU takes nothing returns boolean
return(GetRandomInt(1,8)==5)
endfunction
function sU takes nothing returns boolean
return(IsPlayerEnemy(GetOwningPlayer(GetFilterUnit()),GetOwningPlayer(GetAttacker())))
endfunction
function SU takes nothing returns nothing
call lf(GetAttacker(),GetEnumUnit(),180000.,1.,1.5,ATTACK_TYPE_NORMAL,DAMAGE_TYPE_UNIVERSAL)
endfunction
function tU takes nothing returns boolean
return(GetRandomInt(1,20)==5)
endfunction
function TU takes nothing returns nothing
if(QU())then
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLocFacingLocBJ(1,'h0CC',GetOwningPlayer(GetAttacker()),ov,ov)
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call RemoveLocation(ov)
endif
if(tU())then
set ov=GetUnitLoc(GetTriggerUnit())
set Xe=Dd(ov,350.,(GetUnitFacing(GetAttacker())+.0))
call SetUnitAnimation(bj_lastCreatedUnit,"attack slam")
call CreateNUnitsAtLoA(1,'h0CB',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+180.))
call CreateNUnitsAtLoA(1,'h0CF',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+180.))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(450.,ov,Condition(function sU)),function SU)
call RemoveLocation(Xe)
call RemoveLocation(ov)
endif
endfunction
function UU takes nothing returns boolean
return(GetRandomInt(1,$C)==5)
endfunction
function wU takes nothing returns boolean
return(GetRandomInt(1,9)==5)
endfunction
function WU takes nothing returns nothing
if(UU())then
set ov=GetUnitLoc(GetAttacker())
set Xe=Dd(ov,GetRandomReal(0,350.),GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0DN',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call UnitApplyTimedLife(bj_lastCreatedUnit,'Bmec',2)
call UnitAddAbility(bj_lastCreatedUnit,'A0NJ')
call IssueTargetOrderById(bj_lastCreatedUnit,$D0214,GetTriggerUnit())
call RemoveLocation(Xe)
set Xe=Dd(ov,GetRandomReal(0,350.),GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0DN',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call UnitApplyTimedLife(bj_lastCreatedUnit,'Bmec',2)
call UnitAddAbility(bj_lastCreatedUnit,'A0NL')
call IssueTargetOrderById(bj_lastCreatedUnit,$D0214,GetTriggerUnit())
call RemoveLocation(Xe)
set Xe=Dd(ov,GetRandomReal(0,350.),GetRandomReal(0,360))
call CreateNUnitsAtLoA(1,'h0DN',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call UnitApplyTimedLife(bj_lastCreatedUnit,'Bmec',2)
call UnitAddAbility(bj_lastCreatedUnit,'A0NK')
call IssueTargetOrderById(bj_lastCreatedUnit,$D0214,GetTriggerUnit())
call RemoveLocation(Xe)
call RemoveLocation(ov)
endif
if(wU())then
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0DP')
call IssueImmediateOrderById(bj_lastCreatedUnit,$D022E)
call RemoveLocation(ov)
endif
