function lf takes unit a,unit b,real c,real Lf,real mf,attacktype f,damagetype g returns nothing
local real r=GetRandomReal(Lf,mf)
local real r2=r*c
call UnitDamageTargetBJ(a,b,c,f,g)
set r=0
set r2=0
endfunction
function Mf takes nothing returns nothing
local integer Md=GetUnitTypeId(GetAttacker())
local integer pf=0
local integer i=1
if HaveSavedInteger(dv,Md,0)then
set pf=LoadInteger(dv,Md,0)
endif
loop
exitwhen(i>pf)
call ConditionalTriggerExecute(LoadTriggerHandle(dv,Md,i))
set i=i+1
endloop
endfunction
function Pf takes integer Md,trigger qf returns nothing
local integer pf=0
if HaveSavedInteger(dv,Md,0)then
set pf=LoadInteger(dv,Md,0)
endif
set pf=pf+1
call SaveTriggerHandle(dv,Md,pf,qf)
call SaveInteger(dv,Md,0,pf)
endfunction
function Qf takes real ld,location Ld,boolexpr Hd returns group
set bj_randomSubGroupGroup=CreateGroup()
call GroupEnumUnitsInRangeOfLoc(bj_randomSubGroupGroup,Ld,ld,Hd)
call DestroyBoolExpr(Hd)
return bj_randomSubGroupGroup
endfunction
function sf takes rect r,player kd returns group
set bj_randomSubGroupGroup=CreateGroup()
set bj_groupEnumOwningPlayer=kd
call GroupEnumUnitsInRect(bj_randomSubGroupGroup,r,filterGetUnitsInRectOfPlayer)
return bj_randomSubGroupGroup
endfunction
function Sf takes nothing returns nothing
local integer i=$C
local trigger tf=CreateTrigger()
loop
call TriggerRegisterPlayerUnitEvent(tf,Player(i),EVENT_PLAYER_UNIT_ATTACKED,null)
exitwhen i==0
set i=i-1
endloop
call TriggerAddAction(tf,function Mf)
set tf=null
set dv=InitHashtable()
call Pf('h00A',kV)
call Pf('h00D',KV)
call Pf('h00G',KV)
call Pf('h00F',KV)
call Pf('h00F',lV)
