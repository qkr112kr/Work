call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetTriggerUnit())
call CreateNUnitsAtLoA(1,'h00N',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
call UnitAddAbility(bj_lastCreatedUnit,'A0KF')
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D00FA,ov)
set bj_forLoopBIndex=1
set bj_forLoopBIndexEnd=6
loop
exitwhen bj_forLoopBIndex>bj_forLoopBIndexEnd
set Xe=Dd(ov,400.,(1.+(60.*I2R(bj_forLoopBIndex))))
call CreateNUnitsAtLoA(1,'h0B9',GetOwningPlayer(GetAttacker()),Xe,bj_UNIT_FACING)
call RemoveLocation(Xe)
call IssuePointOrderByIdLoc(bj_lastCreatedUnit,$D0012,ov)
set bj_forLoopBIndex=bj_forLoopBIndex+1
endloop
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
