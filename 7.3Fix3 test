function Getting_Teleport2 takes nothing returns nothing
if GetTriggerPlayer()==Selected_Check[(20*23)+GetPlayerId(GetTriggerPlayer())]then
if(GetIssuedOrderIdBJ()==String2OrderIdBJ("patrol"))then
call SetUnitPositionLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())],GetOrderPointLoc())
call SetUnitPositionLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())],GetUnitLoc(GetOrderTargetUnit()))
endif
if GetTriggerPlayer()==Selected_Check[(20*33)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*23)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*33)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
