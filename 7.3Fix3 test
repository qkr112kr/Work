function Fm takes nothing returns nothing
set U=(U+1)
set Y=(Y+1)
call StartTimerBJ(Ce,true,.6)
call DestroyTimerDialog(S)
call StartTimerBJ(T,false,49.)
call CreateTimerDialogBJ(T,("|cffFF0000다음레벨->|r"+("|cffFF0000"+(I2S((Y+1))+"|r"))))
set S=bj_lastCreatedTimerDialog
call TimerDialogDisplay(bj_lastCreatedTimerDialog,true)
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffff0000라운드가 끝났습니다.|r")
call StartTimerBJ(ce[1],false,24.)
call PauseTimerBJ(true,ce[6])
call TriggerExecute(ya)
call DisableTrigger(GetTriggeringTrigger())
call DestroyTrigger(GetTriggeringTrigger())
endfunction
