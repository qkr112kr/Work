function Ey takes nothing returns nothing
set ov=GetRectCenter(Po)
call CreateNUnitsAtLoA(1,'ewsp',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),5.,("|cffff00ff떼를 썼더니 !|r"+(GetUnitName(bj_lastCreatedUnit)+"를 얻었다!")))
if(ay())then
set ov=GetRectCenter(Fr)
call CreateNUnitsAtLoA(1,'e002',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),5.,("|cffff00ff추가로|r"+(GetUnitName(bj_lastCreatedUnit)+"를 얻었다!")))
endif
if(ny())then
set ov=GetRectCenter(gr)
call CreateNUnitsAtLoA(1,'e003',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),5.,("|cffff00ff추가로|r"+(GetUnitName(bj_lastCreatedUnit)+"를 얻었다!")))
endif
if(Vy())then
set ov=GetRectCenter(Gr)
call CreateNUnitsAtLoA(1,'e004',GetOwningPlayer(GetTriggerUnit()),ov,GetUnitFacing(GetTriggerUnit()))
call RemoveLocation(ov)
call DisplayTimedTextToForce(pd(GetOwningPlayer(GetTriggerUnit())),5.,("|cffff00ff추가로|r"+(GetUnitName(bj_lastCreatedUnit)+"를 얻었다!")))
endif
