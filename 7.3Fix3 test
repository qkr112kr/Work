function PY takes nothing returns nothing
if(pY())then
else
call SetUnitAbilityLevelSwapped('AItl',Ux[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))],(GetUnitAbilityLevelSwapped('S009',Ux[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))])+1))
call SetUnitAbilityLevelSwapped('S009',Ux[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))],(GetUnitAbilityLevelSwapped('S009',Ux[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))])+1))
if(MY())then
call UnitAddAbility(Ux[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))],'S01N')
else
if(mY())then
call UnitAddAbility(Ux[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))],'S01M')
else
if(LY())then
call IncUnitAbilityLevel(Ux[(1+GetPlayerId(GetOwningPlayer(GetKillingUnit())))],'A0U0')
endif
endif
endif
endif
endfunction
function QY takes nothing returns boolean
return(IsPlayerEnemy(GetOwningPlayer(GetFilterUnit()),GetOwningPlayer(GetAttacker())))
endfunction
function sY takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),(850000.+(50000.*I2R(GetUnitAbilityLevelSwapped('S009',Ux[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))])))),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_NORMAL)
endfunction
