function Vrv takes nothing returns nothing
if(Vov())then
set ov=GetUnitLoc(GetTriggerUnit())
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000카포네 갱 뱃지 : |r|cffFA8072내가 혼자인것 같나? |r")
call CreateNUnitsAtLoA(1,'h012',GetOwningPlayer(GetTriggerUnit()),ov,bj_UNIT_FACING)
call RemoveUnit(GetTriggerUnit())
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function n8v))))
call RemoveUnit(GroupPickRandomUnit(hd(bj_mapInitialPlayableArea,Condition(function n9v))))
call RemoveLocation(ov)
endif
