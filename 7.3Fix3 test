call SetUnitTimeScalePercent(bj_lastCreatedUnit,160.)
call CreateNUnitsAtLoA(1,'h0L1',GetOwningPlayer(GetAttacker()),ov,(GetUnitFacing(GetAttacker())+180.))
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(500.,ov,Condition(function QY)),function sY)
call lf(bj_lastCreatedUnit,GetTriggerUnit(),(850000.+(150000.*I2R(GetUnitAbilityLevelSwapped('S009',GetAttacker())))),1.,1.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
