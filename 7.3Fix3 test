function Rz takes nothing returns nothing
if(az())then
call SetUnitManaBJ(GetAttacker(),0)
set ov=GetUnitLoc(GetAttacker())
call CreateNUnitsAtLoA(1,'h0EH',GetOwningPlayer(GetAttacker()),ov,bj_UNIT_FACING)
