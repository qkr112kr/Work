set lo[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=.0
call SetUnitManaBJ(GetAttacker(),0)
set Ko[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetTriggerUnit()
set ov=GetUnitLoc(GetTriggerUnit())
set Jo[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))]=GetUnitLoc(Ko[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))])
call CreateNUnitsAtLoA(1,'h07N',GetOwningPlayer(GetAttacker()),ov,GetRandomReal(0,360))
call SetUnitScalePercent(bj_lastCreatedUnit,450.,450.,450.)
call CreateNUnitsAtLoA(1,'h0I6',GetOwningPlayer(GetAttacker()),ov,GetRandomReal(0,360))
call IssueImmediateOrderById(bj_lastCreatedUnit,$D009F)
call CameraSetEQNoiseForPlayer(GetOwningPlayer(GetAttacker()),25.)
call StartTimerBJ(jo[(1+GetPlayerId(GetOwningPlayer(GetAttacker())))],true,.12)
call RemoveLocation(ov)
else
call SetUnitManaBJ(GetAttacker(),(GetUnitStateSwap(UNIT_STATE_MANA,GetAttacker())+1))
endif
endfunction
function T5 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),Player(-1+(ko))))
endfunction
function u5 takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),300000.,1.,2.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_NORMAL)
endfunction
function U5 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),Player(-1+(ko))))
endfunction
function w5 takes nothing returns nothing
call lf(bj_lastCreatedUnit,GetEnumUnit(),1250000.,1.,2.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endfunction
function W5 takes nothing returns boolean
return(lo[ko]==9.)
endfunction
function y5 takes nothing returns nothing
set ko=1
loop
exitwhen ko>5
exitwhen GetExpiredTimer()==jo[ko]
set ko=ko+1
endloop
set lo[ko]=(lo[ko]+1)
if(W5())then
call CameraClearNoiseForPlayer(Player(-1+(ko)))
call CreateNUnitsAtLoA(1,'h0LS',Player(-1+(ko)),Jo[ko],(lo[ko]*50.))
call SetUnitScalePercent(bj_lastCreatedUnit,(450.+(50.*lo[ko])),'d','d')
call SetUnitTimeScale(bj_lastCreatedUnit,1.5)
call CreateNUnitsAtLoA(1,'h0LS',Player(-1+(ko)),Jo[ko],(lo[ko]*150.))
call SetUnitScalePercent(bj_lastCreatedUnit,(450.+(50.*lo[ko])),'d','d')
call SetUnitTimeScale(bj_lastCreatedUnit,1.5)
call CreateNUnitsAtLoA(1,'h0LS',Player(-1+(ko)),Jo[ko],(lo[ko]*75.))
call SetUnitScalePercent(bj_lastCreatedUnit,(450.+(50.*lo[ko])),'d','d')
call SetUnitTimeScale(bj_lastCreatedUnit,1.5)
set bj_wantDestroyGroup=true
call ForGroupBJ(Kd(850.,Jo[ko],Condition(function U5)),function w5)
call RemoveLocation(Jo[ko])
call PauseTimerBJ(true,jo[ko])
else
set bj_wantDestroyGroup=true
call CreateNUnitsAtLoA(1,'h0LS',Player(-1+(ko)),Jo[ko],(lo[ko]*47.))
call SetUnitScalePercent(bj_lastCreatedUnit,(450.+(50.*lo[ko])),'d','d')
call SetUnitTimeScale(bj_lastCreatedUnit,1.5)
call ForGroupBJ(Kd(600.,Jo[ko],Condition(function T5)),function u5)
endif
endfunction
function z5 takes nothing returns boolean
return(IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(GetAttacker())))
endfunction
function Z5 takes nothing returns nothing
call lf(GetAttacker(),GetEnumUnit(),380000.,1.,2.,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
endfunction
