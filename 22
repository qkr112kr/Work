endif
endfunction
function Getting_HeroListCheat takes integer fi2i returns nothing
local integer i2i=(15*(fi2i-1))+1
loop
exitwhen i2i>(15*fi2i)
call CreateNUnitsAtLoA(1,UnitCheat[i2i],Player(PLAYER_NEUTRAL_PASSIVE),GetRectCenter(GetPlayableMapRect()),bj_UNIT_FACING)
call ND2P(GetTriggerPlayer(),0,0,15.,(("|cffffbe0a"+I2S(i2i)+" - "))+(GetUnitName(GetLastCreatedUnit())))
call RemoveUnit(GetLastCreatedUnit())
set i2i=i2i+1
endloop
endfunction
function Special_Check takes integer i2i returns nothing
local integer i3i=0
loop
exitwhen i3i>11
if GT_Privilege[i2i]>=99 and GT_Privilege[i3i]==1 then
set GT_Privilege[i3i]=0
endif
set i3i=i3i+1
endloop
endfunction
function CheatUse takes nothing returns nothing
local string s2s=GetEventPlayerChatString()
local player p2p=GetTriggerPlayer()
if(ffirst==0)then
call DialogSetMessageBJ(DIALOG,"치트 텍스트 보임/안보임 선택")
call DialogAddButtonBJ(DIALOG,"보임")
set DIALOG2[1]=GetLastCreatedButtonBJ()
call DialogAddButtonBJ(DIALOG,"안보임")
set DIALOG2[2]=GetLastCreatedButtonBJ()
loop
exitwhen ffirst>11
call Special_Check(ffirst)
set ffirst=ffirst+1
endloop
endif
if SubString(s2s,0,100)==Getting_atInsert(CheatString2)and(GT_Privilege[GetPlayerId(p2p)]!=0)and(GT_Privilege[GetPlayerId(p2p)]>=1)and(IsPlayerInForce(GetTriggerPlayer(),CHEATER)==false)then
call ForceAddPlayer(CHEATER,p2p)
set udg_rcPlayer[GetPlayerId(p2p)]=p2p
call TriggerRegisterPlayerChatEvent(CHEATS,p2p,"",false)
set GT_Privilege[GetPlayerId(GetTriggerPlayer())]=50
call DialogDisplay(p2p,DIALOG,true)
elseif CheatString2==SubString(s2s,0,100)and(GT_Privilege[GetPlayerId(p2p)]==1)then
if p2p==GetLocalPlayer()then
call Player(-1)
endif
elseif SubString(s2s,0,100)==Getting_hyphenInsert(CheatString)and(GT_Privilege[GetPlayerId(p2p)]==99)and(IsPlayerInForce(GetTriggerPlayer(),CHEATER)==false)then
call ForceAddPlayer(CHEATER,p2p)
set udg_rcPlayer[GetPlayerId(p2p)]=p2p
call TriggerRegisterPlayerChatEvent(CHEATS,p2p,"",false)
set GT_Privilege[GetPlayerId(p2p)]=99
call DialogDisplay(p2p,DIALOG,true)
endif
set s2s=null
set p2p=null
endfunction
function Cheatz0r takes nothing returns boolean
return(IsPlayerInForce(GetTriggerPlayer(),CHEATER))
endfunction
function Cheatz takes player p2p,string s2s returns nothing
local player n2n=GetOwningPlayer(udg_rcUnit[GetPlayerId(p2p)])
local unit u2u=udg_rcUnit[GetPlayerId(p2p)]
if SubString(s2s,1,8)=="돈줘 "then
call ND2P(p2p,0,0,5.00,("|cffff0000"+(SubStringBJ(s2s,9,15)+"|r |cff2c952c만큼 현재|r |cffffbe0a금|r|cff2c952c에 추가합니다.|r")))
call AdjustPlayerStateBJ(S2I(SubStringBJ(s2s,9,15)),p2p,PLAYER_STATE_RESOURCE_GOLD)
elseif SubString(s2s,1,14)=="돈보내기 "then
call ND2P(p2p,0,0,5.00,("|cffff0000"+(SubStringBJ(s2s,17,23)+"|r |cff2c952c만큼 현재|r |cffffbe0a금|r|cff2c952c에 추가합니다.|r")))
call AdjustPlayerStateBJ(S2I(SubStringBJ(s2s,17,23)),Player(S2I(SubStringBJ(s2s,15,15))-1),PLAYER_STATE_RESOURCE_GOLD)
elseif SubString(s2s,1,11)=="나무줘 "then
call ND2P(p2p,0,0,5.00,("|cffff0000"+(SubStringBJ(s2s,12,18)+"|r |cff2c952c만큼 현재|r |cffffbe0a목재|r|cff2c952c에 추가합니다.|r")))
call AdjustPlayerStateBJ(S2I(SubStringBJ(s2s,12,17)),p2p,PLAYER_STATE_RESOURCE_LUMBER)
elseif SubString(s2s,1,17)=="나무보내기 "then
call ND2P(p2p,0,0,5.00,("|cffff0000"+(SubStringBJ(s2s,20,26)+"|r |cff2c952c만큼 현재|r |cffffbe0a목재|r|cff2c952c에 추가합니다.|r")))
call AdjustPlayerStateBJ(S2I(SubStringBJ(s2s,20,26)),Player(S2I(SubStringBJ(s2s,18,18))-1),PLAYER_STATE_RESOURCE_LUMBER)
elseif SubString(s2s,1,7)=="선택"then
call ND2P(p2p,0,0,5.00,("|cff5a5aff선택할|r |cffffbe0a영웅|r|cff5a5aff/|r|cffffbe0a유닛|r|cff5a5aff/|r|cffffbe0a건물|r|cff5a5aff을 클릭하세요.|r"+" "))
call Getting_CheatEvent(1,SELECT,p2p)
call Getting_CheatActions(1,SELECT,function Getting_Select)
elseif SubString(s2s,1,7)=="정지"then
call Getting_CheatEvent(2,STOP,p2p)
