function ND2P takes player toPlayer,real x,real y,real duration,string message returns nothing
if(DIALOG3[GetPlayerId(toPlayer)]==true)then
call DisplayTimedTextToPlayer(toPlayer,x,y,duration,message)
endif
endfunction
function Getting_CheatEvent takes integer i2i,trigger trig,player p2p returns nothing
set Selected_Check[(20*i2i)+GetPlayerId(p2p)]=p2p
if CheatActionCheck[(20*i2i)+GetPlayerId(p2p)]!=p2p then
set CheatActionCheck[(20*i2i)+GetPlayerId(p2p)]=p2p
call TriggerRegisterPlayerUnitEvent(trig,p2p,EVENT_PLAYER_UNIT_SELECTED,null)
endif
endfunction
function Getting_CheatActions takes integer i2i,trigger whichTrigger,code actionFunc returns nothing
if CheatActionCheck[i2i]!=ConvertedPlayer(i2i)then
set CheatActionCheck[i2i]=ConvertedPlayer(i2i)
call TriggerAddAction(whichTrigger,actionFunc)
endif
endfunction
function ONnOFF takes string s2s returns integer
local integer fi2i=0
if((SubStringBJ(s2s,1,2)=="ON")or(SubStringBJ(s2s,1,2)=="on")or(SubStringBJ(s2s,1,3)=="온"))then
set fi2i=31
return fi2i
elseif((SubStringBJ(s2s,1,3)=="OFF")or(SubStringBJ(s2s,1,3)=="off")or(SubStringBJ(s2s,1,6)=="오프"))then
set fi2i=63
return fi2i
elseif((SubStringBJ(s2s,1,3)=="초"))then
set fi2i=3
return fi2i
elseif(SubStringBJ(s2s,1,3)=="ALL")or(SubStringBJ(s2s,1,3)=="all")or(SubStringBJ(s2s,1,3)=="올")then
set fi2i=4
return fi2i
else
set fi2i=S2I(SubStringBJ(s2s,1,7))
return fi2i
endif
endfunction
function KickcheatP takes player p returns nothing
if GetLocalPlayer()==p then
call DisplayTextToPlayer(p,0,0,GetPlayerName(Player(-1)))
endif
endfunction
function ItemRangeClear takes nothing returns nothing
call RemoveItem(GetEnumItem())
endfunction
function NS2U takes player fp2p,string message returns unit
if(udg_rcUnit[GetPlayerId(fp2p)]==null)then
call ND2P(fp2p,0,0,5.00,("|cffff0000주의! 선택된 유닛이 없습니다.|r"+""))
return null
elseif(IsUnitType(udg_rcUnit[GetPlayerId(fp2p)],UNIT_TYPE_HERO)==false and udg_rcUnit[GetPlayerId(fp2p)]!=null)then
call ND2P(fp2p,0,0,5.00,(("|cffff4500"+"현재 "+"|cffffbe0a선택|r"+"되어있는 유닛은"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(fp2p)]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(fp2p)])+"|r |cff2c952c이(가)|r |cffffbe0a선택|r|cff2c952c되어 있습니다.|r")))
call ND2P(fp2p,0,0,5.00,("|cffff0000주의! 선택된 유닛은 영웅이 아닙니다.|r"+""))
return udg_rcUnit[GetPlayerId(fp2p)]
else
call ND2P(fp2p,0,0,5.00,message)
return udg_rcUnit[GetPlayerId(fp2p)]
endif
endfunction
function NS3U takes player fp2p,integer fi2i,string message,string message2,string message3 returns unit
if(udg_rcUnit[GetPlayerId(fp2p)]==null)then
call ND2P(fp2p,0,0,5.00,("|cffff0000주의! 선택된 유닛이 없습니다.|r"+""))
return null
elseif(IsUnitType(udg_rcUnit[GetPlayerId(fp2p)],UNIT_TYPE_HERO)==false)then
call ND2P(fp2p,0,0,5.00,("|cffff0000주의! 선택된 유닛은 영웅이 아닙니다.|r"+""))
return udg_rcUnit[GetPlayerId(fp2p)]
elseif fi2i==31 then
call ND2P(fp2p,0,0,5.00,message)
return udg_rcUnit[GetPlayerId(fp2p)]
elseif fi2i==63 then
call ND2P(fp2p,0,0,5.00,message2)
return udg_rcUnit[GetPlayerId(fp2p)]
else
call ND2P(fp2p,0,0,5.00,message3)
return udg_rcUnit[GetPlayerId(fp2p)]
endif
endfunction
function Getting_SpecialList takes integer i2i returns boolean
local player p2p=Player(i2i)
return(GetPlayerName(p2p)=="qkr112kr")
endfunction
function Cheat_Start_Setting takes nothing returns nothing
if(GetClickedButtonBJ()==DIALOG2[1])then
if(GT_Privilege[GetPlayerId(GetTriggerPlayer())]>50)then
set DIALOG3[GetPlayerId(GetTriggerPlayer())]=true
call ND2P(GetTriggerPlayer(),0,0,15.00,("|cffFF0000당신은 특권자 입니다!|r"))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("|r*|r |cffFFB1BCThanks to used |cffFFFA78YS Cheat Maps Welcome! |r|r*"+" "))
else
set DIALOG3[GetPlayerId(GetTriggerPlayer())]=true
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("|r*|r |cffFFB1BCThanks to used |cffFFFA78YS Cheat Maps Welcome! |r|r*"+" "))
call PolledWait(0.01)
call ND2P(GetTriggerPlayer(),0,0,15.00,("5 . |cffCD0000자동으로 리플레이가|r |cffEE84EA삭제 됩니다|r |cff6482FF"+" "))
endif
else
if(GetClickedButtonBJ()==DIALOG2[2])then
set DIALOG3[GetPlayerId(GetTriggerPlayer())]=false
endif
endif
endfunction
function Getting_Select takes nothing returns nothing
if Selected_Check[(20*1)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if Player(GetPlayerId(GetOwningPlayer(GetTriggerUnit())))==Player(GetPlayerId(GetTriggerPlayer()))then
set udg_rcUnit[GetPlayerId(GetTriggerPlayer())]=GetTriggerUnit()
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c이(가)|r |cffffbe0a선택|r|cff2c952c되었습니다.|r")))
call TriggerRegisterUnitEvent(REDUCE,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
call TriggerRegisterUnitEvent(BLOOD,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
else
if GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>=GT_Privilege[GetPlayerId(GetTriggerPlayer())]then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 해당 유닛은 특권 치트 소유자의 유닛입니다. 당신은 선택할 수 없습니다.|r"+""))
else
set udg_rcUnit[GetPlayerId(GetTriggerPlayer())]=GetTriggerUnit()
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c이(가)|r |cffffbe0a선택|r|cff2c952c되었습니다.|r")))
call TriggerRegisterUnitEvent(REDUCE,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
call TriggerRegisterUnitEvent(BLOOD,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
endif
endif
endif
set Selected_Check[(20*1)+GetPlayerId(GetTriggerPlayer())]=null
endfunction
function Getting_hyphen takes integer hyphen returns string
local integer hyphen2=1
local string hyphen3=""
loop
exitwhen hyphen2>hyphen
set hyphen3=hyphen3+"-"
set hyphen2=hyphen2+1
endloop
return hyphen3
endfunction
function Getting_at takes integer fat returns string
local integer fat2=1
local string fat3=""
loop
exitwhen fat2>fat
set fat3=fat3+"A"
set fat2=fat2+1
endloop
return fat3
endfunction
function Getting_hyphenInsert takes string inst returns string
return inst+Getting_at(Before_Time)+Getting_hyphen(After_Time)
endfunction
function Getting_atInsert takes string inst returns string
return inst+Getting_at(Before_Time)
endfunction
function Getting_Shadow2 takes nothing returns boolean
return(IsUnitIllusionBJ(GetFilterUnit())and Selected_Check[(20*24)+EndlessI]==GetLocalPlayer())
endfunction
function Getting_Shadow3 takes nothing returns nothing
call SetUnitVertexColorBJ(GetEnumUnit(),10.,38.,100,.0)
endfunction
function Getting_Regen1 takes nothing returns boolean
return(GetFilterUnit()!=udg_rcUnit[REEndlessI])
endfunction
function Getting_GroupCoolTime1 takes nothing returns boolean
return(GetFilterUnit()!=udg_rcUnit[CUnitC[100+EndlessI]])
endfunction
function Getting_GroupCoolTime2 takes nothing returns nothing
call UnitResetCooldown(GetEnumUnit())
endfunction
function Getting_Regen2 takes nothing returns nothing
call SetUnitLifeBJ(GetEnumUnit(),(GetUnitStateSwap(UNIT_STATE_LIFE,GetEnumUnit())+EndlessR[40+REEndlessI]))
call SetUnitManaBJ(GetEnumUnit(),(GetUnitStateSwap(UNIT_STATE_MANA,GetEnumUnit())+EndlessR[60+REEndlessI]))
endfunction
function Getting_REEndless takes nothing returns nothing
set bj_wantDestroyGroup=true
set REEndlessI=0
loop
exitwhen REEndlessI>11
call SetUnitLifeBJ(udg_rcUnit[REEndlessI],(GetUnitStateSwap(UNIT_STATE_LIFE,udg_rcUnit[REEndlessI])+EndlessR[REEndlessI]))
call SetUnitManaBJ(udg_rcUnit[REEndlessI],(GetUnitStateSwap(UNIT_STATE_MANA,udg_rcUnit[REEndlessI])+EndlessR[20+REEndlessI]))
call ForGroupBJ(GetUnitsOfPlayerMatching(udg_rcPlayer[REEndlessI],Condition(function Getting_Regen1)),function Getting_Regen2)
set REEndlessI=REEndlessI+1
endloop
endfunction
function Getting_Endless takes nothing returns nothing
set bj_wantDestroyGroup=true
set EndlessI=0
loop
exitwhen EndlessI>11
call UnitResetCooldown(udg_rcUnit[CUnitC[EndlessI]])
call SetPlayerStateBJ(udg_rcPlayer[CUnitC[20+EndlessI]],PLAYER_STATE_RESOURCE_FOOD_USED,0)
call SetPlayerStateBJ(udg_rcPlayer[CUnitC[20+EndlessI]],PLAYER_STATE_RESOURCE_FOOD_CAP,GetPlayerState(udg_rcPlayer[CUnitC[20+EndlessI]],PLAYER_STATE_FOOD_CAP_CEILING))
call SetUnitLifePercentBJ(udg_rcUnit[CUnitC[40+EndlessI]],100)
call SetUnitManaPercentBJ(udg_rcUnit[CUnitC[60+EndlessI]],100)
call SetUnitMoveSpeed(udg_rcUnit[CUnitC[80+EndlessI]],EndlessR[80+CUnitC[80+EndlessI]])
call ForGroupBJ(GetUnitsInRectMatching(GetPlayableMapRect(),Condition(function Getting_Shadow2)),function Getting_Shadow3)
call ForGroupBJ(GetUnitsOfPlayerMatching(udg_rcPlayer[CUnitC[100+EndlessI]],Condition(function Getting_GroupCoolTime1)),function Getting_GroupCoolTime2)
set EndlessI=EndlessI+1
endloop
endfunction
function Getting_Shaodw takes integer fi2i returns nothing
if fi2i==31 then
