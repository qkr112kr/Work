set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=7
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerStateBJ(Player(-1+(bj_forLoopAIndex)),PLAYER_STATE_RESOURCE_GOLD,10000000)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
if(C3_Lotto())then
call EnableTrigger(trigger002)
call EnableTrigger(trigger003)
call EnableTrigger(trigger004)
call EnableTrigger(trigger006)
call EnableTrigger(trigger007)
call PolledWait(5.)
call TriggerExecute(trigger005)
else
