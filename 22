function Func0042 takes nothing returns boolean
return(((S2I(SubStringBJ(GetEventPlayerChatString(),7,8))<=2)and(S2I(SubStringBJ(GetEventPlayerChatString(),7,8))>=1)))
endfunction
function Func0046 takes nothing returns nothing
set integers004[(1+GetPlayerId(GetTriggerPlayer()))]=S2I(SubStringBJ(GetEventPlayerChatString(),7,8))
if(((S2I(SubStringBJ(GetEventPlayerChatString(),7,8))==1)))then
call DisplayTextToForce(Func0010(GetTriggerPlayer()),(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 원하는 경품번호 1번"))
call DisplayTextToForce(Func0010(GetTriggerPlayer()),(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 님 1번 경품은 나무 15개와 20000골드 입니다"))
else
if(((S2I(SubStringBJ(GetEventPlayerChatString(),7,8))==2)))then
call DisplayTextToForce(Func0010(GetTriggerPlayer()),(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 원하는 경품번호 2번"))
call DisplayTextToForce(Func0010(GetTriggerPlayer()),(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 님 2번 경품은 초월 랜덤 유닛 입니다"))
endif
endif
endfunction
