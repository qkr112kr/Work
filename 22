function Skilldam_Check takes nothing returns nothing
if GetAttacker()==udg_rcUnit[GetPlayerId(GetOwningPlayer(GetAttacker()))]then
set DIALOG3[60+GetPlayerId(GetOwningPlayer(GetAttacker()))]=true
endif
endfunction
function Getting_Skilldamage2 takes nothing returns nothing
local integer i2i=GetPlayerId(GetOwningPlayer(GetEventDamageSource()))
if DIALOG3[60+i2i]==true then
set DIALOG3[60+i2i]=false
elseif GetEventDamage()>0.00 and GetEventDamageSource()==udg_rcUnit[i2i]then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTarget(udg_rcUnit[i2i],GetTriggerUnit(),EndlessR[160+i2i],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_ENHANCED,WEAPON_TYPE_WHOKNOWS)
call EnableTrigger(GetTriggeringTrigger())
elseif GetOwningPlayer(GetEventDamageSource())==udg_rcPlayer[i2i]and DIALOG3[40+i2i]==true then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTarget(udg_rcUnit[i2i],GetTriggerUnit(),EndlessR[140+i2i],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_ENHANCED,WEAPON_TYPE_WHOKNOWS)
call EnableTrigger(GetTriggeringTrigger())
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function Skilldamage_Actions takes nothing returns nothing
local trigger t2t=CreateTrigger()
call TriggerRegisterUnitEvent(t2t,GetEnumUnit(),EVENT_UNIT_DAMAGED)
call TriggerRegisterAnyUnitEventBJ(t2t,EVENT_PLAYER_UNIT_SPELL_CHANNEL)
call TriggerAddAction(t2t,function Getting_Skilldamage2)
set t2t=null
endfunction
function Skilldamage_Condition takes nothing returns boolean
return(GetWidgetLife(GetFilterUnit())>0.405 and IsUnitType(GetFilterUnit(),UNIT_TYPE_MAGIC_IMMUNE)==false and GetUnitAbilityLevel(GetFilterUnit(),'Aloc')==0 and IsUnitEnemy(GetFilterUnit(),GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))==true)
endfunction
function Getting_Skilldamage takes nothing returns nothing
set bj_wantDestroyGroup=true
if GetTriggerUnit()==udg_rcUnit[GetPlayerId(GetTriggerPlayer())]then
call ForGroupBJ(GetUnitsInRangeOfLocMatching(2500.,GetUnitLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]),Condition(function Skilldamage_Condition)),function Skilldamage_Actions)
endif
endfunction
function Getting_SkillAddDamageCheat takes unit fu2u,integer fi2i returns nothing
set EndlessR[160+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(SKILLDAM)
call EnableTrigger(SKILLDAM2)
endfunction
function Getting_SkillAddDamageGroupCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==0 then
set DIALOG3[40+GetPlayerId(GetTriggerPlayer())]=false
else
set DIALOG3[40+GetPlayerId(GetTriggerPlayer())]=true
endif
set EndlessR[140+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(SKILLDAM)
call EnableTrigger(SKILLDAM2)
endfunction
function Getting_CopyCheat2 takes nothing returns nothing
local integer i2i=1
local integer i3i=1
local unit u2u=GetTriggerUnit()
if Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
loop
exitwhen i2i>CopyI[GetPlayerId(GetTriggerPlayer())]
call CreateNUnitsAtLoA(1,GetUnitTypeId(u2u),udg_rcPlayer[GetPlayerId(GetTriggerPlayer())],GetUnitLoc(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]),bj_UNIT_FACING)
call SetHeroLevelBJ(GetLastCreatedUnit(),GetHeroLevel(u2u),false)
call SetHeroStr(GetLastCreatedUnit(),GetHeroStatBJ(bj_HEROSTAT_STR,u2u,false),true)
call SetHeroAgi(GetLastCreatedUnit(),GetHeroStatBJ(bj_HEROSTAT_AGI,u2u,false),true)
call SetHeroInt(GetLastCreatedUnit(),GetHeroStatBJ(bj_HEROSTAT_INT,u2u,false),true)
loop
exitwhen i3i>6
call UnitAddItemByIdSwapped(GetItemTypeId(UnitItemInSlotBJ(u2u,i3i)),GetLastCreatedUnit())
call SetItemCharges(GetLastCreatedItem(),GetItemCharges(UnitItemInSlotBJ(u2u,i3i)))
call UnitDropItemSlotBJ(GetLastCreatedUnit(),GetLastCreatedItem(),i3i)
set i3i=i3i+1
endloop
set i2i=i2i+1
endloop
call ND2P(GetTriggerPlayer(),0,0,5.00,((("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c의|r |cffff0000"))+GetUnitName(GetTriggerUnit()))+(("|r |cff2c952c을(를)|r |cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+("|r |cff2c952c의 옆에|r |cffff0000"+(I2S(CopyI[GetPlayerId(GetTriggerPlayer())])+"기|r|cff2c952c만큼|r |cffffbe0a복제|r|cff2c952c했습니다.|r"))))))
if GetTriggerPlayer()==Selected_Check[(20*17)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*17)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_CopyCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set CopyI[GetPlayerId(GetTriggerPlayer())]=1
elseif fi2i==63 then
set Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]=null
else
set Selected_Check[(20*7)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set Selected_Check[(20*17)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
if fi2i==0 then
set CopyI[GetPlayerId(GetTriggerPlayer())]=1
else
set CopyI[GetPlayerId(GetTriggerPlayer())]=fi2i
endif
endif
endfunction
