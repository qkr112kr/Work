call TriggerRegisterPlayerChatEvent(trigger002,Player(4),"번호 ",false)
call TriggerRegisterPlayerChatEvent(trigger002,Player(5),"번호 ",false)
call TriggerRegisterPlayerChatEvent(trigger002,Player(6),"번호 ",false)
call TriggerAddCondition(trigger002,Condition(function Func0023))
call TriggerAddAction(trigger002,function Func0024)
endfunction
function Func0025 takes nothing returns boolean
return(booleans002[(1+GetPlayerId(GetTriggerPlayer()))]==false)
endfunction
function Func0026 takes nothing returns nothing
set forces002[(1+GetPlayerId(GetTriggerPlayer()))]=Func0010(GetTriggerPlayer())
if(((booleans002[(1+GetPlayerId(GetTriggerPlayer()))]==false)))then
set booleans002[(1+GetPlayerId(GetTriggerPlayer()))]=true
call DisplayTextToForce(forces002[(1+GetPlayerId(GetTriggerPlayer()))],(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 자동번호 ON"))
call DisplayTextToForce(forces002[(1+GetPlayerId(GetTriggerPlayer()))],(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 다시한번 입력하면 자동번호가 종료됩니다."))
else
set booleans002[(1+GetPlayerId(GetTriggerPlayer()))]=false
call DisplayTextToForce(forces002[(1+GetPlayerId(GetTriggerPlayer()))],(GetPlayerName(GetTriggerPlayer())+"|cffFF2118 자동번호 OFF"))
endif
endfunction
