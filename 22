call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c의|r |cffffbe0a쿨타임|r|cff2c952c을|r |cffffbe0a무한|r|cff2c952c으로|r |cffffbe0a초기화|r|cff2c952c시킵니다.|r")))
call EnableTrigger(ENDLESS)
elseif fi2i==63 then
set CUnitC[GetPlayerId(GetTriggerPlayer())]=100
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c의|r |cffffbe0a무한 쿨타임|r |cff2c952c능력을|r |cffffbe0a제거|r|cff2c952c합니다.|r")))
