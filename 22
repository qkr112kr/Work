function GTDalg_Create_Action takes nothing returns nothing
call DialogClearBJ(GT_Dalg_Base)
call DialogSetMessage(GT_Dalg_Base,"Cheat_Mode")
if(GT_W2())then
set GT_STR[1]="위습 2배 ON"
else
set GT_STR[1]="위습 2배 OFF"
endif
if(GT_W4())then
set GT_STR[2]="위습 4배 ON"
else
set GT_STR[2]="위습 4배 OFF"
endif
if(GT_Gold())then
set GT_STR[3]="돈무한 ON"
else
set GT_STR[3]="돈무한 OFF"
endif
if(GT_Lotto())then
set GT_STR[4]="로또 ON"
else
set GT_STR[4]="로또 OFF"
endif
if(GT_Upgrade2())then
set GT_STR[5]="갓적선 모드 ON (해적선 지급)"
else
set GT_STR[5]="갓적선 모드 OFF (해적선 지급)"
endif
if(GT_Upgrade3())then
set GT_STR[6]="쿠마 2배 ON"
else
set GT_STR[6]="쿠마 2배 OFF"
endif
if(GT_Skip())then
set GT_STR[7]="스킵 ON"
else
set GT_STR[7]="스킵 OFF"
endif
if(GT_Upgrade())then
set GT_STR[8]="업글 맥스 ON"
else
set GT_STR[8]="업글 맥스 OFF"
endif
if(GT_Upgrade1())then
set GT_STR[9]="업글 맥스 ON (히든 제외)"
else
set GT_STR[9]="업글 맥스 OFF (히든 제외)"
endif
set GT_STR[10]="완료"
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=10
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
set GT_Dalg_select[GetForLoopIndexA()]=DialogAddButtonBJ(GT_Dalg_Base,"* "+GT_STR[GetForLoopIndexA()])
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call DialogDisplayBJ(true,GT_Dalg_Base,Player(0))
endfunction
