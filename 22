function CreateNUnitsAtLoA takes integer count,integer unitId,player whichPlayer,location loc,real face returns group
if C_W2 or C_W4 then
if unitId=='e000' or unitId=='e001' or unitId=='e002' or unitId=='e003' or unitId=='e004' or unitId=='e005' or unitId=='ewsp' then
set count=count*GT_WSP
endif
endif
if C_Upgrade3 then
if unitId=='h05O' then
set count=2
endif
endif
call GroupClear(bj_lastCreatedGroup)
loop
set count=count-1
exitwhen count<0
call CreateUnitAtLocSaveLast(whichPlayer,unitId,loc,face)
call GroupAddUnit(bj_lastCreatedGroup,bj_lastCreatedUnit)
endloop
return bj_lastCreatedGroup
endfunction
function Trig_exp_ScoreSet_Actions2 takes nothing returns nothing
call DisplayTimedTextToPlayer(GetTriggerPlayer(),0,0,15.,("|CFF79FFCE"+"착하고 멋진 용수님이 "+(GetPlayerName(GetOwningPlayer(GetTriggerUnit())))+"에게 날개를 달아주마!"))
call AddSpecialEffectTargetUnitBJ("chest",GetTriggerUnit(),"YH2.mdx")
call DisableTrigger(GetTriggeringTrigger())
endfunction
function InitTrig_exp_ScoreSet2 takes nothing returns nothing
set SELECTA=CreateTrigger()
call DisableTrigger(SELECTA)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(0),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(1),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(2),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(3),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(4),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(5),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(6),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(7),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(8),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(9),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(10),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(11),true)
call TriggerRegisterPlayerSelectionEventBJ(SELECTA,Player(12),true)
call TriggerAddAction(SELECTA,function Trig_exp_ScoreSet_Actions2)
endfunction
function Func0010 takes player loc_player01 returns force
set force001=CreateForce()
call ForceAddPlayer(force001,loc_player01)
return force001
endfunction
function Func0023 takes nothing returns boolean
return(((S2I(SubStringBJ(GetEventPlayerChatString(),7,8))<=9)and(S2I(SubStringBJ(GetEventPlayerChatString(),7,8))>=0)))
endfunction
function Func0024 takes nothing returns nothing
