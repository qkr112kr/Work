function Getting_StopCheat2 takes nothing returns nothing
local integer i2i=0
if Selected_Check[(20*2)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
loop
exitwhen(StopCheatUnit[i2i]==null)
set i2i=i2i+1
endloop
if(IsUnitPausedBJ(GetTriggerUnit())==true)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 정지한 유닛입니다. 정지하지 않은 유닛을 선택해 주세요.|r"+" "))
else
if(GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다! 정지할 수 없습니다!|r")))
else
set StopCheatUnit[i2i]=GetTriggerUnit()
call PauseUnitBJ(true,GetTriggerUnit())
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+("|r |cff2c952c을(를) |cffffbe0a정지|r|cff2c952c시킵니다.|r"))))
endif
endif
if GetTriggerPlayer()==Selected_Check[(20*12)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*2)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*12)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_StopCheat takes unit fu2u,integer fi2i returns nothing
