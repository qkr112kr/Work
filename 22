else
set DIALOG3[20+GetPlayerId(GetTriggerPlayer())]=true
endif
set EndlessR[140+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(ADDDAMAGE)
endfunction
function Getting_ReduceDamageCheat2 takes nothing returns nothing
local integer i2i=0
loop
exitwhen i2i>11
if(GetTriggerUnit()==udg_rcUnit[i2i])then
call SetUnitLifeBJ(GetTriggerUnit(),(GetUnitStateSwap(UNIT_STATE_LIFE,GetTriggerUnit())+(GetEventDamage()*(EndlessR[120+i2i]*0.01))))
endif
set i2i=i2i+1
endloop
endfunction
