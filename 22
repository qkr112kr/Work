endfunction
function Getting_BloodCheat3 takes nothing returns nothing
local integer i2i=0
loop
exitwhen i2i>11
if(GetEventDamageSource()==udg_rcUnit[i2i]and GetEventDamage()>0.00)then
call SetUnitLifeBJ(GetEventDamageSource(),(GetUnitStateSwap(UNIT_STATE_LIFE,GetEventDamageSource())+(EndlessR[100+i2i]*GetEventDamage())))
endif
set i2i=i2i+1
endloop
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function Getting_BloodCheat2 takes nothing returns nothing
