call Getting_Teleport(NS3U(p2p,ONnOFF(SubString(s2s,4,100)),(("|cffff4500"+(GetPlayerName(n2n)+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(u2u)+"|r |cff2c952c이(가) 지정한 장소로|r |cffffbe0a텔레포트|r|cff2c952c합니다.|r |cffff0000(지속, 패트롤을 이용해서 사용하세요.)|r")),(("|cffff4500"+(GetPlayerName(n2n)+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(u2u)+"|r |cff2c952c의|r |cffffbe0a텔레포트|r|cff2c952c 능력을 |cffffbe0a제거|r|cff2c952c합니다.|r")),(("|cffff4500"+(GetPlayerName(n2n)+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(u2u)+"|r |cff2c952c이(가) 지정한 장소로|r |cffffbe0a텔레포트|r|cff2c952c합니다.|r |cffff0000(단발, 패트롤을 이용해서 사용하세요.)|r"))),ONnOFF(SubString(s2s,4,100)))
elseif SubString(s2s,1,7)=="맵핵"then
call Getting_MapCheat(ONnOFF(SubString(s2s,7,100)))
elseif SubString(s2s,1,8)=="추뎀 "then
call Getting_AddDamageCheat(NS2U(p2p,(("|cffff4500"+(GetPlayerName(n2n)+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(u2u)+("|r |cff2c952c의|r |cffffbe0a추가 데미지|r|cff2c952c를|r |cffff0000"+(SubString(s2s,8,16)+"|r |cff2c952c으로 변경합니다.|r"))))),ONnOFF(SubString(s2s,8,100)))
elseif SubString(s2s,1,14)=="그룹추뎀 "then
call Getting_AddDamageGroupCheat(NS2U(p2p,(("|cffff4500"+(GetPlayerName(udg_rcPlayer[GetPlayerId(p2p)])+"|r |cff2c952c님의|r |cffff0000"))+("모든 유닛"+("|r |cff2c952c의|r |cffffbe0a추가 데미지|r|cff2c952c를|r |cffff0000"+(SubStringBJ(s2s,15,99))+"|r |cff2c952c으로 변경합니다.|r")))),ONnOFF(SubString(s2s,14,100)))
elseif SubString(s2s,1,8)=="뎀감 "then
call Getting_ReduceDamageCheat(NS2U(p2p,(("|cffff4500"+(GetPlayerName(n2n)+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(u2u)+("|r |cff2c952c의|r |cffffbe0a추가 데미지 감소|r|cff2c952c를|r |cffff0000"+(SubString(s2s,8,16)+"%|r |cff2c952c로 변경합니다.|r"))))),ONnOFF(SubString(s2s,8,100)))
elseif SubString(s2s,1,8)=="흡혈 "then
call Getting_BloodCheat(NS2U(p2p,(("|cffff4500"+(GetPlayerName(n2n)+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(u2u)+("|r |cff2c952c의|r |cffffbe0a흡혈량|r|cff2c952c을|r |cffff0000"+(SubString(s2s,8,100))+"%|r |cff2c952c로 변경합니다.|r")))),ONnOFF(SubString(s2s,8,100)))
elseif SubString(s2s,1,8)=="스뎀 "then
call Getting_SkillAddDamageCheat(NS2U(p2p,(("|cffff4500"+(GetPlayerName(n2n)+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(u2u)+("|r |cff2c952c의|r |cffffbe0a스킬 추가 데미지|r|cff2c952c를|r |cffff0000"+(SubString(s2s,8,100))+"|r |cff2c952c으로 변경합니다.|r")))),ONnOFF(SubString(s2s,8,100)))
elseif SubString(s2s,1,14)=="그룹스뎀 "then
call Getting_SkillAddDamageGroupCheat(NS2U(p2p,(("|cffff4500"+(GetPlayerName(udg_rcPlayer[GetPlayerId(p2p)])+"|r |cff2c952c님의|r |cffff0000"))+("모든 유닛"+("|r |cff2c952c의|r |cffffbe0a공격 및 스킬 추가 데미지|r|cff2c952c를|r |cffff0000"+(SubStringBJ(s2s,15,100))+"|r |cff2c952c으로 변경합니다.|r")))),ONnOFF(SubString(s2s,14,100)))
elseif SubString(s2s,1,8)=="템중 "then
call Getting_ReiterationItemCheat(NS2U(p2p,((("|cffff4500"+(GetPlayerName(n2n)+"|r |cff2c952c님의|r |cffff0000"))+GetUnitName(u2u))+("|r |cff2c952c의|r |cffff0000"+(SubStringBJ(s2s,9,9)+("|r|cff2c952c번째 인벤토리의 아이템의|r |cffffbe0a중첩 수|r|cff2c952c를|r |cffff0000"+(SubStringBJ(s2s,11,14)+"|r|cff2c952c개로 변경했습니다.|r")))))),ONnOFF(SubString(s2s,7,100)),ONnOFF(SubString(s2s,10,100)))
elseif SubString(s2s,1,7)=="올템"then
call Getting_AllItemCheat(NS2U(p2p,("|cffDC6089모든 아이템을 생성합니다."+".")))
elseif SubString(s2s,1,10)=="템제거"then
call EnumItemsInRectBJ(RectFromCenterSizeBJ(GetUnitLoc(u2u),650.00,650.00),function ItemRangeClear)
elseif SubString(s2s,1,7)=="채팅"then
call Getting_ChatCheat(ONnOFF(SubString(s2s,7,100)),p2p)
elseif SubString(s2s,1,10)=="무한힐"then
call Getting_EndlessHealingCheat(ONnOFF(SubString(s2s,10,100)))
elseif SubString(s2s,1,13)=="무한쿨탐"then
call Getting_EndlessCooltimeCheat(ONnOFF(SubString(s2s,13,100)))
elseif SubString(s2s,1,14)=="원시생성 "then
call CreateItemLoc(udg_rcItemType[ONnOFF(SubString(s2s,14,100))-1],GetUnitLoc(NS2U(p2p,("|c00808080아이템을 생성하였습니다.|r"))))
elseif SubString(s2s,1,14)=="원시목록 "then
call Getting_PrimitiveCreateCheat(ONnOFF(SubString(s2s,14,100)))
elseif SubString(s2s,1,8)=="목록 "then
call Getting_WOLFList(ONnOFF(SubString(s2s,7,100)))
elseif SubString(s2s,1,14)=="목록생성 "then
call CreateItemLoc(CopyI[40+S2I(SubStringBJ(s2s,15,18))],GetUnitLoc(u2u))
call ND2P(p2p,0,0,7.,("|cffff4500"+(GetPlayerName(n2n)+("|r |cff2c952c님의|r |cffff0000"+(GetUnitName(u2u)+("|r |cff2c952c에게|r |cffffbe0a"+(Player_Color[20+S2I(SubStringBJ(s2s,15,17))]+"|r|cff2c952c을|r |cffff0000생성시켜|r |cff2c952c줍니다.")))))))
elseif SubString(s2s,1,11)=="칸목록 "then
call Getting_WOLFListCreate2(ONnOFF(SubString(s2s,10,100)))
elseif SubString(s2s,1,14)=="부분생성 "then
call Getting_WOLFListPart(s2s)
elseif SubString(s2s,1,8)=="생성 "then
call Getting_WOLFCreate(s2s)
elseif SubString(s2s,1,10)=="텍스트"then
call DialogDisplay(p2p,DIALOG,true)
elseif SubString(s2s,1,11)=="도움말 "then
call Getting_HelpCheat(ONnOFF(SubString(s2s,11,100)))
endif
set p2p=null
set n2n=null
set u2u=null
set s2s=""
endfunction
function Getting_Commander takes player fp2p,string fs2s returns nothing
local integer fi2i=StringLength(fs2s)
local integer fi3i=0
call Cheatz(fp2p,SubString(fs2s,fi3i,fi2i))
endfunction
function DirectCheat takes nothing returns nothing
local player p2p=GetTriggerPlayer()
local string s2s=GetEventPlayerChatString()
call Getting_Commander(p2p,s2s)
set p2p=null
set s2s=""
endfunction
function C2A takes string c returns integer
local integer i=0
local string str="0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"
local string t
loop
set t=SubString(str,i,i+1)
exitwhen t==null
exitwhen t==c
set i=i+1
endloop
if i<10 then
return i+48
elseif i<36 then
return i+55
endif
return i+61
endfunction
function S2A takes string s returns integer
if(StringLength(s)!=4)then
return 0
endif
return((C2A(SubString(s,0,1))*16777216)+(C2A(SubString(s,1,2))*65536)+(C2A(SubString(s,2,3))*256)+(C2A(SubString(s,3,4))))
endfunction
function GT_SourceCode takes nothing returns nothing
local string itemStr=""
local integer i=0
loop
exitwhen i>245
set udg_rcItemType[i]=S2A(SubString(itemStr,i*4,i*4+4))
set i=i+1
if ModuloInteger(i,30)==0 then
call TriggerSleepAction(0.01)
endif
endloop
endfunction
function Cheat_Setting takes nothing returns nothing
endfunction
function CheatInstall takes nothing returns nothing
local integer zzz=0
local integer i2i=0
loop
exitwhen i2i>220
set CUnitC[i2i]=100
set i2i=i2i+1
endloop
loop
exitwhen zzz>11
set GT_Privilege[zzz]=1
call TriggerRegisterPlayerChatEvent(ICHEAT,Player(zzz),"@",false)
call TriggerRegisterPlayerChatEvent(CHATTING,Player(zzz),"",false)
if Getting_SpecialList(zzz)==true then
set GT_Privilege[zzz]=99
endif
set zzz=zzz+1
endloop
set Player_Color[0]="FF0303"
set Player_Color[1]="0042FF"
