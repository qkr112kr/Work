set Selected_Check[(20*14)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endif
endfunction
function Getting_MapCheat takes integer fi2i returns nothing
if fi2i==31 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffffbe0a맵핵|r|cff2c952c을 |cffff0000가동|r |cff2c952c했습니다.|r |cffff0000(동맹은 켜지지 않습니다.)|r"+" "))
call CreateFogModifierRectBJ(true,Player(PLAYER_NEUTRAL_AGGRESSIVE),FOG_OF_WAR_VISIBLE,GetEntireMapRect())
call SetPlayerAllianceStateBJ(Player(PLAYER_NEUTRAL_AGGRESSIVE),GetTriggerPlayer(),bj_ALLIANCE_UNALLIED_VISION)
elseif fi2i==63 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffffbe0a맵핵|r|cff2c952c을|r |cffff0000중단|r |cff2c952c했습니다.|r"+" "))
call SetPlayerAllianceStateBJ(Player(PLAYER_NEUTRAL_AGGRESSIVE),GetTriggerPlayer(),bj_ALLIANCE_UNALLIED)
endif
endfunction
function Getting_AddAttack takes nothing returns nothing
local integer i2i=GetPlayerId(GetOwningPlayer(GetEventDamageSource()))
if GetEventDamageSource()==udg_rcUnit[i2i]and IsUnitIllusionBJ(GetEventDamageSource())!=true then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTarget(udg_rcUnit[i2i],GetTriggerUnit(),EndlessR[180+i2i],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_ENHANCED,WEAPON_TYPE_WHOKNOWS)
call EnableTrigger(GetTriggeringTrigger())
elseif udg_rcPlayer[i2i]==GetOwningPlayer(GetEventDamageSource())and DIALOG3[20+i2i]==true then
call DisableTrigger(GetTriggeringTrigger())
call UnitDamageTarget(GetEventDamageSource(),GetTriggerUnit(),EndlessR[140+i2i],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_ENHANCED,WEAPON_TYPE_WHOKNOWS)
call EnableTrigger(GetTriggeringTrigger())
endif
call DestroyTrigger(GetTriggeringTrigger())
endfunction
function Getting_BasicAttack takes nothing returns nothing
local trigger t2t=CreateTrigger()
call TriggerRegisterUnitEvent(t2t,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
call TriggerAddAction(t2t,function Getting_AddAttack)
set t2t=null
endfunction
function Getting_AddDamageCheat takes unit fu2u,integer fi2i returns nothing
set EndlessR[180+GetPlayerId(GetTriggerPlayer())]=fi2i
call EnableTrigger(ADDDAMAGE)
endfunction
function Getting_AddDamageGroupCheat takes unit fu2u,integer fi2i returns nothing
