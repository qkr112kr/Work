endif
endfunction
function Getting_Stopoff takes nothing returns nothing
local integer i2i=0
if(StopCheatUnit[i2i]!=null)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000"+("정지시킨 모든 유닛"+"|r |cff2c952c을 다시|r |cffffbe0a움직이게|r |cff2c952c합니다.|r")))
loop
exitwhen StopCheatUnit[i2i]==null
call PauseUnitBJ(false,StopCheatUnit[i2i])
set StopCheatUnit[i2i]=null
set i2i=i2i+1
endloop
else
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff0000"+"주의! 정지된 유닛이 없습니다.")+"|r"))
endif
endfunction
function Getting_DeleteCheat2 takes nothing returns nothing
if Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(GetTriggerUnit()==udg_rcUnit[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 치트 선택 유닛입니다! 삭제할 수 없습니다!|r")))
else
if(GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다! 정지할 수 없습니다!|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+"|r |cff2c952c을(를) 게임 상에서|r |cffffbe0a삭제|r|cff2c952c합니다.|r")))
call RemoveUnit(GetTriggerUnit())
endif
endif
if GetTriggerPlayer()==Selected_Check[(20*13)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*13)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_DeleteCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
set Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
elseif fi2i==63 then
set Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]=null
else
set Selected_Check[(20*3)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
set Selected_Check[(20*13)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endif
endfunction
function Getting_KillCheat2 takes nothing returns nothing
if Selected_Check[(20*4)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(GetTriggerUnit()==udg_rcUnit[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 치트 선택 유닛입니다! 살해할 수 없습니다. 자살을 이용하세요!|r")))
else
if GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())]then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다! 살해할 수 없습니다!|r")))
else
if(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]==null)then
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+"|r |cff2c952c을(를) 즉시|r |cffffbe0a살해|r|cff2c952c합니다.|r")))
call KillUnit(GetTriggerUnit())
else
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(GetTriggerUnit())+"|r |cff2c952c을(를) 즉시|r |cffffbe0a살해|r|cff2c952c합니다.|r |cffff0000( 선택 유닛이 있기에 돈과 경험치를 획득했습니다.)|r")))
call UnitDamageTargetBJ(udg_rcUnit[GetPlayerId(GetTriggerPlayer())],GetTriggerUnit(),1000000000.00,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
call KillUnit(GetTriggerUnit())
endif
endif
endif
if GetTriggerPlayer()==Selected_Check[(20*14)+GetPlayerId(GetTriggerPlayer())]then
set Selected_Check[(20*4)+GetPlayerId(GetTriggerPlayer())]=null
set Selected_Check[(20*14)+GetPlayerId(GetTriggerPlayer())]=null
endif
endif
endfunction
function Getting_KillCheat takes unit fu2u,integer fi2i returns nothing
if fi2i==31 then
