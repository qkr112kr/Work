endfunction
function Getting_GivingCheat takes integer fi2i returns nothing
set fi2i=fi2i-1
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<=49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
if udg_rcPlayer[fi2i]!=Player(fi2i)then
set udg_rcPlayer[fi2i]=Player(fi2i)
set GT_Privilege[fi2i]=25
call ForceAddPlayer(CHEATER,Player(fi2i))
call TriggerRegisterPlayerChatEvent(CHEATS,Player(fi2i),"",false)
set DIALOG3[fi2i]=true
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000"+(GetPlayerName(Player(fi2i))+"|r |cff2c952c에게|r |cffffbe0a치트 권한|r|cff2c952c을 주었습니다.|r")))
call ND2P(Player(fi2i),0,0,15.00,("|cffff0000>>>|r |cff646464YS|r |cff646eff의|r |cff288C28치트맵입니다.|r"+""))
call ND2P(Player(fi2i),0,0,5.00,("|cffff0000"+(GetPlayerName(GetTriggerPlayer())+"|r |cff2c952c님께서 당신에게|r |cffffbe0a치트 권한|r|cff2c952c을 주었습니다.|r |cffff0000(주의! 모든 치트를 사용할 수는 없습니다.)|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(Player(fi2i))+"는 이미 치트 권한을 가지고 있습니다.|r")))
endif
endif
endfunction
function Getting_BanCheat takes nothing returns nothing
local integer i2i=0
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<=49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
loop
exitwhen i2i>11
if(GT_Privilege[i2i]>=GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(Player(i2i))+"는 이미 치트를 사용하고 있습니다. 당신과 동등한 권한 또는 상위 권한을 가지고 있기 때문에 빼앗지 못합니다.|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cff"+Player_Color[i2i]+(GetPlayerName(Player(i2i))+"|r |cff2c952c가|r |cffffbe0a치트|r|cff2c952c를 사용하지 못하게 했습니다.|r")))
call ForceRemovePlayer(CHEATER,Player(i2i))
set GT_Privilege[i2i]=0
endif
set i2i=i2i+1
endloop
endif
endfunction
function Getting_Explosion takes nothing returns nothing
local integer i2i=0
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<=49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
loop
exitwhen i2i>11
if(GT_Privilege[i2i]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(Player(i2i),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+" 이(가) 이 방을 방폭하려고 시도합니다.|r")))
return
endif
set i2i=i2i+1
endloop
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cff646eff이 방은|r |cffffbe0a3초 후|r|cff646eff에 저절로 |cffffbe0a폭파|r|cff646eff됩니다.|r |cffff0000(취소따윈 못합니다. 이미 늦었어!)|r"+""))
call PolledWait(1.00)
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000 3 |r"))
call PolledWait(1.00)
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000 2 |r"))
call PolledWait(1.00)
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000 1 |r"))
call PolledWait(1.00)
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000Game Over!!|r"+""))
call PolledWait(1.00)
call EndGame(false)
call ClearTextMessagesBJ(GetPlayersAll())
endif
endfunction
