set ov=GetRectCenter(Po)
call CreateNUnitsAtLoc(15*GT_WSP,'ewsp',Player(-1+(bj_forLoopAIndex)),ov,bj_UNIT_FACING)
call RemoveLocation(ov)
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
set GT_WSP=4
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,30,"|CFFFF8000"+"처음에 받지못한 위습 15마리를 지급합니다.")
endif
