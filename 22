call EnableTrigger(ENDLESS)
elseif fi2i==63 then
set Selected_Check[(20*24)+GetPlayerId(GetTriggerPlayer())]=null
call ND2P(GetTriggerPlayer(),0,0,5.,("|cffffbe0a분신 구별|r|cff2c952c을|r |cffff0000중단|r |cff2c952c합니다.|r"+" "))
endif
endfunction
function Getting_Speed takes integer fi2i returns nothing
local player fp2p=GetTriggerPlayer()
if fi2i==3 then
set CUnitC[80+GetPlayerId(fp2p)]=100
call SetUnitMoveSpeed(NS2U(fp2p,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(fp2p)]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(fp2p)])+("|r |cff2c952c의|r |cffffbe0a이동 속도|r|cff2c952c를|r |cffff0000초기값 ("+(I2S(R2I(GetUnitDefaultMoveSpeed(udg_rcUnit[GetPlayerId(fp2p)])))+")|r |cff2c952c(으)로 변경합니다.|r"))))),GetUnitDefaultMoveSpeed(udg_rcUnit[GetPlayerId(fp2p)]))
else
call ND2P(fp2p,0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(fp2p)]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(fp2p)])+("|r |cff2c952c의|r |cffffbe0a이동 속도|r|cff2c952c를|r |cffff0000"+I2S(fi2i)+"|r |cff2c952c로 변경합니다.|r"))))
set EndlessR[80+GetPlayerId(fp2p)]=fi2i
set CUnitC[80+GetPlayerId(fp2p)]=GetPlayerId(fp2p)
call EnableTrigger(ENDLESS)
endif
endfunction
function Getting_Healing takes unit fu2u,integer fi2i returns nothing
if fi2i<=1 then
call SetUnitLifePercentBJ(fu2u,100)
call SetUnitManaPercentBJ(fu2u,100)
else
call SetUnitLifePercentBJ(fu2u,fi2i)
call SetUnitManaPercentBJ(fu2u,fi2i)
endif
endfunction
function Getting_Check takes nothing returns nothing
local integer i2i=0
loop
exitwhen i2i>11
if GT_Privilege[i2i]>=99 then
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+" |cff0000ff(특권 치트 사용자)|r")))))))))))))
elseif GT_Privilege[i2i]>=50 then
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+" |cffff0000(치트 사용자)|r")))))))))))))
elseif GT_Privilege[i2i]>=25 then
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+" |cffff4500(제한 치트 사용자)|r")))))))))))))
elseif GT_Privilege[i2i]==0 then
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+" |cff808000(치트 금지자)|r")))))))))))))
else
call ND2P(GetTriggerPlayer(),0,0,10.00,("|cff"+Player_Color[i2i]+(I2S(i2i+1)+(". "+(GetPlayerName(Player(i2i))+("|r - |cffffbe0a금 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_GOLD))+(", |cff2c952c나무 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_LUMBER))+(", |cff646eff유닛상황 :|r "+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_USED))+("|cff646464/|r"+(I2S(GetPlayerState(Player(i2i),PLAYER_STATE_RESOURCE_FOOD_CAP))+", |cff646eff점수 :|r "+(I2S(GT_Privilege[i2i]))+"")))))))))))))
