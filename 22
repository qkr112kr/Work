call DisableTrigger(trigger003)
call DisableTrigger(trigger004)
call DisableTrigger(trigger005)
call DisableTrigger(trigger006)
call DisableTrigger(trigger007)
endif
if(C3_Skip())then
call EnableTrigger(GTDiag_Skip_T)
else
call DisableTrigger(GTDiag_Skip_T)
endif
if(C3_Upgrade())then
loop
exitwhen techid>'R034'
if(not(GT_UpgradeComplete_Func001(techid)))then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerTechResearched(Player(-1+bj_forLoopAIndex),techid,GetPlayerTechCount(Player(-1+(bj_forLoopAIndex)),techid,true)+30)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
if(techid=='R009')then
set techid='R00A'
elseif(techid=='R00Z')then
set techid='R010'
elseif(techid=='R019')then
set techid='R01A'
elseif(techid=='R01Z')then
set techid='R020'
elseif(techid=='R029')then
set techid='R02A'
elseif(techid=='R02Z')then
set techid='R030'
else
set techid=techid+1
endif
endloop
endif
if(C3_Upgrade1())then
loop
exitwhen techid>'R03Z'
if(not(GT_UpgradeComplete_Func002(techid)))then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
call SetPlayerTechResearched(Player(-1+bj_forLoopAIndex),techid,GetPlayerTechCount(Player(-1+(bj_forLoopAIndex)),techid,true)+30)
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
endif
if(techid=='R009')then
set techid='R00A'
elseif(techid=='R00Z')then
set techid='R010'
elseif(techid=='R019')then
set techid='R01A'
elseif(techid=='R01Z')then
set techid='R020'
elseif(techid=='R029')then
set techid='R02A'
elseif(techid=='R02Z')then
set techid='R030'
elseif(techid=='R039')then
set techid='R03A'
else
set techid=techid+1
endif
endloop
endif
if(C3_Upgrade2())then
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=5
loop
exitwhen bj_forLoopAIndex>bj_forLoopAIndexEnd
if(GT_Start_Func001())then
call CreateNUnitsAtLoA(1,'h02J',Player(-1+bj_forLoopAIndex),Qe,bj_UNIT_FACING)
endif
set bj_forLoopAIndex=bj_forLoopAIndex+1
endloop
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000 해적선이 스토리에 생성 되었습니다.|r")
endif
if(C3_Upgrade3())then
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,10.,"|cffFF0000 초월 쿠마를 뽑을시 2마리가 생성됩니다.|r")
endif
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20,"|c00ff1700설|r|c00ff2e00정|r|c00ff4600이|r|c00ff5d00 |r|c00ff7400완|r|c00ff8b00료|r|c00ffa200되|r|c00ffb900었|r|c00ffd100습|r|c00ffe800니|r|c00ffff00다.|r")
call DestroyTrigger(GTDalg_Creats_T)
endif
endif
endif
endif
endif
endif
endif
endif
endif
endif
endfunction
function InitTrig_GTES_T takes nothing returns nothing
set GTDiag_Select_T=CreateTrigger()
call TriggerRegisterDialogEvent(GTDiag_Select_T,GT_Dalg_Base)
call TriggerAddAction(GTDiag_Select_T,function GTDalg_Select_Actions)
endfunction
function GTDalg_Skip_Actions takes nothing returns nothing
if U<=30 then
call StartTimerBJ(ce[1],false,.01)
call StartTimerBJ(T,false,.01)
else
call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS,20,"|c00ff1700 31라운드 이상은 사용이 불가능합니다.|r")
endif
endfunction
function InitTrig_GTSC_T takes nothing returns nothing
set GTDiag_Skip_T=CreateTrigger()
call DisableTrigger(GTDiag_Skip_T)
call TriggerRegisterPlayerChatEvent(GTDiag_Skip_T,Player(0),"-스킵",true)
call TriggerAddAction(GTDiag_Skip_T,function GTDalg_Skip_Actions)
endfunction
