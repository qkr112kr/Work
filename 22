function Getting_CopyItemCheat2 takes nothing returns nothing
local item array CopyItem
if Selected_Check[(20*9)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[GetPlayerId(GetTriggerPlayer())])==null)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 선택한 슬롯은 비어있습니다.|r"+""))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,((("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+GetUnitName(GetTriggerUnit()))+("|r |cff2c952c의|r |cffff0000"+(I2S(ItemCopyI[GetPlayerId(GetTriggerPlayer())])+"|r|cff2c952c번째 인벤토리의 아이템을|r |cffffbe0a복제|r|cff2c952c했습니다.|r"))))
set CopyItem[GetPlayerId(GetTriggerPlayer())]=UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[GetPlayerId(GetTriggerPlayer())])
call UnitAddItemByIdSwapped(GetItemTypeId(CopyItem[GetPlayerId(GetTriggerPlayer())]),udg_rcUnit[GetPlayerId(GetTriggerPlayer())])
call SetItemCharges(GetLastCreatedItem(),GetItemCharges(UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[GetPlayerId(GetTriggerPlayer())])))
call UnitDropItemSlotBJ(udg_rcUnit[GetPlayerId(GetTriggerPlayer())],GetLastCreatedItem(),ItemCopyI[GetPlayerId(GetTriggerPlayer())])
endif
set Selected_Check[(20*9)+GetPlayerId(GetTriggerPlayer())]=null
endif
endfunction
function Getting_CopyItemCheat takes unit fu2u,integer fi2i returns nothing
set ItemCopyI[GetPlayerId(GetTriggerPlayer())]=fi2i
set Selected_Check[(20*9)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endfunction
function Getting_DeleteItemCheat2 takes nothing returns nothing
if Selected_Check[(20*10)+GetPlayerId(GetTriggerPlayer())]==GetTriggerPlayer()then
if(UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[20+GetPlayerId(GetTriggerPlayer())])==null)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 선택한 슬롯은 비어있습니다.|r"+""))
else
if GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())]then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetUnitName(GetTriggerUnit())+" 은(는) 특권 치트 사용자의 유닛입니다! 아이템을 삭제 할 수 없습니다!|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,((("|cffff4500"+(GetPlayerName(GetOwningPlayer(GetTriggerUnit()))+"|r |cff2c952c님의|r |cffff0000"))+GetUnitName(GetTriggerUnit()))+("|r |cff2c952c의|r |cffff0000"+(I2S(ItemCopyI[20+GetPlayerId(GetTriggerPlayer())])+"|r|cff2c952c번째 인벤토리의 아이템을|r |cffffbe0a삭제|r|cff2c952c했습니다.|r"))))
call RemoveItem(UnitItemInSlotBJ(GetTriggerUnit(),ItemCopyI[20+GetPlayerId(GetTriggerPlayer())]))
endif
endif
endif
set Selected_Check[(20*10)+GetPlayerId(GetTriggerPlayer())]=null
endfunction
function Getting_DeleteItemCheat takes unit fu2u,integer fi2i returns nothing
set ItemCopyI[20+GetPlayerId(GetTriggerPlayer())]=fi2i
set Selected_Check[(20*10)+GetPlayerId(GetTriggerPlayer())]=GetTriggerPlayer()
endfunction
function Getting_ReiterationItemCheat takes unit fu2u,integer fi2i,integer fi3i returns nothing
if UnitItemInSlotBJ(fu2u,fi2i)==null then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 선택한 슬롯은 비어있습니다.|r"+""))
else
if(GetItemCharges(UnitItemInSlotBJ(fu2u,fi2i))==0)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 선택한 슬롯의 아이템은 중첩할 수 없는 종류의 아이템입니다.|r"+""))
else
if(fi3i<=0)then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! 아이템의 중첩 수를 0이나 음수로 할 수 없습니다.|r"+""))
else
call SetItemCharges(UnitItemInSlotBJ(fu2u,fi2i),fi3i)
endif
endif
endif
endfunction
function Getting_KickCheat takes integer fi2i returns nothing
set fi2i=fi2i-1
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
if GT_Privilege[fi2i]>=GT_Privilege[GetPlayerId(GetTriggerPlayer())]then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(Player(fi2i))+"는 이미 치트를 사용하고 있습니다. 당신과 동등한 권한을 가지고 있기 때문에 빼앗지 못합니다.|r")))
call ND2P(Player(fi2i),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+" 이(가) 당신을 강퇴하려고 시도했습니다.|r")))
else
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cff"+Player_Color[fi2i]+(GetPlayerName(Player(fi2i))+"|r |cffB4C3FF를|r |cffffbe0a강퇴|r|cffB4C3FF시켰습니다.|r")))
call KickcheatP(Player(fi2i))
endif
endif
endfunction
function CheckI takes integer ITEM returns integer
if ModuloInteger(ITEM,128)==57 then
set ITEM=ITEM+8
elseif ModuloInteger(ITEM,128)==90 then
set ITEM=ITEM+214
else
set ITEM=ITEM+1
endif
return ITEM
endfunction
function Getting_AllItemCheat takes unit fu2u returns nothing
local integer ITEM='I000'
loop
exitwhen ITEM>'I1ZZ'
call CreateItemLoc(ITEM,GetUnitLoc(fu2u))
set ITEM=CheckI(ITEM)
endloop
endfunction
function Getting_ChatCheat2 takes nothing returns nothing
local integer i2i=0
loop
exitwhen i2i>11
if IsPlayerInForce(Player(i2i),Chat_Hear)then
call ND2P(Player(i2i),0,0,15,"|cff"+Player_Color[GetPlayerId(GetTriggerPlayer())]+GetPlayerName(GetTriggerPlayer())+"|r : "+GetEventPlayerChatString())
endif
set i2i=i2i+1
endloop
endfunction
function Getting_ChatCheat takes integer fi2i,player p2p returns nothing
if fi2i==31 then
call Getting_CheatActions(11,CHATTING,function Getting_ChatCheat2)
call ForceAddPlayer(Chat_Hear,p2p)
call ND2P(GetTriggerPlayer(),0,0,5.,("|cffDC6089모든 플레이어의 채팅을 봅니다."+""))
elseif fi2i==63 then
call ForceRemovePlayer(Chat_Hear,p2p)
call ND2P(GetTriggerPlayer(),0,0,5.,("|cffDC6089모든 플레이어의 채팅을 그만봅니다."+""))
endif
endfunction
function Getting_StopGameCheat takes integer fi2i returns nothing
set fi2i=fi2i-1
if GT_Privilege[GetPlayerId(GetTriggerPlayer())]<49 then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+"님은 제한된 치트 권한을 가지고 있습니다.|r")))
else
if(GT_Privilege[GetPlayerId(GetOwningPlayer(GetTriggerUnit()))]>GT_Privilege[GetPlayerId(GetTriggerPlayer())])then
call ND2P(GetTriggerPlayer(),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(Player(fi2i))+"는 당신과 동등한 권한 또는 상위 권한을 가지고 있기 때문에 게임금지를 못합니다.|r")))
call ND2P(Player(fi2i),0,0,5.00,("|cffff0000주의! "+(GetPlayerName(GetTriggerPlayer())+" 이(가) 당신을 게임금지 하려고 시도했습니다.|r")))
else
call SetUserControlForceOff(GetForceOfPlayer(Player(fi2i)))
endif
endif
endfunction
function Getting_EndlessHealingCheat takes integer fi2i returns nothing
if fi2i==31 then
set CUnitC[40+GetPlayerId(GetTriggerPlayer())]=GetPlayerId(GetTriggerPlayer())
set CUnitC[60+GetPlayerId(GetTriggerPlayer())]=GetPlayerId(GetTriggerPlayer())
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c의|r |cffffbe0a체력/마나|r|cff2c952c를|r |cffffbe0a무한|r|cff2c952c으로|r |cffffbe0a회복|r|cff2c952c시킵니다.|r")))
call EnableTrigger(ENDLESS)
elseif fi2i==63 then
set CUnitC[40+GetPlayerId(GetTriggerPlayer())]=100
set CUnitC[60+GetPlayerId(GetTriggerPlayer())]=100
call ND2P(GetTriggerPlayer(),0,0,5.00,(("|cffff4500"+(GetPlayerName(GetOwningPlayer(udg_rcUnit[GetPlayerId(GetTriggerPlayer())]))+"|r |cff2c952c님의|r |cffff0000"))+(GetUnitName(udg_rcUnit[GetPlayerId(GetTriggerPlayer())])+"|r |cff2c952c의|r |cffffbe0a무한 체력/마나 회복|r |cff2c952c능력을|r |cffffbe0a제거|r|cff2c952c합니다.|r")))
endif
endfunction
function Getting_EndlessCooltimeCheat takes integer fi2i returns nothing
